<script type="text/javascript">
var leftHeight=$("#div_areaLeft").height();
			var rightHeight = $("#div_areaRight").height();
			if(leftHeight && rightHeight){
				if(leftHeight < rightHeight){
					rightHeight = Math.max(leftHeight, 1800);
				}else{
					rightHeight = leftHeight;
				}
				$("#div_areaRight").height(rightHeight);
			}

			var totalPage = parseInt($("#hid_totalPage").val());
			var currentPageId = "IDNewsDtail";
			if(totalPage && totalPage > 1){
				var docId = $("#hid_docId").val();
				var $div0=$("#doc_"+docId+"_0");
				$("div[id^='doc_"+docId+"_']").each(function(index, domObj){if(index>0){$(this).insertAfter($div0);$div0 = $(this);}});
				var page=0;
				var param=location.search.match(new RegExp("[\?&]page=([^&]*)(&?)", "i"));
				if(param && !isNaN(param[1])){page = parseInt(param[1])-1;}
				if(page<0)page=0;
				if(page>=totalPage)page=totalPage-1;
				currentPageId = "doc_"+docId+"_"+page;
				try{
					$("div[id^='doc_"+docId+"_']").hide();
					$("#"+currentPageId).show();
					if(page==0)
						$("#span_previous_page").hide();
					if(page==(totalPage-1))
						$("#span_next_page").hide();
					var $spanPageNumbers = $("span[id^='span_pagerNumber_']").removeClass().addClass("otherPages");
					$("#span_pagerNumber_" + page).removeClass().addClass("currentPage");
					if(totalPage>11){
						$spanPageNumbers.each(function(i, ele){
							if(i<page-5 || i>page+5)$(ele).hide();
						});
					}else{
						$("#span_total_page").hide();
					}
				}catch(e){}
			}
			/*if($("#"+currentPageId+" img,#"+currentPageId+" table").length>0){*/
     		if($("#"+currentPageId+" table").length>0){
				$("#adframe").hide();
			}
		
			function goPage(p){
				var page=parseInt(p)+1;
				var pos=location.href.indexOf("?");
				if(pos>0){
					location=location.href.substring(0, pos+1) + "page=" + page;
				}else{
					location += "?page=" + page;
				}
			}
//设置字体的大小
function ChangeShow(x)
{
  switch (x){
    case 1:
      document.getElementById("IDNewsDtail").className="fbig";
      break;
    case 2:
      document.getElementById("IDNewsDtail").className="fnomal";
      break;
    case 3:
      document.getElementById("IDNewsDtail").className="fsmall";
      break;
  }
}

var commentUrl = 'http://comment1.cnstock.com/newscomments/';
var newsUrl = document.location.href;
var i = newsUrl.indexOf("?");
if(i > 0){
	newsUrl = newsUrl.substring(0,i);
}
var newsTitle = escape($.trim($(".newstitle").text())) ;
var newsGuid = newsUrl.match(/.*[/\\](\d+)\..*/)[1];
var data = {
	NewsUrl: newsUrl,
	NewsTitle: newsTitle,
	NewsGUID: newsGuid
};
$.getJSON(commentUrl + "CommentXml.aspx?jsoncallback=?", data, function(jsonObj){
	var htmlCode = jsonObj.commnetNumber;
	if(htmlCode.indexOf("还没有网友发表评论")>0){
		htmlCode = htmlCode.replace("还没有网友发表评论","")
	}else{
		//htmlCode = htmlCode.replace("我要评论","")
	}
	htmlCode = htmlCode.replace(/CommentsList.aspx\?/g, commentUrl+"CommentsList.aspx?").replace(/&quot;?|&#34;?/ig,"\"").replace(/&#39;?/ig,"\'");	$("#CommentCount").html(htmlCode);
	$("#div_CommentCount_0").html(htmlCode);
});
$.getJSON(commentUrl + "ShortList.aspx?jsoncallback=?", data, function(jsonObj){
    $("#CommentList").html(jsonObj.commnetNumber.replace(/CommentsList.aspx\?/g, commentUrl+"CommentsList.aspx?").replace(/&quot;?|&#34;?/ig,"\"").replace(/&#39;?/ig,"\'"));});
$("#iframe_comment").attr("src", commentUrl+"CommentAdd.aspx?NewsUrl="+newsUrl+"&NewsTitle="+ newsTitle +"&NewsGUID="+newsGuid);

</script>






