    <script type="text/javascript">
      $(function(){

        $('input.onfocus').focus(function(){

          var value = $(this).attr('title');

          if($(this).attr('value') == value)
            $(this).attr('value', '');

        });

        $('input.onfocus').blur(function(){

          var value = $(this).attr('title');

          if($(this).attr('value') == '')
            $(this).attr('value', value);

        });

      });
    </script>
