<p>	$(function () {</p>
<p>			$(window).scroll(function () {</p>
<p>				if ($(this).scrollTop() > 100)</p>
<p>					$('a#move_up').fadeIn();</p>
<p>				else</p>
<p>					$('a#move_up').fadeOut(400);</p>
<p>			});</p>
