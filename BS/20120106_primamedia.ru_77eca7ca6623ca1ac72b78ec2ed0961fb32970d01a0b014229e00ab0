<p>    $(document).ready(function() {</p>
<p>        $('.show-popup').click(function(){</p>
<p></p>
<p>        $('.popup').hide();</p>
<p>        var cont = $(this).attr('rel');</p>
<p>        var pos = $(this).offset();</p>
<p></p>
<p>        $(cont).css({</p>
<p>            'left': 535,</p>
<p>            'top': pos.top - 45</p>
<p>        })</p>
<p>        .fadeIn('normal');</p>
<p></p>
<p>        return false;</p>
<p>    });</p>
<p></p>
<p>    $(document).click(function(e) {</p>
<p>        var $clicked = $(e.target);</p>
<p>        if (!$clicked.parents().hasClass('popup') ) {</p>
<p>        $('.popup').hide();</p>
<p>    }</p>
<p>});</p>
<p></p>
