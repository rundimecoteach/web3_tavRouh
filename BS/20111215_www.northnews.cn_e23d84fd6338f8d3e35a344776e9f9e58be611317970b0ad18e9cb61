<p>		alert('д!');</p>
<p>		document.getElementById('username').focus();</p>
<p>		return false;	</p>
<p>	}</p>
<p>	if(document.getElementById('xspace-seccode').value == '') {</p>
<p>		alert('֤!');</p>
<p>		document.getElementById('xspace-seccode').focus();</p>
<p>		return false;	</p>
<p>	}</p>
<p>	return true;</p>
<p>}</p>
