<p>function ActionChar(s){</p>
<p></p>
<p>   var s = $("#searchText").val();</p>
<p></p>
<p>  if(s ==" " && s=="请输入关键字")</p>
<p></p>
<p>  {</p>
<p></p>
<p>      $("#searchText").focus();</p>
<p>      return false;</p>
<p></p>
<p>  }</p>
<p></p>
<p>  var reg = /^([a-zA-Z])|([\u0391-\uFFE5]+)|([,])|([\s])|([0-9])$/;</p>
<p></p>
<p>  //var reg1 = /^$/</p>
<p></p>
<p>  var str = s;</p>
<p></p>
<p>  if(str == "")</p>
<p></p>
<p>  {</p>
<p></p>
<p>      alert("对不起关键词不能为空");</p>
<p></p>
<p>	  $("#searchText").focus();</p>
<p></p>
<p>	  //document.all.search.select();</p>
<p></p>
<p>	  return false;</p>
<p></p>
<p>  }else</p>
<p></p>
<p>  {</p>
<p></p>
<p>        var _sivalue=false;</p>
<p></p>
<p>		  for(var i =0; i<str.length;i++){</p>
<p></p>
<p>			 var _iKeyCode =str.substring(i,i+1);</p>
<p></p>
