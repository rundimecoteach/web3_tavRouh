<p>			  _sivalue=true;</p>
<p></p>
<p>		      }</p>
<p></p>
<p>		  }</p>
<p></p>
<p>		  if(_sivalue){</p>
<p></p>
<p>			alert("格式不正确!");</p>
<p></p>
<p>			$("#searchText").focus();</p>
<p></p>
<p>	        return false;</p>
<p></p>
<p>		  } else</p>
<p>          {</p>
<p>		   if(trim($("#searchText").val())!="请输入关键字" && trim($("#searchText").val())!="")</p>
<p>		   {</p>
<p>           var gotoUrl = "http://search.caixun.com/?k="+trim($("#searchText").val());</p>
<p>	       window.open(encodeURI(gotoUrl));</p>
