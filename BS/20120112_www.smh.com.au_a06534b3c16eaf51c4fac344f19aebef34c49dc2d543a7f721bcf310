<p>    if (typeof(twttr) != "undefined") {</p>
<p>        twttr.events.bind('tweet', function(event) {</p>
<p>            if (event) {</p>
<p>                var targetUrl;</p>
<p>                if (event.target && event.target.nodeName == 'IFRAME') {</p>
<p>                    targetUrl = FD.getUrlParam('url',event.target.src);</p>
<p>                    if(typeof(_gaq) != "undefined") {</p>
<p>                        _gaq.push(['_trackSocial', 'twitter', 'tweet', targetUrl]);</p>
<p>                    }</p>
<p>                }</p>
<p>            }</p>
<p>        });</p>
