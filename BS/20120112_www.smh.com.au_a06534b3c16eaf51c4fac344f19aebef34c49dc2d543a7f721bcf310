<p>	if (window["FD"]) {</p>
<p>		/**</p>
<p>		 * Updates the white strip with the new time and number of users.</p>
<p>		 *</p>
<p>		 * @param data Date to extract new time and number of users from.</p>
<p>		 * @private</p>
<p>		 */</p>
<p>		function updateWhiteStrip(data) {</p>
<p>			var response = $.parseJSON(data);</p>
<p>			var whiteStripTimeElement = $("whiteStrip-time");</p>
<p>			if ((whiteStripTimeElement) && (whiteStripTimeElement.length > 0)) {</p>
<p>				whiteStripTimeElement.empty();</p>
<p>				whiteStripTimeElement.html(response.time);</p>
<p>			}</p>
<p>			var whiteStripPeopleElement = $("whiteStrip-users");</p>
<p>			if ((whiteStripPeopleElement) && (whiteStripPeopleElement.length > 0)) {</p>
<p>				whiteStripPeopleElement.empty();</p>
<p>				whiteStripPeopleElement.html(response.people + " online now");</p>
<p>			}</p>
<p>		}</p>
<p>		/**</p>
<p>		 * Initiates the client side asset view tracking component (self executing).</p>
<p>		 */</p>
<p>		(function() {</p>
<p>			FD.pingServerWithSuccessCallback("2889420", "Article", "National", updateWhiteStrip);</p>
<p>		})();</p>
<p>	}</p>
