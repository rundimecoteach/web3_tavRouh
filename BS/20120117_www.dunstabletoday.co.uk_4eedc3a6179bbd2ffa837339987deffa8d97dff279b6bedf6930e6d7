<p>        function onFocus(field) {</p>
<p>            if (field.defaultValue == field.value) field.value = '';</p>
<p>        }</p>
<p>		function postComment(field) {</p>
<p>		var body = $('#body');</p>
<p>		var val = body.val();</p>
<p>		var def = body.attr ('defaultValue');</p>
<p>		if (val==def || val=='') {</p>
<p>			alert ("Please enter a comment");</p>
<p>			return false;</p>
<p>		}</p>
<p>		var btn = document.getElementById("postCommentBtn");</p>
<p>		btn.disabled=true;</p>
<p>		btn.textContent='Sending...';</p>
<p>		</p>
<p>		var myJSONObject = {"body": val, "action": "postcomment", "submitted": "true", "ajax": "true", "contentid": "ContentId(1.3426169)"};</p>
<p>		var postMessage = "<br /><b>Your comment has been posted.</b>";</p>
<p>		</p>
<p>		jQuery.post("http://www.dunstabletoday.co.uk/news/local/norovirus_closes_five_wards_at_l_d_1_3426169", myJSONObject, function(result){</p>
