<p>jQuery(document).ready(function(){</p>
<p>	jQuery('.productFormAgreeCheck').live('click', function(){</p>
<p>		if(jQuery(this).is('.CheckIn')){</p>
<p>			jQuery(this).removeClass('CheckIn').addClass('CheckOut');</p>
<p>			jQuery(this).find('input').val('nie')</p>
<p>			jQuery(this).parent().find('.error_msg').show()</p>
<p>		}else if(jQuery(this).is('.CheckOut')){</p>
<p>			jQuery(this).removeClass('CheckOut').addClass('CheckIn')</p>
<p>			jQuery(this).find('input').val('tak')</p>
<p>			jQuery(this).parent().find('.error_msg').hide()	</p>
<p>		}</p>
<p>	})</p>
