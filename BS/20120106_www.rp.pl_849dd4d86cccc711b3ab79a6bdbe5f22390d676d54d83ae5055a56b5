<p>	function closeAndShowModalBox(){</p>
<p>		</p>
<p>		jQuery('html, body').animate({scrollTop: '0px'}, 300);</p>
<p>		if(jQuery('#glassBox').is(":hidden")){</p>
<p>			jQuery('#dialogBox').css({</p>
<p>				'position' : 'absolute',</p>
<p>				'top' : '50px',</p>
<p>				'bottom' : 'auto'</p>
<p>			});</p>
<p>			jQuery('#dialogBox').empty();</p>
<p>			jQuery('.subscriptFormBox').clone().appendTo('#dialogBox').show("fast", function(){</p>
<p>				jQuery('#glassBox').fadeTo(400, 0.5, function(){</p>
<p>					jQuery('#dialogBox').addClass('noAccessLoginBoxDialog').css('left', (document.body.clientWidth - jQuery('#dialogBox').outerWidth()) / 2);</p>
<p>					jQuery('#dialogBox').fadeIn(300, function(){</p>
<p>						jQuery('#dialogBox').find('.subscriptForm').attr('id', 'subscriptFormSender')</p>
<p>						jQuery('#dialogBox').find('.subscriptForm').find('.dateto').attr('id', 'dateto')</p>
<p>						jQuery('#dialogBox').find('.subscriptForm').find('.datefrom').attr('id', 'datefrom')</p>
<p>						</p>
<p>						jQuery('#subscriptFormSender').validate({</p>
<p>							submitHandler : function(form) {</p>
<p>								jQuery(form).ajaxSubmit({</p>
<p>									url : '/call-action',</p>
<p>									type : 'POST',</p>
<p>									success: showResponse,</p>
<p>									error: showError</p>
<p>								});</p>
<p>							},</p>
<p>							groups: {</p>
<p>								postal: "postal1 postal2",</p>
<p>								date: "datefrom dateto"</p>
<p>							},</p>
<p>	  						errorPlacement: function(error, element) {</p>
<p>	     						if (element.attr("name") == "postal1" || element.attr("name") == "postal2" )</p>
<p>	   								error.insertAfter("#subscriptFormSender #postal2");</p>
<p>	    						else if (element.attr("name") == "datefrom" || element.attr("name") == "dateto" )</p>
<p>	   								error.insertAfter("#subscriptFormSender #dateto");</p>
<p>	       						else</p>
<p>	       							error.insertAfter(element);</p>
<p>	   							}</p>
<p>						})</p>
<p>						</p>
<p>					});</p>
<p>				});	</p>
<p>			});</p>
<p>			</p>
<p>			</p>
<p>		}else{</p>
<p>			jQuery('#dialogBox').css({</p>
<p>				'position' : 'absolute',</p>
<p>				'top' : '50px',</p>
<p>				'bottom' : 'auto'</p>
<p>			});		</p>
<p>			jQuery('#dialogBox').fadeOut(300, function(){</p>
<p>				jQuery(this).empty();</p>
<p>				jQuery('.subscriptFormBox').clone().appendTo('#dialogBox').show();</p>
<p>				jQuery(this).fadeIn(300, function(){</p>
<p>					jQuery('#dialogBox').find('.subscriptForm').attr('id', 'subscriptFormSender')</p>
<p>					jQuery('#dialogBox').find('.subscriptForm').find('.dateto').attr('id', 'dateto')</p>
<p>					jQuery('#dialogBox').find('.subscriptForm').find('.datefrom').attr('id', 'datefrom')</p>
<p>					jQuery('#subscriptFormSender').validate({</p>
<p>						submitHandler : function(form) {</p>
<p>							jQuery(form).ajaxSubmit({</p>
<p>								url : '/call-action',</p>
<p>								type : 'POST',</p>
<p>								success: showResponse,</p>
<p>								error: showError</p>
<p>							});</p>
<p>							</p>
<p>						},</p>
<p>						groups: {</p>
<p>							postal: "postal1 postal2",</p>
<p>							date: "datefrom dateto"</p>
<p>						},</p>
<p>  						errorPlacement: function(error, element) {</p>
<p>     						if (element.attr("name") == "postal1" || element.attr("name") == "postal2" )</p>
<p>   								error.insertAfter("#subscriptFormSender #postal2");</p>
<p>    						else if (element.attr("name") == "datefrom" || element.attr("name") == "dateto" )</p>
<p>   								error.insertAfter("#subscriptFormSender #dateto");</p>
<p>       						else</p>
<p>       							error.insertAfter(element);</p>
<p>   							}</p>
<p>   							</p>
<p>   							</p>
<p>					})</p>
<p>				})</p>
<p>			});</p>
<p>		}</p>
<p>		</p>
<p>	}</p>
<p>	</p>
<p>	function showResponse(responseText, statusText, xhr, $form){</p>
<p>		jQuery('html, body').animate({scrollTop: '0px'}, 300);</p>
<p>		responseText = jQuery.trim(responseText);</p>
<p>		if(responseText == 'duplicatePhone'){</p>
<p>			jQuery('.duplicatePhone').show();</p>
<p>		}else if(responseText == 'wrongDate'){</p>
<p>			jQuery('.wrongDate').show();</p>
<p>		}else{</p>
<p>			jQuery('#dialogBox').find('.subscriptFormBox_responseBox').html(responseText);	</p>
<p>		}</p>
<p>		jQuery('#dialogBox').undelegate('.confirmFormBtn','click', function(){</p>
<p>			return false</p>
<p>		})</p>
<p>	}</p>
<p>	function showError(){</p>
<p>		jQuery('html, body').animate({scrollTop: '0px'}, 300);</p>
<p>		jQuery('#dialogBox').find('.subscriptFormBox_formBody').slideUp(300,function(){</p>
<p>			jQuery('#dialogBox').find('.subscriptFormBox_Info').html('<strong class="errorLabel">Wysłanie formularza nie powiodło się</strong><br />prosimy spróbować ponownie później');	</p>
<p>		});		</p>
<p>	}</p>
<p>	jQuery(function(a) {a.datepicker.regional.pl = {</p>
<p>			closeText : "Zamknij",</p>
<p>			prevText : "&#x3c;Poprzedni",</p>
<p>			nextText : "Następny&#x3e;",</p>
<p>			currentText : "Dziś",</p>
<p>			monthNames : ["Styczeń", "Luty", "Marzec", "Kwiecień", "Maj", "Czerwiec", "Lipiec", "Sierpień", "Wrzesień", "Październik", "Listopad", "Grudzień"],</p>
<p>			monthNamesShort : ["Sty", "Lu", "Mar", "Kw", "Maj", "Cze", "Lip", "Sie", "Wrz", "Pa", "Lis", "Gru"],</p>
<p>			dayNames : ["Niedziela", "Poniedziałek", "Wtorek", "Środa", "Czwartek", "Piątek", "Sobota"],</p>
<p>			dayNamesShort : ["Nie", "Pn", "Wt", "Śr", "Czw", "Pt", "So"],</p>
<p>			dayNamesMin : ["N", "Pn", "Wt", "Śr", "Cz", "Pt", "So"],</p>
<p>			weekHeader : "Tydz",</p>
<p>			dateFormat : "yy-mm-dd",</p>
<p>			firstDay : 1,</p>
<p>			isRTL : !1,</p>
<p>			showMonthAfterYear : !1,</p>
<p>			yearSuffix : "",</p>
<p>			changeMonth: true,</p>
<p>			changeYear: true</p>
<p>		}, a.datepicker.setDefaults(a.datepicker.regional.pl)</p>
<p>	})</p>
<p>	</p>
<p>	jQuery('#datefrom').live('focus', function(event) {</p>
<p>		jQuery(this).datepicker();</p>
<p>	});</p>
<p>	jQuery('#dateto').live('focus', function(event) {</p>
<p>		jQuery(this).datepicker();</p>
<p>	});</p>
<p>	jQuery('.subscriptionBtn').bind('click', function(){</p>
<p>		closeAndShowModalBox();</p>
<p>	})</p>
<p>	jQuery('.cancelFormBtn').live('click', function(){</p>
<p>		closeModalBox()</p>
<p>	})</p>
<p>	</p>
<p>	jQuery('#dialogBox').delegate('.confirmFormBtn','click', function(){</p>
<p>		var sender = false;</p>
<p>		jQuery('.valid_checkAgree').each(function(index) {</p>
<p>			  if(jQuery(this).val() != 'tak'){</p>
<p>			  	jQuery(this).parent().parent().find('.error_msg').show();</p>
<p>			  	sender = false;</p>
<p>			  }else{</p>
<p>				sender = true</p>
<p>			  }</p>
<p>		});</p>
<p>		if(sender = true){</p>
<p>			jQuery('#subscriptFormSender').submit();	</p>
<p>		}else{</p>
<p>			return false;</p>
<p>		}</p>
<p>		</p>
<p>	})</p>
<p>	</p>
<p>	jQuery('.subscriptFormBox_closeIcon').live('click', function(){</p>
<p>		closeModalBox()</p>
<p>	})</p>
<p>	</p>
<p>})</p>
