<p>jQuery(document).ready(</p>
<p>	function() {</p>
<p>	jQuery('.articleLoginInput').live('focus', function(event) {</p>
<p>		if(jQuery(this).val() == jQuery(this).attr("data-default")){</p>
<p>			jQuery(this).val("");</p>
<p>		}</p>
<p>	}).live('blur', function(event) {</p>
<p>		if(jQuery.trim(jQuery(this).val()) == ""){</p>
<p>			jQuery(this).val(jQuery(this).attr("data-default"));</p>
<p>		}</p>
<p>	});</p>
<p></p>
<p>	jQuery('.articleLoginInputPass').live(</p>
<p>			'focus',</p>
<p>			function() {</p>
<p>				if (jQuery(this).attr('type') == 'text') {</p>
<p>					this.type = "password";</p>
<p>				}</p>
<p>				if(jQuery(this).val() == jQuery(this).attr("data-default")){</p>
<p>					jQuery(this).val("");</p>
<p>				}</p>
<p>			}).live('blur', function(){</p>
<p>				if(jQuery.trim(jQuery(this).val()) == "" || jQuery.trim(jQuery(this).val()) == jQuery(this).attr("data-default")){</p>
<p>					this.type = "text";</p>
<p>					this.value = jQuery(this).attr("data-default");</p>
<p>				}</p>
<p>			});</p>
<p>			</p>
<p>	jQuery('.noAccessLoginBox_smsInput').bind('focus', function(event) {</p>
<p>		if(jQuery(this).val() == jQuery(this).attr("data-default")){</p>
<p>			jQuery(this).val("");</p>
<p>		}</p>
<p>	});</p>
<p>});</p>
