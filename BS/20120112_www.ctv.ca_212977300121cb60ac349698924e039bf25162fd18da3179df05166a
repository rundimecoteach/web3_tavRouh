<p>var ctv_mobile_device = new Array("iphone", "ipod", "blackberry");</p>
<p>var agent = navigator.userAgent.toLowerCase();	</p>
<p>for(var i=0; i<ctv_mobile_device.length;i++){		</p>
<p>	if (ctv_mobile_device[i] != "" && agent.indexOf(ctv_mobile_device[i]) > -1) {			</p>
<p>		window.location = getMobileURL();</p>
<p>	} </p>
<p>}</p>
<p>function getMobileURL(){</p>
<p>	var mobileURL = "http://m.ctv.ca";	</p>
<p>	var hub = "Entertainment";</p>
<p>	var hub = hub.toLowerCase();</p>
<p>	if(hub == "topstoriesv2")</p>
<p>		hub = "topstories";</p>
<p>	if(hub == "entertainmentv2")</p>
<p>		hub = "entertainment";</p>
<p>	if(hub == "nationalnews")</p>
<p>		hub = "thedesk";</p>
<p>	if(hub == "scitech")</p>
<p>		hub = "tech";</p>
<p>	var slug = "alternative-cancer-treatment-robin-gibb-120111";</p>
<p>	var date = "20120111";</p>
<p>	mobileURL = mobileURL + "/" + hub + "/" + date + "/" + slug + ".html";</p>
<p>	return mobileURL;</p>
<p>}</p>
