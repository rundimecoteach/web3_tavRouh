<p>			function getPluckContent(ArtKey) {</p>
<p>			    var commentsPageRequest = new PluckSDK.CommentsPageRequest();</p>
<p>			    commentsPageRequest.CommentedOnKey = new PluckSDK.ExternalResourceKey({ Key: ArtKey });</p>
<p></p>
<p>			    PluckSDK.SendRequests([commentsPageRequest], responsePluckContentCallback);</p>
<p>			}</p>
<p></p>
<p>			function responsePluckContentCallback(responses) {</p>
<p>				//safely dump the response to Firebug</p>
<p>				var totalItems = isNaN(responses[0].TotalItems) ? 0 : responses[0].TotalItems;</p>
<p>				jQuery("#pluckCommentCount").text(totalItems);</p>
<p>			}</p>
<p></p>
