<p>      function clear_comment_info_msg() {</p>
<p>        var comment_body_element = document.getElementById('comment_body');</p>
<p>        var comment_body = comment_body_element.value;</p>
<p>        comment_body_element.value = comment_body.replace(/^Maximum\sof\s1750\scharacters\s\(about\s300\swords\)/, '');</p>
<p>      }</p>
<p>      function check_comment_length() {</p>
<p>        var comment_body_element = document.getElementById('comment_body');</p>
<p>        var comment_body = comment_body_element.value;</p>
<p>        if (comment_body.length > 1750) {</p>
<p>          alert('Your comment must not exceed 1750 characters.');</p>
<p>          comment_body_element.value = comment_body.substr(0, 1750);</p>
<p>        }</p>
<p>      }</p>
<p>      function validate_comment() {</p>
<p>          var chkd = document.getElementById('tnc_acceptance').checked;</p>
<p>          if (!chkd) {</p>
<p>              alert('You must accept the terms and conditions to post a comment.'); return false;</p>
<p>          }</p>
<p>          if (!document.getElementById('poster_name').value) {</p>
<p>              alert('You must enter a name to post a comment.'); return false;</p>
<p>          }</p>
<p>          if (!Account.logged_in) {</p>
<p>            if (!document.getElementById('poster_email').value.match(/^['A-Z0-9._%+-]+@[A-Z0-9.-]+\.[A-Z]{2,}$/i)) {</p>
<p>                alert('You have not entered a valid email address. Please enter a valid address to post a comment.'); return false;</p>
<p>            }</p>
<p>          }</p>
<p>          clear_comment_info_msg();</p>
<p>          if (!document.getElementById('comment_body').value.match(/[\w]+/)) {</p>
<p>              alert('You must enter a comment to post.'); return false;</p>
<p>          }</p>
<p>          return true;</p>
<p>      }</p>
