<p>jQuery(document).ready(function(){</p>
<p>OpenPlayer = function (s) {</p>
<p>if( s.substr(0,7) != "http://" ) s = "http://www.rmfon.pl/play,"+s;</p>
<p>window.open(s, "RMFplayerWindow", 'width=820,height=710,menubar=no,toolbar=no,location=no,scrollbars=no,resizable=no,status=no');</p>
<p>};</p>
<p></p>
<p>getJson = function ()</p>
<p>{</p>
<p>time = new Date();</p>
<p>// get JSON data</p>
<p>jQuery.getJSON("http://d.iplsc.com/rmf/rmfonPlayer_5.json.txt?"+ time.getTime() +"&jsoncallback=?",</p>
<p>function (data) {</p>
<p></p>
<p>});</p>
<p>};</p>
<p></p>
<p>putHtml = function ()</p>
<p>{</p>
<p>// put html template</p>
<p>jQuery('#serviceRmfCityofmusicObjectPlayer1 .player').html('<div class="now"><div class="description"><b>Teraz gramy</b><h3><a href="">&nbsp;</a></h3><span class="title">&nbsp;</span></div><div class="play"><a target="_blank" onclick=\'OpenPlayer("5");return false;\' href="#">Posłuchaj</a></div></div><div class="next"><div class="description"><b>Następny</b><h3><a href="">&nbsp;</a></h3><span class="title">&nbsp;</span></div></div>');</p>
<p>getJson();</p>
<p>};</p>
<p></p>
<p>player = function (data)</p>
<p>{</p>
<p>var item_now;</p>
<p>var item_next;</p>
<p>jQuery.each(data, function(index, value) {</p>
<p>if(value.order == 0 ) {</p>
<p>item_now = value;</p>
<p>}</p>
<p>if(value.order == 1 ) {</p>
<p>item_next = value;</p>
<p>}</p>
<p>});</p>
<p>if (data && item_now && item_next) {</p>
<p>/* change player data */</p>
<p>if( item_now.bio != '' || item_now.title != '' ) {</p>
<p>jQuery('#serviceRmfCityofmusicObjectPlayer1 .player .now .description').show();</p>
<p>jQuery('#serviceRmfCityofmusicObjectPlayer1 .player .now .description h3 a').attr('href', 'http://www.rmfon.pl/artysci/'+ item_now.bio+'.html' );</p>
<p>jQuery('#serviceRmfCityofmusicObjectPlayer1 .player .now .description h3 a').html(item_now.author);</p>
<p>jQuery('#serviceRmfCityofmusicObjectPlayer1 .player .now .description span.title').html(item_now.title);</p>
<p>} else {</p>
<p>jQuery('#serviceRmfCityofmusicObjectPlayer1 .player .now .description').hide();</p>
<p>}</p>
<p></p>
<p>if( item_next.bio != '' || item_next.title != '' ) {</p>
<p>jQuery('#serviceRmfCityofmusicObjectPlayer1 .player .next .description').show();</p>
<p>jQuery('#serviceRmfCityofmusicObjectPlayer1 .player .next .description h3 a').attr('href', 'http://www.rmfon.pl/artysci/'+ item_next.bio+'.html' );</p>
<p>jQuery('#serviceRmfCityofmusicObjectPlayer1 .player .next .description h3 a').html(item_next.author);</p>
<p>jQuery('#serviceRmfCityofmusicObjectPlayer1 .player .next .description span.title').html(item_next.title);</p>
<p>} else {</p>
<p>jQuery('#serviceRmfCityofmusicObjectPlayer1 .player .next .description').hide();</p>
<p>}</p>
<p>}</p>
<p>};</p>
<p></p>
<p>var refreshTime = "60";</p>
<p>setInterval(</p>
<p>function(){</p>
<p>getJson();</p>
<p>},</p>
<p>refreshTime * 1000 );</p>
<p></p>
<p>/* if don't get data from API, try to get JSON */</p>
<p>if( jQuery('#serviceRmfCityofmusicObjectPlayer1 .player .now .description h3 a').length === 0 )</p>
<p>{</p>
<p>putHtml();</p>
<p>}</p>
<p>});    //]]></p>
