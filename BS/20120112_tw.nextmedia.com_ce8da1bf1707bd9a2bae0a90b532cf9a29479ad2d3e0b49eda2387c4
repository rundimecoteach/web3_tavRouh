<p>      function setURLCookie(key, value, expire, domain, path)</p>
<p>      {</p>
<p>        var ck=key +'='+ encodeURIComponent(value);</p>
<p>        if( expire )</p>
<p>        {</p>
<p>          var epr=new Date();</p>
<p>          epr.setTime(epr.getTime()+ expire*1000 );</p>
<p>          ck+=';expires='+ epr.toUTCString();</p>
<p>        }</p>
<p>        if( domain )</p>
<p>          ck+=';domain='+ domain;</p>
<p>        if( path )</p>
<p>          ck+=';path='+ path;</p>
<p>        document.cookie=ck;</p>
<p>      }</p>
