<p>    var $next =  $active.next().length ? $active.next()</p>
<p>        : $('#promoshow li:first');</p>
<p></p>
<p>    // uncomment the 3 lines below to pull the images in random order</p>
<p>    </p>
<p>    // var $sibs  = $active.siblings();</p>
<p>    // var rndNum = Math.floor(Math.random() * $sibs.length );</p>
<p>    // var $next  = $( $sibs[ rndNum ] );</p>
<p></p>
<p></p>
<p>    $active.addClass('last-active');</p>
<p></p>
<p>    $next.css({opacity: 0.0})</p>
<p>        .addClass('active')</p>
