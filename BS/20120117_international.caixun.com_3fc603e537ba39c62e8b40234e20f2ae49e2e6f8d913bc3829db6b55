<p>function ActionChar(s){</p>
<p></p>
<p>   var s = $("#searchText").val();</p>
<p></p>
<p>  if(s ==" " && s=="请输入关键字")</p>
<p></p>
<p>  {</p>
<p></p>
<p>      $("#searchText").focus();</p>
<p>      return false;</p>
<p></p>
<p>  }</p>
<p></p>
<p>  var reg = /^([a-zA-Z])|([\u0391-\uFFE5]+)|([,])|([\s])|([0-9])$/;</p>
<p></p>
<p>  //var reg1 = /^$/</p>
<p></p>
<p>  var str = s;</p>
<p></p>
