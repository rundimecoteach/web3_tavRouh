<p>function formatOnAirEvents(){</p>
<p>	var container = $('#tvonair');</p>
<p>	// Clear floating</p>
<p>	$('<div/>').addClass('clr').appendTo(container);</p>
<p>	// Remove right border from last column</p>
<p>	container.find('.onair_channel_block:last').css('border-right-width', '0px');</p>
<p>	var height = container.height();</p>
<p>	container.find('.onair_channel_block').each(function (i, block){</p>
<p>		// change columns height to have separator line from top to bottom</p>
<p>		$(block).height(height);</p>
<p>		// remove bottom border from last element in each column</p>
<p>		$(block).find('.onair_event:last').css('border', 0);</p>
<p>		$(block).find('.onair_title').wrapInner('<a href="http://programma.delfi.ee" />');</p>
<p>	});</p>
<p>	//set logo sizes (not viable as it is done afther document onliad)</p>
<p>	//$(".onair_channel img").each(function () {</p>
<p>	//	$(this).attr('widht', this.width);</p>
<p>    //    $(this).attr('height', this.height);</p>
<p>    //});</p>
<p>}</p>
<p>$(document).ready(function (){</p>
<p>	delfitvonair.language_id = 131;</p>
<p>	delfitvonair.service_url = "http://kava.delfi.ee/tvguide/fe/service.php";</p>
<p>	delfitvonair.channel_ids = [135, 136, 32, 106, 198, 199];</p>
<p>	delfitvonair.placeholder = 'tvonair';</p>
<p>	delfitvonair.limit = 4;</p>
<p>	delfitvonair.format_callback = formatOnAirEvents;</p>
<p>	delfitvonair.requestEvents();</p>
<p>});</p>
