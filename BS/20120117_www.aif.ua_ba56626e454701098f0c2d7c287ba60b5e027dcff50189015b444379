<p>function limitChars(textid, limit, infodiv)</p>
<p>{</p>
<p>	var text = $('#'+textid).val();</p>
<p>	var textlength = text.length;</p>
<p>	if(textlength > limit)</p>
<p>	{</p>
<p>		$('#' + infodiv).html('Достигнуто ограничение в '+limit+' символов!');</p>
<p>		$('#'+textid).val(text.substr(0,limit));</p>
<p>		return false;</p>
<p>	}</p>
<p>	else</p>
<p>	{</p>
<p>		$('#' + infodiv).html('Осталось символов: '+ (limit - textlength) + '.');</p>
<p>		return true;</p>
<p>	}</p>
<p>}</p>
<p></p>
<p>$(document).ready(function() {</p>
<p></p>
<p> 	$('#comments_inp').keyup(function(){</p>
<p> 		limitChars('comments_inp', 2500, 'charlimitinfo');</p>
<p> 	})</p>
<p></p>
<p>	var login = $.cookie('login');</p>
<p>	var hash = $.cookie('hash');</p>
<p></p>
<p>	if (login == null)</p>
<p>	{</p>
<p>		// BC with old auth!</p>
<p>		if ( hash != null ) {</p>
<p>			$.post('/ajax/setlogin', function(data) {$('#user_name').val(data.result); $('#user_name').attr('readonly', true); $('#login_msg').html('Зарегистрированный пользователь:');});</p>
<p>			$('#form_email').hide();</p>
<p>			$('#auth_msg').hide();</p>
<p>			var heightAF = 385;</p>
<p>		} else {</p>
<p>			setCookie('comments', 'f7c02944a6cc36583f3892e1518ca020', {expires: 7, path: '/', domain: 'www.aif.ua', secure: false});</p>
<p>			$('#user_msg').hide();</p>
<p>			$('#user_name').val('');</p>
<p></p>
<p>			var heightAF = 460;</p>
<p>		}</p>
<p></p>
<p>	} else {</p>
<p>		$('#form_email').hide();</p>
<p>		$('#auth_msg').hide();</p>
<p>		$('#user_name').val(login);</p>
<p>		$('#user_name').attr('readonly', true);</p>
<p>		$('#login_msg').html('Зарегистрированный пользователь:');</p>
<p>		var heightAF = 385;</p>
<p>	}</p>
<p></p>
<p>	$('#comments_inp').val(getRulesAF());</p>
<p>	$('#comments_inp').css('color', '#999999');</p>
<p></p>
<p>	$('#tabs-block').height(heightAF);</p>
<p>	$('#tabs-block').show();</p>
<p></p>
<p></p>
<p>	$('#comments_inp').focus(function()</p>
<p>	{</p>
<p>		//alert(showRule);</p>
<p>		if (showRule) {</p>
<p>			$('#comments_inp').css('color', '#000000');</p>
<p>			$('#comments_inp').val('');</p>
<p>			showRule = false;</p>
<p>		}</p>
<p>	});</p>
<p></p>
<p>});</p>
