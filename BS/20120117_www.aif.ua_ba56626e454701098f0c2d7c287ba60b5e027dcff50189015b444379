<p>function SuccessGrantedAF(json) {</p>
<p>	if (json.content != null) {</p>
<p>		if ($.cookie('auth_referer') !== null) { // see user recupes for example</p>
<p>			var ref = $.cookie('auth_referer').split('|')</p>
<p>			window.location.replace(ref[0]);</p>
<p>		} else {</p>
<p>			$(afId).html(json.content);</p>
<p>		}</p>
<p>	} else {</p>
<p>		$(afId).html(json.result);</p>
<p>	}</p>
<p>}</p>
