<p>function afSuccess(json)</p>
<p>{</p>
<p>	if( json.result == 'ok')</p>
<p>	{</p>
<p>		$('textarea[name=comments_inp]').each(function(){</p>
<p>			$(this).get(0).value = '';</p>
<p>		});</p>
<p></p>
<p>		$('#comments').html(json.content);</p>
<p></p>
<p>       $('#c_content_2').load('http://www.aif.ua/aforism/?url='+document.URL+'&'+Math.random());</p>
<p>	} else {</p>
<p>		SuccessFailedAF(json);</p>
<p>	}</p>
<p></p>
<p>	setSubmitAF();</p>
<p>}</p>
