<p>['<scr'+'ipt language="javascript" type="text/javascript" src="http://img.ifeng.com/tres/html/birangtest/adsame.js\">'+'</'+'script>',2,true,"_ADSAME_CONTROL_"],</p>
<p>['<scr'+'ipt language="javascript" type="text/javascript" src="http://img.ifeng.com/tres/recommend/cpro/quantone/2010/10/101030/101030-SmartMedia-ent.js\">'+'</'+'script>',2,true,"_SMARTMEDIA_3327886665615566_CTRL_"]</p>
