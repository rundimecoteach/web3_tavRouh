<p>var date = new Date();</p>
<p></p>
<p>// 0=Sun 1=Mon 2=Tue 3=Wed 4=Thu 5=Fri 6=Sat</p>
<p>var dow = date.getDay();</p>
<p>var d  = date.getDate();</p>
<p>var day = (d < 10) ? '0' + d : d;</p>
<p>var m = date.getMonth() + 1;</p>
<p>var month = (m < 10) ? '0' + m : m;</p>
<p>var yy = date.getYear();</p>
<p></p>
<p>var year = (yy < 1000) ? yy + 1900 : yy;</p>
<p>var hours = date.getHours();</p>
<p>var minutes = date.getMinutes();</p>
<p></p>
<p>zDate = day + "/" + month + "/" + year;</p>
<p>zTime = hours + ":" + minutes;</p>
<p></p>
<p>//SAT & Sun</p>
<p>if(dow == 6 || dow == 0){</p>
<p>//MORNING</p>
<p>if(hours == 6){document.write('<div style=\"width:647px; height:auto; margin-left:-25px; margin-bottom:-25px;\"><a href=/news/livestreaming/><img src=http://tv.trb.com/includes/tvdata/wgntv/livestreaming/2012/weekend_news.png></a></div>');}</p>
<p></p>
<p>//NINE</p>
<p>else if(hours == 21){document.write('<div style=\"width:647px; height:auto; margin-left:-25px; margin-bottom:-25px;\"><a href=/news/livestreaming/><img src=http://tv.trb.com/includes/tvdata/wgntv/livestreaming/2012/weekend_news.png></a></div>');}</p>
<p>	</p>
<p>//ANYTIME</p>
<p>else {document.write('<div style=\"width:647px; height:auto; margin-left:-25px; margin-bottom:-25px;\"><a href=/news/livestreaming/><img src=http://tv.trb.com/includes/tvdata/wgntv/livestreaming/2012/online_anytime.png></a></div>');}</p>
<p>}</p>
<p></p>
<p>//MON - Friday</p>
<p>else if(dow > 0 && dow < 6){</p>
<p></p>
<p>//MORNING</p>
<p>if(hours > 3 && hours < 9){document.write('<div style=\"width:647px; height:auto; margin-left:-25px; margin-bottom:-25px;\"><a href=/news/livestreaming/><img src=http://tv.trb.com/includes/tvdata/wgntv/livestreaming/2012/morning_news.png></a></div>');}</p>
<p></p>
<p>//MIDDAY</p>
<p>else if(hours > 10 && hours < 13){document.write('<div style=\"width:647px; height:auto; margin-left:-25px; margin-bottom:-25px;\"><a href=/news/livestreaming/><img src=http://tv.trb.com/includes/tvdata/wgntv/livestreaming/2012/midday_news.png></a></div>');}</p>
<p></p>
<p>//FIVE</p>
<p>else if(hours == 17){document.write('<div style=\"width:647px; height:auto; margin-left:-25px; margin-bottom:-25px;\"><a href=/news/livestreaming/><img src=http://tv.trb.com/includes/tvdata/wgntv/livestreaming/2012/news_at_five.png></a></div>');}</p>
<p></p>
<p>//POLITICS TONIGHT</p>
<p>else if(hours == 18 && minutes < 30){document.write('<div style=\"width:647px; height:auto; margin-left:-25px; margin-bottom:-25px;\"><a href=/news/livestreaming/><img src=http://tv.trb.com/includes/tvdata/wgntv/livestreaming/2012/politics_tonight.png></a></div>');}</p>
<p></p>
<p>//NINE</p>
<p>else if(hours == 21){document.write('<div style=\"width:647px; height:auto; margin-left:-25px; margin-bottom:-25px;\"><a href=/news/livestreaming/><img src=http://tv.trb.com/includes/tvdata/wgntv/livestreaming/2012/news_at_nine.png></a></div>');}</p>
<p></p>
<p>//ANYTIME</p>
<p>else {document.write('<div style=\"width:647px; height:auto; margin-left:-25px; margin-bottom:-25px;\"><a href=/news/livestreaming/><img src=http://tv.trb.com/includes/tvdata/wgntv/livestreaming/2012/online_anytime.png></a></div>');}</p>
<p>}</p>
