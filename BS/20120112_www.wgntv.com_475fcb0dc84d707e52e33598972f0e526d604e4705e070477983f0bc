<p>(function() {</p>
<p>  var cookieName = 'tribanalyticscookie',</p>
<p>      cookieData = readCookie(cookieName),</p>
<p>      ssorId = carnival.user.profile().masterId,</p>
<p>      data;</p>
<p>  </p>
<p>  if (!cookieData) {</p>
<p>    cookieData = guidGenerator();</p>
<p>    createCookie(cookieName, cookieData, 3650);</p>
<p>  }</p>
<p>  data = 'k=viewcontent&u=' + cookieData + '&url=' + encodeURIComponent(window.location.href);</p>
<p>  if (ssorId) data += '&es=' + ssorId;</p>
<p>  if (trb.data.contentId) data += '&contentId=P2P-' + trb.data.contentId;</p>
<p>  if (trb.data.marketCode) data += '&marketCode=' + trb.data.marketCode;</p>
<p>  if (trb.data.section) data += '&section=' + encodeURIComponent(trb.data.section);</p>
