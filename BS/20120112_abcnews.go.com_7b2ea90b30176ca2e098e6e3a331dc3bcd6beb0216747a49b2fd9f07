<p>                    $('#'+id).css('margin-bottom', '0px');</p>
<p>                    $('#'+id+' > .rel_active').toggle(100, function() {</p>
<p>                        $('#'+id+' > .rel_content').show();</p>
<p>                        $('#'+id+' > .rel_active').removeClass('rel_active').addClass('rel_inactive');</p>
<p>                        $('#'+id+' > .rel_inactive').hide();</p>
<p>                    });</p>
<p>                    return false;</p>
<p>                }</p>
<p>			}</p>
<p>			</p>
<p>			leftRail.openCaption = function() {</p>
<p>				$('#cap_full').show();</p>
<p>				$('#cap_short').hide();</p>
<p>                if(leftRailVideoOpen){</p>
<p>                    setTimeout(positionPlayer(leftRailContainerObj),500);</p>
<p>                }</p>
<p>				return false;</p>
<p>			}</p>
<p>			</p>
<p>			leftRail.closeCaption = function() {</p>
<p>				$('#cap_short').show();</p>
<p>				$('#cap_full').hide();</p>
<p>                if(leftRailVideoOpen){</p>
<p>                    setTimeout(positionPlayer(leftRailContainerObj),500);</p>
<p>                }</p>
<p>				return false;</p>
<p>			}</p>
<p>			</p>
<p>			var mainMedia = new Object;</p>
<p>			mainMedia.openCaption = function() {</p>
<p>				$('#main_cap_full').show();</p>
<p>				$('#main_cap_short').hide();</p>
<p>				mainMedia.sponsoredByAdPosition();</p>
<p>				if(leftRailVideoOpen){</p>
<p>                    setTimeout(positionPlayer(leftRailContainerObj),500);</p>
<p>                }</p>
