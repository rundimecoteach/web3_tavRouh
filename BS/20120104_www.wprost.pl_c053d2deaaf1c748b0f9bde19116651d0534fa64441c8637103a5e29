<p>       function comments_geturl()</p>
<p>       {                  </p>
<p>          var u = $('#lista-komentarzy #comment_url').val();</p>
<p>          if (!u)</p>
<p>            u = location.href;</p>
<p>          return u;  </p>
<p>       }</p>
<p>       function coments_textCollapse(destObj, showHeight) </p>
<p>       {                 </p>
<p>          $(destObj+' .comment-item-expand').each(function () {</p>
<p>              var cie_h = $(this).height();   </p>
<p>              //if (showHeight)</p>
<p>              //  alert(cie_h); </p>
<p>              if (cie_h > 120) </p>
<p>              {    </p>
<p>                  $(this).addClass('comment-item-expand-active');  </p>
<p>                  $(this).attr('expand-height', cie_h); </p>
<p>                  $('.comment-item-expand-row', this).click(function () {  </p>
<p>                       var ccie_h = $(this).parent('.comment-item-expand-active').attr('expand-height'); </p>
<p>                       $(this).parent('.comment-item-expand-active').css({'max-height': 'none', 'height': '120px'});</p>
<p>                       $(this).parent('.comment-item-expand-active').animate({'height': ccie_h}, function () {  </p>
<p>                            $(this).css('height', 'auto');</p>
<p>                            $(this).removeClass('comment-item-expand-active');</p>
<p>                            $(this).removeClass('comment-item-expand');</p>
<p>                       });  </p>
<p>                  });  </p>
<p>              }           </p>
<p>              else  </p>
<p>                  $(this).removeClass('comment-item-expand');</p>
<p>          });        </p>
<p>       }                    </p>
<p>       function comments_get(id_element, id_parent, pos, update_arrows, dont_scroll, expand_pos, first_level, order)</p>
<p>       {        </p>
<p>           if (typeof order == 'undefined' || order === false) </p>
<p>              order = -1; </p>
<p>           $('body').css('cursor', 'wait');     </p>
<p>           $('#comments-container-'+id_element+'-'+id_parent).animate({opacity:0.4});</p>
<p>           $.ajax({  </p>
<p>              type: 'POST',  </p>
<p>              url:  '/F/ajax/comments.php',  </p>
<p>              data: {</p>
<p>                 'get': 1, </p>
<p>                 'id_element': id_element, </p>
<p>                 'id_parent': id_parent, </p>
<p>                 'pos': pos,          </p>
<p>                 'order': order,</p>
<p>                 'update_arrows': (update_arrows?1:0),</p>
<p>                 'first_level': first_level,</p>
<p>                 'url': comments_geturl(),</p>
<p>                 'curl': comments_geturl()</p>
<p>              },               </p>
<p>              dataType: 'json',</p>
<p>              success: function(oJsonObject) {   </p>
<p>                 $('body').css('cursor', 'default'); </p>
<p>                 if (oJsonObject)</p>
<p>                 {       </p>
<p>                     $('#comments-counter-'+id_element).html(oJsonObject.count);   </p>
<p>                     $('#comments-counter-'+id_element).attr('title','Wpisów:'+oJsonObject.count);               </p>
<p>                    </p>
<p>                     if (first_level && oJsonObject.inlist_count==0)</p>
<p>                     {</p>
<p>                        comments_showAddForm(id_element, id_parent);</p>
<p>                        $('#lista-komentarzy-toolbar').hide();</p>
<p>                        oJsonObject.html = '<br /><br />';</p>
<p>                     }</p>
<p>                     </p>
<p>                     returnHTML = oJsonObject.html;              </p>
<p>                    </p>
<p>                     if (oJsonObject.update_arrows==1)</p>
<p>                     {  </p>
<p>                         $('#comments-arrows-'+id_element).replaceWith(oJsonObject.arrows);                </p>
<p>                         if (!dont_scroll)</p>
<p>                         {   </p>
<p>                             var tpx = $('#lista-komentarzy').offset().top;</p>
<p>                             tpx = tpx - 45;</p>
<p>                             $('html,body').animate({scrollTop: tpx });</p>
<p>                         }     </p>
<p>                     }  </p>
<p>                     else</p>
<p>                     {       </p>
<p>                         if (oJsonObject.canExpand)</p>
<p>                         {</p>
<p>                             returnHTML += '<div id="comments-container-'+id_element+'-'+id_parent+'-'+oJsonObject.nwxtPos+'">'+                                                                                                          </p>
<p>                                           '<div class="comment-item-expand-bar">'+</p>
<p>                                           '<input onclick="comments_get('+id_element+', '+id_parent+', '+oJsonObject.nwxtPos+', false, false, '+oJsonObject.nwxtPos+');" type="button" class="form-button-white" value="Wyświetl więcej odpowiedzi" />'</p>
<p>                                           '</div></div>';                </p>
<p>                         }</p>
<p>                     }        </p>
<p>                     if (!expand_pos)</p>
<p>                         var destObj = '#comments-container-'+id_element+'-'+id_parent;</p>
<p>                     else</p>
<p>                         var destObj = '#comments-container-'+id_element+'-'+id_parent+'-'+expand_pos;</p>
<p>  </p>
<p>                     if (oJsonObject.large_votes_buttons)</p>
<p>                        $('#comment-large-votes-'+id_element+'-'+id_parent).html(oJsonObject.large_votes_buttons);</p>
<p>                    </p>
<p>                     $(destObj).html(returnHTML); </p>
<p>                     if (first_level)</p>
<p>                     {</p>
<p>                         coments_textCollapse(destObj); </p>
<p>                         $('#lista-komentarzy-combobox-order').val(oJsonObject.orderIndex);  </p>
<p>                     }</p>
<p>                     $('#comments-container-'+id_element+'-'+id_parent).animate({opacity:1});</p>
<p>                           </p>
<p>                 }</p>
<p>              },        </p>
<p>              error: function() {       </p>
<p>                 $('#comments-container-'+id_element+'-'+id_parent).animate({opacity:1});</p>
<p>                 $('body').css('cursor', 'default'); </p>
<p>              }</p>
<p>           });              </p>
<p>       }</p>
<p>       function comment_toggleAnswers(id_element, id_parent)</p>
<p>       {                                                 </p>
<p>          var answbtn  = $('#comment-answers-'+id_element+'-'+id_parent); </p>
<p>          var answcntr = $('#comments-container-'+id_element+'-'+id_parent);</p>
<p>          if (answbtn.hasClass('answers-link-down')) </p>
<p>          {</p>
<p>              answbtn.addClass('answers-link-up');</p>
<p>              answbtn.removeClass('answers-link-down');  </p>
<p>              if (answcntr.text()=='')                 </p>
<p>              {</p>
<p>                  comments_get(id_element, id_parent, false, false, false);</p>
<p>              }</p>
<p>              answcntr.slideDown();  </p>
<p>              setTimeout(function() {</p>
<p>                      coments_textCollapse('#comments-container-'+id_element+'-'+id_parent, true); </p>
<p>              }, 500);</p>
<p>          }                            </p>
<p>          else</p>
<p>          {</p>
<p>              answbtn.removeClass('answers-link-up');</p>
<p>              answbtn.addClass('answers-link-down');  </p>
<p>              answcntr.slideUp();</p>
<p>          }     </p>
<p>       }     </p>
