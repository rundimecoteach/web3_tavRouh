<p>function getCookie (name) {</p>
<p>	var arg = name + "=";</p>
<p>	var alen = arg.length;</p>
<p>	var clen = document.cookie.length;</p>
<p>	var i = 0;</p>
<p>	while (i < clen) {</p>
<p>		var j = i + alen;</p>
<p>		if (document.cookie.substring(i, j) == arg) return getCookieVal (j);</p>
<p>		i = document.cookie.indexOf(" ", i) + 1;</p>
<p>		if (i == 0) break; </p>
<p>	}</p>
<p>	return null;</p>
<p>}</p>
