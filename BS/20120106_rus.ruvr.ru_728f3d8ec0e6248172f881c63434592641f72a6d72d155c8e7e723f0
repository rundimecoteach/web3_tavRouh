<p>  function _openForm(form) { </p>
<p>	if(!window.loaded_forms) window.loaded_forms={};</p>
<p>        var wform = window.loaded_forms[form];</p>
<p>	var place = $("#popup-placeholder");</p>
<p>	if( wform) {</p>
<p></p>
<p>	    place.html(wform);</p>
<p>	    if(window['initialize_' + form]) window['initialize_' + form](place);</p>
<p>	} else { </p>
<p>/*</p>
<p>	    wajax('getHTML/' + form, {},'GET', {dataType:'html'}, function(r) { </p>
<p>		window.loaded_forms[form] =  r;</p>
<p>		place.html(r);</p>
<p>		if(window['initialize_' + form]) window['initialize_' + form](place); </p>
<p>	    });</p>
<p>*/</p>
<p>            wajax('getCTPP/' + form, {},'GET', {dataType:'html'}, function(r) {</p>
<p>                window.loaded_forms[form] =  r;                                </p>
<p>                place.html(r);                                                 </p>
<p>                if(window['initialize_' + form]) window['initialize_' + form](place); </p>
<p>            });</p>
<p>	}</p>
