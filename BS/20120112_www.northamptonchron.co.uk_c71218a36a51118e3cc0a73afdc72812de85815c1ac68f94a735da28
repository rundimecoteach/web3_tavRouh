<p>        function onBlur(field) {</p>
<p>            if (field.value == '') field.value = field.defaultValue;</p>
<p>        }</p>
<p>        function onFocus(field) {</p>
<p>            if (field.defaultValue == field.value) field.value = '';</p>
<p>        }</p>
