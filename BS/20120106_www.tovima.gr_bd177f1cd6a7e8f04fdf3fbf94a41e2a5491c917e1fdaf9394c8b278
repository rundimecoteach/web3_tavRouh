<p>                $("select option:selected").each(function () {</p>
<p>                    if ($(this).attr('value') != 'null') {</p>
<p>                        window.open($(this).attr('value'), "newwindow");</p>
<p></p>
<p>                    }</p>
<p>                });</p>
<p>            });</p>
