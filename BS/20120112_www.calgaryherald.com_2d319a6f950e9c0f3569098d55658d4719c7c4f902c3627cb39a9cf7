<p>var _publishflow_config = {</p>
<p>    client_id: 'calgaryherald.com'</p>
<p>};</p>
<p>(function() {</p>
<p>    function execPublishflowLogger() {</p>
<p>        var _element = document.createElement('script');</p>
<p>        _element.setAttribute('language', 'javascript');</p>
<p>        _element.setAttribute('type', 'text/javascript');</p>
<p>        _element.setAttribute('src', 'http://s3.amazonaws.com/publishflow/javascript/media.js');</p>
<p>        document.body.appendChild(_element);</p>
<p>    }</p>
<p>    var _onLoadMethod = window.onload;</p>
<p>    window.onload = (typeof window.onload != 'function') ? execPublishflowLogger : function() {</p>
<p>        _onLoadMethod();</p>
<p>        execPublishflowLogger();</p>
<p>    };</p>
<p>})();</p>
