<p>var docBody = null;</p>
<p>function zoomDoc(size) {</p>
<p>  if (!docBody) {</p>
<p>    return;</p>
<p>  }</p>
<p>  if(size == 14) {</p>
<p>	document.getElementById('small').className="smallsize";</p>
<p>	document.getElementById('big').className="bigsize";</p>
<p>  } else {</p>
<p>	document.getElementById('small').className="bigsize";</p>
<p>	document.getElementById('big').className="smallsize";</p>
<p>  }</p>
<p>  docBody.style.fontSize = size + 'px';</p>
<p>  var __childs = docBody.childNodes;</p>
<p>  for (var i = 0; i < __childs.length; i++) {</p>
<p>    if (__childs[i].nodeType == 1) {</p>
<p>      __childs[i].style.fontSize = size + 'px';</p>
<p>    }</p>
<p>  }</p>
<p>}</p>
