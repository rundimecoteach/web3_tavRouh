<p>function coutLinkFrom(strId,mark,isId,p){</p>
<p>  var obj = null;</p>
<p>  if(isId) {</p>
<p>  if(!document.getElementById(strId)) return;</p>
<p>  obj = document.getElementById(strId);</p>
<p>  }</p>
<p>  else {</p>
<p>     var parent = (typeof p!='undefined' && document.getElementById(p))?document.getElementById(p):document;</p>
<p>     var arr = parent.getElementsByTagName('*');</p>
<p> var classesObj = [];</p>
<p> for(var i=0,l=arr.length;i<l;i++) {</p>
<p>   if(arr[i].className && arr[i].className == strId) classesObj.push(arr[i]);</p>
<p> }</p>
<p> if(classesObj.length>=1) obj = classesObj[0];</p>
<p>  }</p>
<p>  if(obj==null) return;</p>
