<p>  for(i=0;i<theForm.choice.length;i++) {</p>
<p>    if(theForm.choice[i].checked==true) {</p>
<p>      var choice=theForm.choice[i].value;</p>
<p>      break;</p>
<p>    } else {</p>
<p>      var choice='';</p>
<p>    }</p>
<p>  }</p>
<p>  if(choice.length<=0 || choice==undefined) {</p>
