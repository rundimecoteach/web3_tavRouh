<p>(function ($, NCA) {</p>
<p>	NCA.page = NCA.page || {};</p>
<p>	NCA.page.story = function(){</p>
<p>		var saveid, $storyheader = $('#story div.story-header');</p>
<p>		if ($('#story').length) {</p>
<p>			saveid = location.href;</p>
<p>	        if (saveid) {</p>
<p>	            saveid = saveid.match(/(story|gallery)+-+(\w{8})+-(\d{13})/);</p>
<p>				if (saveid) {</p>
<p>				    $('#story div.story-header').attr('id', saveid[0]);</p>
<p>				}</p>
<p>	        }</p>
<p>			$storyheader.saveicon({ 'appendto': '.story-headline', 'flavourtext': ['<strong>Save this story</strong> to read later', '<strong>This story is saved</strong> to read later'] });</p>
<p>			$storyheader.append($('#story div.story-summary-list'));</p>
<p>			$storyheader.append($('div.plmnt-alsoin')); // may use IDs for speed</p>
<p>			$('#story div.story-intro').before($('#story div.story-promo-middle'));</p>
<p>			$('#story div.story-intro').before($('#story div.story-sidebar'));</p>
<p>			$storyheader.append($('#story-social'));</p>
<p>			$storyheader.breadcrumbs();</p>
<p>		}</p>
<p>	};</p>
<p>}(ndm.jQuery, ndm.newscomau));</p>
