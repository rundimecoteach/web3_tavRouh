<p>	$(document).ready(function() {</p>
<p>        initFB();</p>
<p>		tryVK();</p>
<p>	});</p>
<p>	</p>
<p>	function tryVK() {</p>
<p>		requirejs.config({</p>
<p>		   baseUrl: 'http://userapi.com/js/api/',</p>
<p>		   urlArgs: ''</p>
<p>		});</p>
<p>		</p>
<p>		require(['openapi'], function () {</p>
<p>			initVK();</p>
<p>			tryTwitter();</p>
<p>		}, function (err) {</p>
<p>			tryTwitter();</p>
<p>		});</p>
<p>	};</p>
<p>	</p>
<p>	function tryTwitter() {</p>
<p>		requirejs.config({</p>
<p>		   baseUrl: 'http://platform.twitter.com/'</p>
<p>		});</p>
<p>		</p>
<p>		require(['widgets'], function () {</p>
<p>			initTwitter();</p>
<p>			tryOdkl();</p>
<p>		}, function (err) {</p>
<p>			tryOdkl();</p>
<p>		});</p>
<p>	};</p>
<p>	</p>
<p>	function tryOdkl() {</p>
<p>		requirejs.config({</p>
<p>		   baseUrl: 'http://stg.odnoklassniki.ru/share/'</p>
<p>		});</p>
<p>		</p>
<p>		require(['odkl_share'], function () {</p>
<p>			initOdkl();</p>
<p>		}, function (err) {});</p>
<p>	};</p>
<p>	</p>
<p>	function initFB() {</p>
<p>		if ( window.FB ) {</p>
<p>			$('.facebookButtonBig').css({visibility: 'visible'});</p>
<p>			_ga.trackFacebook();</p>
<p>		};</p>
<p>	};</p>
<p>	</p>
<p>	function initVK() {</p>
<p>		if ( window.VK ) {</p>
<p>			$('.VKbuttonBig').css({visibility: 'visible'});</p>
<p>			try { VK.init({apiId: 2947582, onlyWidgets: true}) } catch(err) {};</p>
<p></p>
<p>			var title_654651 = $('TITLE').text();</p>
<p>			try {</p>
<p>				VK.Widgets.Like("vk_like", {width: 130, pageTitle: title_654651, type: "button"});</p>
<p>				_ga.trackVkontakte();</p>
<p>			} catch(err) { $('.VKbuttonBig').hide() }</p>
<p>		};</p>
<p>	};</p>
<p>	</p>
<p>	function initTwitter() {</p>
<p>		if ( window.__twttrlr ) {</p>
<p>			$('.twitterButtonBig').css({visibility: 'visible'});</p>
<p>			_ga.trackTwitter();</p>
<p>		}</p>
<p>	};</p>
<p>	</p>
<p>	function initOdkl() {</p>
<p>		if ( window.ODKL ) {</p>
<p>			$(window).load(function() {</p>
<p>				try {</p>
<p>					$('BODY').append('<link href="http://stg.odnoklassniki.ru/share/odkl_share.css" rel="stylesheet" />');</p>
<p>					$('.odnoklassnikiButtonBig').css({visibility: 'visible'});</p>
<p>					ODKL.init();</p>
<p>				} catch(err) {}</p>
<p>			});</p>
<p>		};</p>
<p>	};</p>
<p></p>
