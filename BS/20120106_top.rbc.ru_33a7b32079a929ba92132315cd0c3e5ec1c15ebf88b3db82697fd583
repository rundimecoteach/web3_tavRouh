<p> </p>
<p></p>
<p>			var arrayWithItems = [</p>
<p>				</p>
<p>			];</p>
<p>			</p>
<p>			</p>
<p>				var REDIRECT_URI = 'http://redir.rbc.ru/cgi-bin/redirect.cgi?';</p>
<p>				var ITEMS_TO_SHOW = 1;</p>
<p></p>
<p>				var Cookie = {</p>
<p>					/**</p>
<p>					 * Установка значения куки</p>
<p>					 * 1) если на задавать параметр expires, то кука установится на год</p>
<p>					 * 2) чтобы удалить куку нужно задать отрицательное значение параметра expires</p>
<p>					 * 3) если задать параметр expires как "session", то кука станет сессионной</p>
<p>					 * @param {String} name</p>
<p>					 * @param {String} value</p>
<p>					 * @param {Number} expires</p>
<p>					 */</p>
<p>					set: function( name, value, expires, domain ){</p>
<p>						// дата истечения срока действия куки</p>
<p>						var expiresValue = '';</p>
<p>						if( expires !== 'session' ){</p>
<p>							expiresValue = 'expires=' +</p>
<p>								( new Date( new Date().getTime() + ( expires || 365 ) * 86400000 ) ).toGMTString() + '; '</p>
<p>						}</p>
<p>						// запись куки</p>
<p>						document.cookie = name + '=' + value + '; ' + expiresValue + 'path=/' + ((domain) ? '; domain=' + domain : '');</p>
<p></p>
<p>					},</p>
<p>					/**</p>
<p>					 * Получение значения куки</p>
<p>					 * @param {String} name</p>
<p>					 * @return значение куки с именем name</p>
<p>					 * @type String</p>
<p>					 */</p>
<p>					get: function( name ){</p>
<p>						var re = new RegExp( '\\s*' + name + '=' );</p>
<p>						var cookies = document.cookie.split(';');</p>
<p>						// проверка всех кук страницы</p>
<p>						for( var i = cookies.length; i--; ){</p>
<p>							var c = cookies[i];</p>
<p>							if ( re.test( c ) ){ return c.replace( re, '' ); }</p>
<p>						}</p>
<p>						return '';</p>
<p>					}</p>
<p>				};</p>
<p></p>
<p>				</p>
<p>				window.onload = show_informers();</p>
<p>				</p>
<p>				function show_informers() {</p>
<p>					var countInf = 0;</p>
<p>					return function() {</p>
<p>						if ( countInf++ > 0 ) return;</p>
<p>						//Cookie.set('someCounter','3');</p>
<p>	</p>
<p>						//загружаем массив из куки</p>
<p>						var firstElementNumber = Cookie.get('firstElementNumber');</p>
<p>						if (firstElementNumber) {</p>
<p>							var newArray = [];</p>
<p>							var y = firstElementNumber;</p>
<p>							for (var i = 0; i < arrayWithItems.length; i++) {</p>
<p>								newArray.push(arrayWithItems[y]);</p>
<p>								y++;</p>
<p>								if (y == arrayWithItems.length) {</p>
<p>									y = 0;</p>
<p>								}</p>
<p>							}</p>
<p>							arrayWithItems = newArray;</p>
<p>						}</p>
<p>						firstElementNumber++;</p>
<p>						if (firstElementNumber >= arrayWithItems.length) {</p>
<p>							firstElementNumber = 0;</p>
<p>						}</p>
<p>						//записываем новый порядок к куку</p>
<p>						Cookie.set('firstElementNumber', firstElementNumber);</p>
<p>	</p>
<p>						var informerContainer = document.getElementById('informer');</p>
<p>						var oldContent = informerContainer.innerHTML;</p>
<p>	</p>
<p>						informerContainer.innerHTML = '';</p>
<p>						var htmlCache = printInformer( arrayWithItems, ITEMS_TO_SHOW );</p>
<p>	</p>
<p>						for ( var i = 0; i < htmlCache.length; i++ ) {</p>
<p>							informerContainer.appendChild( htmlCache[i] );</p>
<p>						}</p>
<p>	</p>
<p>						informerContainer.innerHTML += oldContent;</p>
<p>					}</p>
<p></p>
<p>				}</p>
<p>				</p>
<p></p>
<p>                function create_element_first( itemToShow ) {</p>
<p>                    var div = document.createElement('div');</p>
<p>                    div.align = 'center';</p>
<p></p>
<p>                    var href = document.createElement('a');</p>
<p>                    href.href = REDIRECT_URI + itemToShow.url;</p>
<p>                    href.title = itemToShow.header;</p>
<p></p>
<p>                    var img = document.createElement('img');</p>
<p>                    img.src = itemToShow.imgSrc;</p>
<p>                    img.alt = itemToShow.header;</p>
<p>                    img.width = 145;</p>
<p>                    img.height = 96;</p>
<p></p>
<p>                    href.appendChild( img );</p>
<p>                    div.appendChild( href );</p>
<p></p>
<p>                    img = null; href = null;</p>
<p></p>
<p>                    return div;</p>
<p>                }</p>
<p></p>
<p>                function create_element_seconds( itemToShow ) {</p>
<p>                    var li = document.createElement('li');</p>
<p></p>
<p>                    var href = document.createElement('a');</p>
<p>                    href.href = REDIRECT_URI + itemToShow.url;</p>
<p>                    href.title = itemToShow.header;</p>
<p></p>
<p>                    var text = document.createTextNode( itemToShow.header );</p>
<p>                    href.appendChild( text );</p>
<p></p>
<p>                    li.appendChild( href );</p>
<p></p>
<p>                    href = null; text = null;</p>
<p></p>
<p>                    return li;</p>
<p>                }</p>
<p></p>
<p>				function printInformer(array, numberOfItemsToShow) {</p>
<p>					htmlCache = [];</p>
<p>					var itemToShow = get_informer( array[0] );</p>
<p>					if ( itemToShow ) {</p>
<p>						htmlCache.push( create_element_first( itemToShow ) );</p>
<p>						htmlCache.push( create_element_seconds( itemToShow ) );</p>
<p>					}</p>
<p>					for ( var i = 1; i < numberOfItemsToShow; i++ ) {</p>
<p>						itemToShow = get_informer( array[i] );</p>
<p>						if ( itemToShow ) {</p>
<p>							htmlCache.push( create_element_seconds( itemToShow ) );</p>
<p>						}</p>
<p>					}</p>
<p></p>
<p>					return htmlCache;</p>
<p>				}</p>
<p></p>
<p>				function get_informer( infpl ) {</p>
<p>					if ( ! infpl )			return false;</p>
<p>					if ( ! infpl.length )	return false;</p>
<p>					var num = Math.floor( Math.random() * infpl.length );</p>
<p>					return infpl[ num ];</p>
<p>				}</p>
