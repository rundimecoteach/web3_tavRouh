<p> var headerHeight;</p>
<p> var orgHeight;</p>
<p> var headline_count;</p>
<p> var headline_interval;</p>
<p> var old_headline = 0;</p>
<p> var current_headline = 0;</p>
<p></p>
<p>$(document).ready(function(){</p>
<p>	headerHeight = $("#header").css('height');</p>
<p>	orgHeight = headerHeight;</p>
<p>	headerHeight = parseInt(headerHeight);</p>
<p>	headerHeight = headerHeight - 75;</p>
<p>	headerHeight = headerHeight / 2;</p>
<p>	</p>
<p>	$("#scrollup").css('height',orgHeight);</p>
<p></p>
<p>   headline_count = $("div.scrollheadline").size();</p>
<p>   $("div.scrollheadline:eq("+current_headline+")").css('top',headerHeight + 'px');</p>
<p> </p>
<p>   headline_interval = setInterval(headline_rotate,8000); //time in milliseconds</p>
<p>   });</p>
<p> </p>
<p> function headline_rotate() {</p>
<p>   current_headline = (old_headline + 1) % headline_count; </p>
<p>   $("div.scrollheadline:eq(" + old_headline + ")").animate({top: -205},2000, function() {</p>
<p>     $(this).css('top','210px');</p>
<p>   });</p>
<p>   $("div.scrollheadline:eq(" + current_headline + ")").show().animate({top: headerHeight},2000);  </p>
<p>   old_headline = current_headline;</p>
<p> }</p>
<p></p>
<p></p>
<p></p>
<p></p>
<p>#scrollup {</p>
<p>   position: relative;</p>
<p>   overflow: hidden;</p>
<p>   height: 90px;</p>
<p>   width: 250px;</p>
<p> }</p>
<p> .scrollheadline {</p>
<p>   position: absolute;</p>
<p>   top: 210px;</p>
<p>   left: 5px;</p>
<p>   height: 75px;</p>
<p>   width: 250px;</p>
<p> }</p>
