<p>function showModalBox() {</p>
<p></p>
<p>	jQuery('#dialogBox').empty();</p>
<p></p>
<p>	jQuery('#glassBox').fadeTo(400, 0.2);</p>
<p>	jQuery('#notLoggedInBox').clone().appendTo('#dialogBox');</p>
<p>	jQuery('#dialogBox').find('.noAccessLoginBox').attr('id',</p>
<p>			'notLoggedInBoxModal').find('.noAccessLoginBox_closeIcon').show();</p>
<p>	jQuery('#dialogBox').find('.closeBoxString').show();</p>
<p>	jQuery('#dialogBox').show().addClass('noAccessLoginBoxDialog')</p>
<p>			.css(</p>
<p>					'left',</p>
<p>					(document.body.clientWidth - jQuery('#dialogBox')</p>
<p>							.outerWidth()) / 2);</p>
<p></p>
<p>	jQuery('.noAccessLoginBox_closeIcon').bind('click', function(event) {</p>
<p>		jQuery('#dialogBox').hide().empty();</p>
<p>		jQuery('#glassBox').fadeOut(400);</p>
<p>		jQuery(".storyContent").find('p').show();</p>
<p>		jQuery('#dialogBox').removeClass('noAccessLoginBoxDialog');</p>
<p>		setCookie('closeId', '774033_1324897');</p>
<p>		location.href = 'http://www.rp.pl/artykul/61991,774033-Brytyjski-wirus-eurozy.html';</p>
<p>	});</p>
<p>	jQuery('.closeBoxString').bind('click', function(event) {</p>
<p>		jQuery('#dialogBox').hide().empty();</p>
<p>		jQuery('#glassBox').fadeOut(400);</p>
<p>		jQuery(".storyContent").find('p').show();</p>
<p>		jQuery('#dialogBox').removeClass('noAccessLoginBoxDialog');</p>
<p>		setCookie('closeId', '774033_1324897');</p>
<p>		location.href = 'http://www.rp.pl/artykul/61991,774033-Brytyjski-wirus-eurozy.html';</p>
<p>	});</p>
<p>}</p>
<p></p>
<p>function hideAndShow() {</p>
<p>	jQuery("#story").find('p').hide();</p>
<p>	jQuery("#story").find('p.lead').show();</p>
<p>	jQuery("#story").find('p:eq(1)').show();</p>
<p></p>
<p>}</p>
