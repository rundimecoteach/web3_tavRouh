<p></p>
<p>        if (plyAudioPlayer.view) {</p>
<p>            plyAudioPlayer.sendEvent('PLAY');</p>
<p>        } else {</p>
<p>            setTimeout(startPlayer, 100);</p>
<p>        }</p>
<p>    }</p>
<p></p>
<p>    function startPlayer() {</p>
<p>        try {</p>
<p>            plyAudioPlayer.sendEvent('PLAY');</p>
<p>        }</p>
<p>        catch (ex) { }</p>
<p>    }</p>
