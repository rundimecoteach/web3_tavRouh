<p>    url: "/forum/postn.jsp?news_id="+DigId,</p>
<p>    async: true,</p>
<p>    success: function(html){</p>
<p>     jQuery("#postn").html(html);</p>
<p>    },</p>
<p>    error: function() {}</p>
<p>  });</p>
<p></p>
<p>}</p>
