<p>function getByteLength(str){</p>
<p>  var iCount = 0;</p>
<p>  for( k=0; k<str.length; k++ ) {</p>
<p>    c = str.charCodeAt(k);</p>
<p>    if ( c>=32 && c<=126 ) iCount++;</p>
<p>    else iCount += 2;</p>
<p>  }</p>
<p>  return iCount;</p>
<p>}</p>
<p></p>
<p>function getSubString(str,maxwclength) {</p>
<p>  var iCount = 0;</p>
<p></p>
<p>  for( k=0; k<str.length; k++ ) {</p>
<p>    c = str.charCodeAt(k);</p>
<p>    if ( c>=32 && c<=126 ) iCount++;</p>
<p>    else iCount += 2;</p>
<p></p>
<p>    if (iCount > maxwclength) {</p>
<p>      str = str.substr(0,k) + "...";</p>
<p>      break;</p>
<p>    }</p>
<p>  }</p>
<p></p>
<p>  return str;</p>
<p>}</p>
