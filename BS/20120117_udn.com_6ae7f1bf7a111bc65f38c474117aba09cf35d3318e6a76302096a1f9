<p>function chgFontImg(size) {</p>
<p></p>
<p>  jQuery("#fs4").attr("src","/2010/images/fontsize_4.gif");</p>
<p>  jQuery("#fs3").attr("src","/2010/images/fontsize_3.gif");</p>
<p>  jQuery("#fs2").attr("src","/2010/images/fontsize_2.gif");</p>
<p>  jQuery("#fs1").attr("src","/2010/images/fontsize_1.gif");</p>
<p></p>
<p>  jQuery("#fs4").bind("mouseleave",function() {</p>
<p>    this.src='/2010/images/fontsize_4.gif'</p>
<p>  });</p>
<p>  jQuery("#fs3").bind("mouseleave",function() {</p>
<p>    this.src='/2010/images/fontsize_3.gif'</p>
<p>  });</p>
<p>  jQuery("#fs2").bind("mouseleave",function() {</p>
<p>    this.src='/2010/images/fontsize_2.gif'</p>
<p>  });</p>
<p>  jQuery("#fs1").bind("mouseleave",function() {</p>
<p>    this.src='/2010/images/fontsize_1.gif'</p>
<p>  });</p>
<p></p>
<p>  if (size == "4") {</p>
<p>    jQuery("#fs4").attr("src","/2010/images/fontsize_4o.gif");</p>
<p></p>
<p>    jQuery("#fs4").bind("mouseleave",function() {</p>
<p>      this.src='/2010/images/fontsize_4o.gif'</p>
<p>    });</p>
<p>  }</p>
<p>  if (size == "3") {</p>
<p>    jQuery("#fs3").attr("src","/2010/images/fontsize_3o.gif");</p>
<p></p>
<p>    jQuery("#fs3").bind("mouseleave",function() {</p>
<p>      this.src='/2010/images/fontsize_3o.gif'</p>
<p>    });</p>
<p>  }</p>
<p>  if (size == "2") {</p>
<p>    jQuery("#fs2").attr("src","/2010/images/fontsize_2o.gif");</p>
<p></p>
<p>    jQuery("#fs2").bind("mouseleave",function() {</p>
<p>      this.src='/2010/images/fontsize_2o.gif'</p>
<p>    });</p>
<p>  }</p>
<p>  if (size == "1") {</p>
<p>    jQuery("#fs1").attr("src","/2010/images/fontsize_1o.gif");</p>
<p></p>
<p>    jQuery("#fs1").bind("mouseleave",function() {</p>
<p>      this.src='/2010/images/fontsize_1o.gif'</p>
<p>    });</p>
<p>  }</p>
<p></p>
<p>}</p>
