<p>function rId() {</p>
<p>  var url=window.location.toString();</p>
<p>  var ary=url.split("/")</p>
<p>  var page=ary[ary.length-1];</p>
<p>  if (page.indexOf("_") != -1) {</p>
<p>    _ary=page.split("_");</p>
<p>    var DigId=_ary[0];</p>
<p>  }</p>
<p>  else {</p>
<p>    var ary2=page.split(".");</p>
<p>    var DigId=ary2[0];</p>
<p>  }</p>
<p></p>
<p>  return DigId;</p>
<p>}</p>
<p></p>
<p>function InitDig(page) {</p>
<p>  DigId = rId();</p>
<p>  jQuery(".content_hot").append("<div id='dignews'></div>");</p>
<p>  jQuery("#dignews").html("<center><img src='/images/forumimg/images/loading2.gif?1x'><br /><br /><font style='font-weight:bolder;font-size:16px'>討論區塊載入中</font></center>");</p>
<p></p>
<p>  if (page == "") page = 1;</p>
<p>  setTimeout("ShowDigContent("+DigId+",'b',"+page+")",100);</p>
<p>  ShowDigCount();</p>
<p>}</p>
<p></p>
<p>function ShowDig(page) {</p>
<p>  DigId = rId();</p>
<p></p>
<p>  jQuery("#dignews").html("<center><img src='/images/forumimg/images/loading2.gif?xx'><br /><br /><font style='font-weight:bolder;font-size:16px'>討論區塊載入中</font></center>");</p>
<p></p>
<p>  if (page == "") page = 1;</p>
<p>  setTimeout("ShowDigContent("+DigId+",'a',"+page+")",100);</p>
<p>}</p>
<p></p>
<p>function ShowDigCount() {</p>
