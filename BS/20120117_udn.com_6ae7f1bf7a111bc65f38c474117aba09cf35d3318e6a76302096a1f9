<p>function chgFontSize(size) {</p>
<p>  if (size == "4") {</p>
<p>    jQuery("div#story").css("font-size","20px");</p>
<p>    jQuery("div#story").css("line-height","190%");</p>
<p>   // jQuery("div.menu_block > span").each(function () { jQuery(this).css("font-size","12pt")});</p>
<p>    chgFontImg("4");</p>
<p>    setCookie("FontSize","4",false,"/","udn.com","","null");</p>
<p>  }</p>
<p>  else if (size == "3") {</p>
<p>    jQuery("div#story").css("font-size","18px");</p>
<p>    jQuery("div#story").css("line-height","190%");</p>
<p>    chgFontImg("3");</p>
<p>    setCookie("FontSize","3",false,"/","udn.com","","null");</p>
<p>  }</p>
<p>  else if (size == "2") {</p>
<p>    jQuery("div#story").css("font-size","15px");</p>
<p>    jQuery("div#story").css("line-height","190%");</p>
<p>    chgFontImg("2");</p>
<p>    setCookie("FontSize","2",false,"/","udn.com","","null");</p>
<p>  }</p>
<p>  else {</p>
<p>    jQuery("div#story").css("font-size","13px");</p>
<p>    jQuery("div#story").css("line-height","190%");</p>
<p>    chgFontImg("1");</p>
<p>    setCookie("FontSize","1",false,"/","udn.com","","null");</p>
<p>  }</p>
<p>}</p>
