<p>function ShowDigContent(DigId,TUrl,page) {</p>
<p>  //var htmlContent = </p>
<p>  jQuery.ajax({</p>
<p>    url: "/forum/post"+TUrl+".jsp?news_id="+DigId+"&currentPage="+page,</p>
<p>    async: true,</p>
