<p>    getObjPos = function(obj){</p>
<p>        var topVal= 0,leftVal= 0;</p>
<p>        while(obj){</p>
<p>            leftVal += obj.offsetLeft;</p>
<p>            topVal += obj.offsetTop;</p>
<p>            obj = obj.offsetParent;</p>
<p>        }</p>
<p>        return {"left":leftVal,"top":topVal};</p>
<p>    }</p>
<p></p>
<p>    positionPlayer = function(obj){</p>
<p></p>
<p>        objPos = $(obj).offset();</p>
<p>        storyCoreObjPos = $('#story_core').offset();</p>
<p>        mediaplayerObjPos = $('#mediaplayerContainer').offset();</p>
<p></p>
<p>        $('#mediaplayerContainer').css('position', 'absolute');</p>
<p>        $('#mediaplayerContainer').css('z-index', '1003');</p>
<p>        $('#mediaplayerContainer').offset({top: objPos.top, left: objPos.left});</p>
<p></p>
<p>        mediaplayerObjPosNew = $('#mediaplayerContainer').offset();</p>
<p>    }</p>
