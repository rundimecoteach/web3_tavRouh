<p>	var t;</p>
<p>		</p>
<p>	function autoRun(){</p>
<p>		var $mt = parseInt($block.css('marginTop'));</p>
<p>		$mt == -29 ? $block.animate({'marginTop':0}, 500) : $block.animate({'marginTop':-29}, 500);</p>
<p>	}</p>
<p>	</p>
<p>	function autoStart(){</p>
<p>		t = setInterval(autoRun, 3000);</p>
<p>	}</p>
<p>	</p>
<p>	autoStart();</p>
<p>	</p>
<p>	$tt.click(function(){</p>
<p>		clearInterval(t);</p>
<p>		var $mt = parseInt($block.css('marginTop'));</p>
<p>		$mt == -29 ? $block.animate({'marginTop':0}, 500) : $block.animate({'marginTop':-29}, 500);</p>
<p>		autoStart();</p>
<p>	});</p>
<p>	</p>
<p>	$tb.click(function(){</p>
<p>		clearInterval(t);</p>
<p>		var $mt = parseInt($block.css('marginTop'));</p>
<p>		$mt == -29 ? $block.animate({'marginTop':0}, 500) : $block.animate({'marginTop':-29}, 500);</p>
<p>		autoStart();</p>
<p>	});</p>
<p>})</p>
