<p>	//头部搜索</p>
<p>	var suggestStock_0;</p>
<p>	var suggestFund_0;</p>
<p>	function toSearch(){</p>
<p>		var searchType1 = document.getElementById("typeSelect_0");</p>
<p>		var value2=document.getElementById('suggestInput_0').value;</p>
<p>		var sc="sh"</p>
<p>		if(value2.substring(0,1)=='0' || value2.substring(0,1)=='2' || value2.substring(0,1)=='3'){</p>
<p>		sc="sz";</p>
<p>		}</p>
<p>		if(searchType1.value=="3"){</p>
<p>			if(value2 !="新闻内容" && value2 !=""){</p>
<p>			var searchURL="http://newsearch.stcn.com/fly/search.jsp?searchText="+value2;</p>
<p>			searchURL=encodeURI(searchURL)</p>
<p>			window.open(searchURL);</p>
<p>			}</p>
<p>		}else if(searchType1.value =='1' || searchType1.value=='2'){</p>
<p>			if (value2 != "代码/名称/拼音" && value2 != "") {</p>
<p>				if(searchType1.value == "1"){</p>
<p>				var stringUrl = "http://stock.stcn.com/"+sc+"/@CODE@/";</p>
<p>				window.open(stringUrl.replace("@CODE@", value2));</p>
<p>				}else if(searchType1.value =="2"){</p>
<p>				var stringUrl = "http://fund.stcn.com/final/@CODE@/";</p>
<p>				window.open(stringUrl.replace("@CODE@", value2));</p>
<p>				}</p>
<p>			}</p>
<p>		}</p>
<p>	}</p>
<p>	function changeViewInputs(key, index) {</p>
<p>		switch (key) {</p>
<p>			case "1":</p>
<p>			suggestStock_0 = new Suggest("suggestInput_0", "代码/名称/拼音", stock_suggest, null, StockSuggestConfiguration, toSearch);</p>
<p>			suggestStock_0.options[12] = false;</p>
<p>			break;</p>
<p>			case "2":</p>
<p>			suggestFund_0 = new Suggest("suggestInput_0", "代码/名称/拼音", fund_suggest, null, FundSuggestConfiguration, toSearch);</p>
<p>			suggestFund_0.options[12] = false;</p>
<p>			break;</p>
<p>			case "3":</p>
<p>			var $s = $('#suggestInput_0').siblings('span');</p>
<p>			$s.remove();</p>
<p>			document.getElementById("suggestInput_0").value="新闻内容";</p>
<p>			break;</p>
<p>	    }</p>
<p>	}</p>
<p>	//头部搜索结束 </p>
<p>	function AddFavorite() { </p>
<p>	  var biaoti = document.getElementById("biaoti").value;</p>
<p>		try { </p>
<p>			window.external.addFavorite(document.location.href, biaoti); </p>
<p>		} catch (e) { </p>
<p>			try { </p>
<p>				window.sidebar.addPanel(biaoti, document.location.href, ""); </p>
<p>			} catch (e) { </p>
<p>				alert("加入收藏失败,请手动添加."); </p>
<p>			} </p>
<p>		} </p>
<p>   } </p>
<p>function copyToClipBoard(){</p>
<p>	var clipBoardContent="";</p>
<p>	//clipBoardContent+=document.title;</p>
<p>	clipBoardContent+="\n";</p>
<p>	clipBoardContent+=this.location.href; //获取地址</p>
<p>	window.clipboardData.setData("Text",clipBoardContent);</p>
<p>	alert("复制成功");</p>
<p>} </p>
