<p>	$mainNavLi.hover(function(){</p>
<p>		$mainNavStrong.addClass('show_downNav');</p>
<p>		$downNav.show();</p>
<p>	}, function(){</p>
<p>		$mainNavStrong.removeClass('show_downNav');</p>
<p>		$downNav.hide();</p>
