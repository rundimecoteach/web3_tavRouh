<p>		* Verify that there are actually ads to display.</p>
<p>		*/</p>
<p>		if (google_ads.length == 0)</p>
<p>			return;</p>
<p></p>
<p>		/*</p>
<p>		* If an image or Flash ad is returned, display that ad.</p>
<p>		* If a rich media ad is returned, display that as "as is."</p>
<p>		* Otherwise, build a string containing all of the ads and</p>
<p>		* then use a document.write() command to print that string.</p>
<p>		*/</p>
<p>		/*attributes ad to Google*/</p>
<p>		if (google_ads[0].type == "text") {</p>
<p>			s += '<div id="sponsoredHeader" align="left"><a href="http://cbsiprivacy.custhelp.com/app/answers/detail/a_id/1272/">'+google_attribution +'</a></div><div id="sponsoredContents">';</p>
<p></p>
<p>			/*</p>
<p>			* Larger text for single ad.</p>
<p>			*/</p>
<p>			if (google_ads.length == 1) {</p>
<p>				title_span = '<span class="adlinks_header">';</p>
<p>				description_span = '<span class="adlinks_body">';</p>
<p>				url_style =' class="adlinks_body">' ;</p>
<p>			}</p>
<p></p>
<p>			/*</p>
<p>			* For text ads, append each ad to the string.</p>
<p>			*/</p>
<p>			for(i=0; i < google_ads.length; ++i) {</p>
<p>	</p>
<p>				s += '<a style="font-weight: bold;" href="'+google_ads[i].url+'" target="new" onmouseover="window.status=\'http:\/\/'+google_ads[i].visible_url+'\'; return true;" onmouseout="window.status=\'\';" class="adlinks_link">' +title_span +'' + google_ads[i].line1 + '</a><br />';</p>
<p>				if (clickable_background == 0){</p>
<p>					s += '</a>';</p>
<p>				}</p>
<p>				s += google_ads[i].line2 + '<br>' +google_ads[i].line3 + '</a><br>';</p>
<p>				if (clickable_background == 1){</p>
<p>					s += '</a>';</p>
<p>				}</p>
<p></p>
<p>				s += '<a href="'+google_ads[i].url+'" target="new" onmouseover="window.status=\'http:\/\/'+google_ads[i].visible_url+'\'; return true;" onmouseout="window.status=\'\';" ' +url_style +google_ads[i].visible_url + '</a>&nbsp;<img src="http://wwwimage.cbsnews.com/common/images/misc_new_window.gif" alt="New Window" /><br />';</p>
<p>				if(i < google_ads.length-1){</p>
<p>					s += '<br />';</p>
<p>				}</p>
<p>			}</p>
<p>		}</p>
<p>		s += '</div>';</p>
<p>		document.write(s);</p>
<p>		return;</p>
<p>	}</p>
<p></p>
