<p>function jumpContentPage(cat, period, mediaFilter, newsId)	{</p>
<p>var _url = document.location.href;</p>
<p>if (/mobile/.test(_url))	{</p>
<p>Channel.util.open_url('/mobile/news/'+cat+'/'+period+'/'+mediaFilter+'/'+newsId+'/1.html', 'no');</p>
<p>}	else	{</p>
<p>Channel.util.open_url('/news/'+cat+'/'+period+'/'+mediaFilter+'/'+newsId+'/1.html', 'no');</p>
<p>}</p>
<p>return false;</p>
<p>}</p>
