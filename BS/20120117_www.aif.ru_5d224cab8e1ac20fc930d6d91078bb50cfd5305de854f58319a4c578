<p> function setAifCookie(name, value, params)</p>
<p>{</p>
<p>// не переписываем имеющуюся куку!</p>
<p>if (jQuery.cookie(name) != null) {</p>
<p>//alert(jQuery.cookie(name));</p>
<p>return false;</p>
<p>}</p>
<p></p>
<p>if (params == null){</p>
<p>jQuery.cookie(name, value, {expires: 7, path: '/', secure: false});</p>
<p>    } else {</p>
<p>jQuery.cookie(name, value, params);</p>
<p>}</p>
<p>return true;</p>
<p>}   </p>
<p>    </p>
<p>$comments_inp = false;</p>
<p>$(document).ready(function()</p>
<p>{</p>
<p>    </p>
<p>   </p>
<p>	var login = $.cookie('login');</p>
<p>	var hash = $.cookie('hash');</p>
<p></p>
<p>	if (login == null)</p>
<p>	{</p>
<p>		// BC with old auth!</p>
<p>                </p>
<p>		if ( hash != null ) {</p>
<p>			</p>
<p>                        $.post('/ajax/setlogin', function(data) {$('#user_name').val(data.result); $('#user_name').attr('readonly', true); $('#login_msg').html('Зарегистрированный пользователь:');});</p>
<p>			</p>
<p>                        $('#user_email').hide();</p>
<p>                   </p>
<p>			$('#auth-register').hide();</p>
<p>                        </p>
<p>		} else { // u/a</p>
<p>                   </p>
<p>			setAifCookie('comments', '4efc15e5f7de7fbc785f8df14601d669', {expires: 7, path: '/', domain: 'www.aif.ru', secure: false});</p>
<p>			</p>
<p>                        $('#user_name').val('');</p>
<p>		}</p>
<p></p>
<p>	} else {</p>
<p></p>
<p>$('#user_label').text('');</p>
<p>		$('#user_email').hide();</p>
<p>		$('.auth-register').html('<a href="/auth/logout">выйти</a>');</p>
<p>		$('#user_name').val(login);</p>
<p>		$('#user_name').attr('readonly', true);</p>
<p>		//$('#login_msg').html('Зарегистрированный пользователь:');</p>
<p>	}</p>
<p>        </p>
<p>          </p>
<p>	$('#ajaxform').submit(function(e){</p>
<p>		e.preventDefault();</p>
<p>		var action = $(this).attr('action'), data = $(this).serialize();</p>
<p>		$.post(action, data, function(r){</p>
<p>			if('ok'==r.result) {</p>
<p>                            </p>
<p>                            				</p>
<p>                                $('#article-comments').html(r.content);</p>
<p>				$('#comments_form').load('http://www.aif.ru/aforism/?'+Math.random());</p>
<p>                            </p>
<p>                                                         </p>
<p>			} else {</p>
<p>				alert(r.result);</p>
<p>			}</p>
<p>		}, 'json');</p>
<p>               </p>
<p>	});</p>
<p></p>
<p></p>
<p>  $("a.reply").click(function()</p>
<p>	{</p>
<p>		if ($comments_inp) {</p>
<p>			var cmt = $('#comments_inp').val() + "\n"; // wipe rules</p>
<p>		} else {</p>
<p>			var cmt = '';</p>
<p>			$comments_inp = true;</p>
<p>			$('#comments_inp').css('color', '#000000');</p>
<p>		}</p>
<p></p>
<p>		var comment_id = $(this).attr('href').replace(/[^0-9]+/, '');</p>
<p>		var author = $('div.username', '#meta'+ comment_id).text();</p>
<p></p>
<p>		var comment = $('blockquote', '#comment'+ comment_id).text();</p>
<p></p>
<p>		//var ocomment = $('#comment'+ comment_id);//alert(ocomment);</p>
<p>		//var selected = comment.text().substring(range.start, range.end); alert(selected);</p>
<p></p>
<p>		//$('#comments_inp').val(cmt + author + " писал(а):\n" + comment + "\n---\n");</p>
<p>		$('#comments_inp').val(cmt + author + " писал(а):\n<i>" + comment + "</i>\n---\n");</p>
<p>		$('#comments_inp').focus();</p>
<p>        return false;</p>
<p>  });</p>
<p>});</p>
