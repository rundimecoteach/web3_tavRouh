<p>        function checkValue(count) {</p>
<p>            if (document.getElementById("stockCode_" + count)) {</p>
<p>                var stockcode = document.getElementById("stockCode_" + count).value;</p>
<p>                if (!isStock(stockcode)) {</p>
<p>                    alert("请您填写正确的股票代码以便及时获得投资情报");</p>
<p>                    //document.getElementById("stockCode").focus();</p>
<p>                    return false;</p>
<p>                }</p>
<p>            }</p>
<p>            //var description = document.getElementById("description").value;</p>
<p>            if (document.getElementById("phone1_" + count)) {</p>
<p>                var phone1 = document.getElementById("phone1_" + count).value;</p>
<p>                if (!isMobel(phone1)) {</p>
<p>                    alert("请您填写真实手机号以便及时获得投资情报");</p>
<p>                    //document.getElementById("phone1").focus();</p>
<p>                    return false;</p>
<p>                }</p>
<p>            }</p>
<p></p>
<p></p>
<p>        }</p>
<p></p>
<p></p>
<p>        function isStock(value) {</p>
<p>            if (value === null || value.length === 0)</p>
<p>                return false;</p>
<p>            var regex = RegExp("^\\d{6}$");</p>
<p>            if (!value.match(regex)) {</p>
<p>                return false;</p>
<p>            }</p>
<p>            else {</p>
<p>                return true;</p>
<p>            }</p>
<p>        }</p>
<p></p>
<p>        function isMobel(value) {</p>
<p>            if (/^13\d{9}$/g.test(value) || (/^15[0-35-9]\d{8}$/g.test(value)) || (/^18[05-9]\d{8}$/g.test(value))) {</p>
<p>                return true;</p>
<p>            } else {</p>
<p>                return false;</p>
<p>            }</p>
<p>        }</p>
<p></p>
<p>        function showMsg(msg) {</p>
<p>            alert(msg);</p>
<p>        }</p>
