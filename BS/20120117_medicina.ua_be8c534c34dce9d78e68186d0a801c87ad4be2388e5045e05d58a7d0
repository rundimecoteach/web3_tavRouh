<p>var auth;</p>
<p>var sensor;</p>
<p>	</p>
<p>$(document).ready(function(){</p>
<p>	auth = $('#authDiv');</p>
<p>	sensor = $('#sensorShadow');</p>
<p>	closeAuth = $('#close');</p>
<p>	</p>
<p>	sensor.click(function(){</p>
<p>		auth.hide();</p>
<p>		sensor.hide();</p>
<p>	})</p>
<p></p>
<p>	closeAuth.click(function(){</p>
<p>		auth.hide();</p>
<p>		sensor.hide();</p>
<p>	})</p>
<p>	</p>
<p>})</p>
<p></p>
<p>function showAuth(id) {</p>
<p>	id.blur();</p>
<p></p>
<p>	auth.css({</p>
<p>		left: "50%",</p>
<p>		margin: "0 0 0 -160px",</p>
<p>		top: "305px",</p>
<p>		display: "block"</p>
<p>	}); </p>
<p>	sensor.css({</p>
<p>		display: "block",</p>
<p>		height: $(document).height() + "px",</p>
<p>		opacity: 0.5</p>
<p>	});</p>
<p>}</p>
