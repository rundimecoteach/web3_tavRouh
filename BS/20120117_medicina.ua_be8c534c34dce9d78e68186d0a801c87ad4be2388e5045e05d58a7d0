<p>	//Шилдик</p>
<p>	$("#zeemaLogo")</p>
<p>		.css( {backgroundPosition: "0px 0px"} )</p>
<p>		.mouseover(function(){</p>
<p>			$(this).stop().animate({backgroundPosition:"(0px -31px)"}, {duration:200})</p>
<p>		})</p>
<p>		.mouseout(function(){</p>
<p>			$(this).stop().animate({backgroundPosition:"(0px -1px)"}, {duration:200, complete:function(){</p>
<p>				$(this).css({backgroundPosition: "0px 0px"})</p>
<p>			}})</p>
<p>		})</p>
<p>});</p>
