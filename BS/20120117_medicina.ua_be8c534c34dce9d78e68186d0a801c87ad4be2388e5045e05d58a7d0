<p>	var where_search = 'all';</p>
<p>	function setWhere(where) {</p>
<p>		if(where == 'drugs') {</p>
<p>			document.frmSearch.action='/search_drug/zakaz/';</p>
<p>		}</p>
<p>		else {</p>
<p>			document.frmSearch.action='/search/';</p>
<p>		}</p>
<p>		$('#search_tp').remove();</p>
<p>		$('#search_id').remove();</p>
<p>		if(where=="clinics") {</p>
<p>			$('#frmSearch').append('<input type="hidden" name="tp" id="search_tp" value="ct">');</p>
<p>			$('#frmSearch').append('<input type="hidden" name="id" id="search_id" value="1">');</p>
<p>		}</p>
<p>		else {</p>
<p>			if(where=="diseases") {</p>
<p>				$('#frmSearch').append('<input type="hidden" name="tp" id="search_tp" value="ct">');</p>
<p>				$('#frmSearch').append('<input type="hidden" name="id" id="search_id" value="3">');</p>
<p>			}</p>
<p>			else {</p>
<p>				$('#search_tp').remove();</p>
<p>				$('#search_id').remove();</p>
<p>			}</p>
<p>		}</p>
<p></p>
<p>		$('#where_search_'+where_search).removeClass('act');</p>
<p>		$('#where_search_'+where).addClass('act');</p>
<p></p>
<p>		where_search = where;</p>
<p>	}</p>
