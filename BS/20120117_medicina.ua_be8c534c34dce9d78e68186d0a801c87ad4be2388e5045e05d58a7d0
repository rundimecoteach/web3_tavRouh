<p>	//-----------------------------------</p>
<p>	function valid_form_login(){</p>
<p>		var err = true;</p>
<p>		if($('#login').val() == ''){</p>
<p>		    err = false;</p>
<p>		    $('#err_login').show();</p>
<p>		}</p>
<p>		else{</p>
<p>		    $('#err_login').hide();</p>
<p>		}</p>
<p>			</p>
<p>		if($('#pass').val() == ''){</p>
<p>		    err = false;</p>
<p>		    $('#err_pass').show();</p>
<p>		}</p>
<p>		else{</p>
<p>			$('#err_pass').hide();</p>
<p>		}</p>
<p>			</p>
<p>		if(err)</p>
<p>        	document.form_login.submit();</p>
<p>			</p>
<p>	}</p>
