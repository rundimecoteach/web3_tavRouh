<p>		$(document).ready(function(){</p>
<p>			$(".popup a").hover(function() {</p>
<p>			$(this).next(".pop").stop(true, true).animate({opacity: "show"}, "fast");</p>
<p>			$(this).css('color','#666969');</p>
<p>			$(this).css('text-decoration','underline');</p>
<p>			}, function() {</p>
<p>			$(this).next(".pop").animate({opacity: "hide"}, "fast");</p>
<p>			$(this).css('color','#3e4141')</p>
<p>			$(this).css('text-decoration','none')</p>
<p>			});</p>
<p>		});</p>
