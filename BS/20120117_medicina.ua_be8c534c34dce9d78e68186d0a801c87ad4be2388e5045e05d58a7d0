<p>	$(document).ready(function(){</p>
<p>		$('.m1 .ti').hover(</p>
<p>		function(){</p>
<p>			$(this).find('.t').addClass('hov');</p>
<p>			$(this).find('.child').css('display','block');	</p>
<p>		},</p>
<p>		function(){</p>
<p>			$(this).find('.t').delay('500').removeClass('hov');</p>
<p>			$(this).find('.child').delay('500').css('display','none');	</p>
<p>		});</p>
<p>		</p>
<p>		$('.m1 .tim').hover(</p>
<p>		function(){</p>
<p>			$(this).find('.t').addClass('hove');</p>
<p></p>
<p>		},</p>
<p>		function(){</p>
<p>			$(this).find('.t').removeClass('hove');</p>
<p>		});</p>
<p>			</p>
<p>	});</p>
