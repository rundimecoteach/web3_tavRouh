<p>  jQuery(document).ready(function(){</p>
<p>    var max = 4000;</p>
<p>    </p>
<p>    function przelicz_znaki() {</p>
<p>      var self = jQuery('#komentarz-tresc');</p>
<p>      if(jQuery(self).val().length > max){</p>
<p>        jQuery(self).val(jQuery(self).val().substr(0, max));</p>
<p>      }</p>
<p></p>
<p>      ilosc_znakow = max - jQuery(self).val().length;</p>
<p>      ilosc_enterow = jQuery(self).val().split(/\\n/).length - 1;</p>
<p>      ilosc_znakow = ilosc_znakow + ilosc_enterow - ilosc_enterow*100;</p>
<p>      jQuery('#komentarz-tresc-limit > b').html(''+(ilosc_znakow)+'');</p>
<p>    }</p>
<p>  </p>
<p>    przelicz_znaki();</p>
<p>    jQuery('#komentarz-tresc').keyup(function() {</p>
<p>      przelicz_znaki();</p>
<p>    });</p>
<p>  });</p>
