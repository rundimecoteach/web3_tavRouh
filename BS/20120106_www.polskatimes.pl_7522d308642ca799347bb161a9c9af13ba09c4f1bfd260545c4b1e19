<p>    (function () {       </p>
<p>        function wstaw_widgety() {</p>
<p>            szerokosc_okna = jQuery(window).width();</p>
<p>            var czy_jest_zewnetrzny = jQuery("#widget_zewnetrzny").html();</p>
<p>            var czy_jest_lodzki = jQuery("#widget-lodzki").html();</p>
<p>            </p>
<p>            if (czy_jest_zewnetrzny == "" || czy_jest_lodzki == "") {</p>
<p>                if (szerokosc_okna >= 480) {</p>
<p>                    var div_widget_lodzki = document.getElementById("widget-lodzki");</p>
<p>                    var div_widget_zewnetrzny = document.getElementById("widget_zewnetrzny");</p>
<p>                    if (div_widget_lodzki == null && div_widget_zewnetrzny != null) {</p>
<p>                        jQuery('#widget_zewnetrzny').append('<iframe src="http://ogloszenia.gratka.pl/widget_zewn.html?id_widget=widget_19&id_gazeta=19&referer=gloswielkopolski.pl" scrolling="no" allowTransparency="true" frameborder=0></iframe>');</p>
<p>                        return true;</p>
<p>                    }</p>
<p>                    return false;</p>
