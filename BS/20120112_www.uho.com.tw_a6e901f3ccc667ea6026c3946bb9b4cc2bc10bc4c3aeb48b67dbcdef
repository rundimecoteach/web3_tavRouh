<p>for (var i=0;i<keywd.length;i++)</p>
<p>{</p>
<p>	 rekeywd = "<a href='/searchresult.asp?mod=ysm&q="+ escape(keywd[i]) +"' class='ysmkw' title='搜尋關於 ["+ keywd[i] + "]' target='ysm'>" + keywd[i] + "</a>";</p>
<p>	 tmp_cont = replaceSubstring(tmp_cont,keywd[i],rekeywd,10);</p>
<p> }</p>
<p>document.getElementById("ysm").innerHTML = tmp_cont;</p>
<p>//--></p>
