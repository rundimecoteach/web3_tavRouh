<p>          				function getElementsByClassName(cn){</p>
<p>            				var arr = new Array();</p>
<p>            				var obj_art2=document.getElementById("artykul");</p>
<p>            				if (obj_art2==null){</p>
<p>          				obj_art2=document.getElementById("art2");</p>
<p>          				}</p>
<p>            				var els = obj_art2.getElementsByTagName("*");</p>
<p>            				var exp= new RegExp("^(.* )?"+cn+"( .*)?$", "g");</p>
<p>            				for (var i = 0; i < els.length; i++ ){</p>
<p>             				if (exp.test(els[i].className)){</p>
<p>               			 arr.push(els[i]);</p>
<p>              			}</p>
<p>            				}</p>
<p>            				return arr;</p>
<p>          				}</p>
<p>          				var obj_art=document.getElementById("artykul");</p>
<p>          				if (obj_art==null){</p>
<p></p>
<p>          				obj_art=document.getElementById("art2");</p>
<p>          				}</p>
<p>          				var obj = null;</p>
<p>          				if(obj_art){</p>
<p>          				 	var br_art = obj_art.getElementsByTagName('br');</p>
<p>          				 	var txt_srodtytul_art = getElementsByClassName('txt_srodtytul');</p>
<p>          					var txt_pytanie_art   = getElementsByClassName('txt_pytanie');</p>
<p>          				 	var ban_div = document.createElement('div');</p>
<p></p>
<p>          				 	var ban = document.getElementById("banP62");</p>
<p></p>
<p>          				 	if(document.getElementById("banP62").innerHTML.length <= 51 || noBan.indexOf("true")!=-1){</p>
<p>          				 		is_ban = false;</p>
<p>          				 	}else{</p>
<p>          				 		is_ban = true;</p>
<p>          				 	}</p>
<p></p>
<p>          				 	ban_div.appendChild(ban);</p>
<p>          				 	if(txt_srodtytul_art.length > 3){</p>
<p>          						obj =txt_srodtytul_art[2];</p>
<p>          				 		obj.parentNode.insertBefore( ban_div, obj );</p>
<p>          					}else</p>
<p>          					if(txt_pytanie_art.length >3){</p>
<p>          				 		obj =txt_pytanie_art[2];</p>
<p>          				 		obj.parentNode.insertBefore( ban_div, obj );</p>
<p>          					}else</p>
<p>          				 	if(br_art.length>4){</p>
<p>          						obj =br_art[3];</p>
<p></p>
<p>          					 		if(obj.previousSibling.previousSibling.className == 'txt_srodtytul' )obj =br_art[5];</p>
<p>          					 		if(obj.previousSibling.previousSibling.className == 'txt_pytanie' )obj =br_art[5];</p>
<p></p>
<p>          					 		obj.parentNode.insertBefore( ban_div, obj.nextSibling );</p>
<p></p>
<p>          				 	}else{</p>
<p>          				 		obj_art.appendChild(ban);</p>
<p></p>
<p>          				 	}</p>
<p>          				}</p>
