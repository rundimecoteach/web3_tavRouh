<p>        function getCookie(name) {</p>
<p>            var start = document.cookie.indexOf(name+"=");</p>
<p>            var len = start+name.length+1;</p>
<p>            if ((!start) && (name != document.cookie.substring(0,name.length))) return null;</p>
<p>            if (start == -1) return null;</p>
<p>            var end = document.cookie.indexOf(";",len);</p>
<p>            if (end == -1) end = document.cookie.length;</p>
<p>            return unescape(document.cookie.substring(len,end));</p>
<p>        }</p>
