<p>    playStoryVideo = function(id,videoId,repos){</p>
<p>    	storyPlayer.pos = id;</p>
<p>    	if (id == 'media' && videoAutoStart == 'true') {</p>
<p>    		kdp.setKDPAttribute("omniture", "videoViewEvent", "event1,event16");</p>
<p>    	} else {</p>
<p>    		kdp.setKDPAttribute("omniture", "videoViewEvent", "event1");</p>
<p>    	}</p>
<p>		pc.fwUpdateKeyValues("show",window.playlist["adContext-"+videoId]);</p>
<p>		currentVideoId = videoId;</p>
<p>		//playDelay = (repos)?500:6000;</p>
<p>		//setTimeout("sendPlaylistToPlayer()",playDelay);</p>
<p>		//kdp.sendNotification('doStop');</p>
<p>		sendPlaylistToPlayer();</p>
<p>		if(repos){</p>
<p>			leftRailVideoOpen = (id == 'media')?false:true;</p>
<p>			if(id == 'media'){</p>
<p>				leftRailVideoOpen = false;</p>
<p>				$('#caption_video').html(videoTitle);</p>
<p>			}else{</p>
<p>				$('#caption_video').html(videoPhotoCaption);</p>
<p>			}</p>
<p>			kdp.sendNotification("doStop");</p>
<p>			if(currLeftRailId){</p>
<p>				leftRail.closeMedia(currLeftRailId);</p>
<p>			}</p>
<p>			leftRail.openMedia(id,currentVideoId);</p>
<p></p>
<p>			leftRailContainerObj = document.getElementById(id);</p>
<p>			positionPlayer(leftRailContainerObj);</p>
<p>        	$('#'+id+' > .rel_active > .active_image_closer').css("visibility","visible");</p>
<p>			mainMedia.sponsoredByAdPosition();</p>
<p>			currLeftRailId = id;</p>
<p>		}</p>
<p></p>
<p>		if (leftRailVideoOpen == false) {</p>
<p>			$('#caption_video').html(videoTitle);</p>
<p>		}</p>
<p></p>
<p>    }</p>
<p></p>
<p></p>
<p>    closeLeftRailVideo = function(id){</p>
<p>		leftRail.closeMedia(id);</p>
<p>		leftRailVideoOpen = false;</p>
<p>		videoTitle = titleList[0];</p>
<p>		kdp.sendNotification("doStop");</p>
<p>		$('#mediaplayerContainer').css('top', '-1000px');</p>
<p>		resetAd();</p>
<p>    }</p>
<p></p>
<p>	playNextVideo = function(){</p>
<p>	    playlistStr = playlistIds;</p>
<p>	    if(playlistStr != ''){</p>
<p>	        if(playlistStr.indexOf('~') != -1){</p>
<p>	            playlistArr = playlistStr.split('~');</p>
<p>	            for(i=0;i<playlistArr.length;i++){</p>
<p>	                if(playlistArr[i] == currentVideoId){</p>
<p>	                    if((i+1) < playlistArr.length){</p>
<p>                            nextVideoId = playlistArr[i+1];</p>
<p>                            videoTitle = titleList[i+1];</p>
<p>	                    }else{</p>
<p>	                        nextVideoId = playlistArr[0];</p>
<p>	                        videoTitle = titleList[0];</p>
<p>	                    }</p>
<p>	                    if (leftRailVideoOpen) {</p>
<p>	                    	 playStoryVideo('rel_'+(i+1),nextVideoId,false);</p>
<p>	                    } else {</p>
<p>	                    	 playStoryVideo('media',nextVideoId,false);</p>
<p>	                    }</p>
<p>	                   </p>
<p>	                    break;</p>
<p>	                }else{</p>
<p>	                    //close the player</p>
<p>	                }</p>
<p>	            }</p>
<p>	        }</p>
<p>	    }</p>
<p>	}</p>
