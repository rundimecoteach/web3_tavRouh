<p>		if(carouselPane1.content){</p>
<p>			carouselPane1.content.style.left="962px";</p>
<p>		}	</p>
<p>		if(carouselPane1.content && carouselPane1.content.parentNode != carouselPane1.viewport){</p>
<p>			carouselPane1.viewport.appendChild(carouselPane1.content);</p>
<p>	};</p>
<p>};</p>
<p>	carLeftSlide.handler = function(val){</p>
<p>		val = Math.round(val);</p>
<p>		carouselPane1.content.style.left = val+"px";</p>
<p>		return val;</p>
<p>	};</p>
<p>	carLeftSlide.callback = function(){</p>
<p>		carouselPane1.content.style.left = "0px";</p>
<p>	}</p>
<p>	carouselLeftAnim1.addSequence(0,carLeftSlide);</p>
<p></p>
<p>	var carouselRightAnim1 = new Animation();</p>
<p></p>
<p>	//old moving right</p>
<p>	var carRightOldSlide = new Sequence();</p>
<p>	carRightOldSlide.start = 0;</p>
<p>carRightOldSlide.end = 962;</p>
<p>	carRightOldSlide.duration = 1000;</p>
<p>	carRightOldSlide.min=0.5;</p>
<p>	carRightOldSlide.ease = Easing['easeBothStrong'];</p>
<p>	carRightOldSlide.handler = function(val){</p>
<p>		val = Math.round(val);</p>
<p>		carouselPane1.oldContent.style.left = val+"px";</p>
<p>		return val;</p>
<p>	};</p>
<p>	carRightOldSlide.callback = function(){</p>
<p>		carouselPane1.oldContent.style.left = "962px";</p>
<p>	};</p>
<p>	carouselRightAnim1.addSequence(0,carRightOldSlide);</p>
<p></p>
<p>	//new moving right</p>
<p>	var carRightSlide = new Sequence();</p>
<p>carRightSlide.start = -962;</p>
<p>	carRightSlide.end = 0;</p>
<p>	carRightSlide.duration = 1000;</p>
<p>	carRightSlide.min=0.5;</p>
<p>	carRightSlide.ease = Easing['easeBothStrong'];</p>
<p>	carRightSlide.init = function(){</p>
<p>		if(carouselPane1.content){</p>
<p>			carouselPane1.content.style.left="-962px";</p>
<p>		}	</p>
<p>		if(carouselPane1.content && carouselPane1.content.parentNode != carouselPane1.viewport){</p>
<p>			carouselPane1.viewport.appendChild(carouselPane1.content);</p>
<p>		};</p>
<p>	};</p>
<p>	carRightSlide.handler = function(val){</p>
<p>		val = Math.round(val);</p>
<p>		carouselPane1.content.style.left = val+"px";</p>
<p>		return val;</p>
<p>	};</p>
<p>	carRightSlide.callback = function(){</p>
<p>		carouselPane1.content.style.left = "0px";</p>
<p>	}</p>
<p>	carouselRightAnim1.addSequence(0,carRightSlide);</p>
<p></p>
<p>	carouselManager.pane=carouselPane1;</p>
<p>	carouselList1.gotoItem('car1-1');</p>
