<p>				getEntryIdFromUrl : function() {</p>
<p>					if(location.hash.indexOf(kdp_embed_default.url_param_name) != -1) {</p>
<p>						// get the entry id from the url document fragment (aka hash):</p>
<p>						return location.hash.split("#")[1].substring((kdp_embed_default.url_param_name.length+1));</p>
<p>					}</p>
<p>					else if(location.search.indexOf(kdp_embed_default.url_param_name) != -1) {</p>
<p>						// get the entry id from the url parameters (aka querystring):</p>
<p>						return location.search.split("?")[1].substring((kdp_embed_default.url_param_name.length+1));</p>
<p>					}</p>
<p>					else {</p>
