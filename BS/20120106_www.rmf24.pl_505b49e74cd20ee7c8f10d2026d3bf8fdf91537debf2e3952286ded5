<p>jQuery(document).ready(function(){</p>
<p>submitMenyTreadsPrevent = function(form){</p>
<p></p>
<p>var form=jQuery(form);</p>
<p>var ret = true;</p>
<p></p>
<p>form.find('li.text').removeClass('invalid').find('.invalidInfo').text('');</p>
<p>form.find('li.title').removeClass('invalid').find('.invalidInfo').text('');</p>
<p>form.find('li.author').removeClass('invalid').find('.invalidInfo').text('');</p>
<p></p>
<p>if(!form.find("#threadText").val()) {</p>
<p>form.find('li.text').addClass('invalid').find('.invalidInfo').text('Wpisz treść');</p>
<p>ret=false;</p>
<p>} else if(form.find("#threadText").val().length > 3000) {</p>
<p>form.find('li.text').addClass('invalid').find('.invalidInfo').text('Treść przekracza dozwoloną długość');</p>
<p>ret=false;</p>
<p>}</p>
<p>if(form.find("#threadTitle").is('input') && !form.find("#threadTitle").val()) {</p>
<p>form.find('li.title').addClass('invalid').find('.invalidInfo').text('Wpisz tytuł');</p>
<p>ret=false;</p>
<p>} else if( form.find("#threadTitle").is('input') && form.find("#threadTitle").val() > 150 ) {</p>
<p>form.find('li.title').addClass('invalid').find('.invalidInfo').text('Tytuł przekracza dozwoloną długość');</p>
<p>ret=false;</p>
<p>}</p>
<p>if(!form.find("#threadAuthor").val()){</p>
<p>form.find('li.author').addClass('invalid').find('.invalidInfo').text('Wpisz swój podpis');</p>
<p>ret=false;</p>
<p>} else if(form.find("#threadAuthor").val().length > 255){</p>
<p>form.find('li.author').addClass('invalid').find('.invalidInfo').text('Podpis przekracza dozwoloną długość');</p>
<p>ret=false;</p>
<p>}</p>
<p></p>
<p>return ret;</p>
<p>}</p>
<p></p>
<p>jQuery('#createThreadForm form').submit(function(){</p>
<p>return submitMenyTreadsPrevent(this);</p>
<p>});</p>
<p></p>
