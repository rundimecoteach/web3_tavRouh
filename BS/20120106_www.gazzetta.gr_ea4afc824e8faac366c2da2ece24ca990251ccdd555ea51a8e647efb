<p> </p>
<p> </p>
<p>function google_ad_request_done(google_ads)</p>
<p>{</p>
<p> </p>
<p>    var s = '';</p>
<p>    var i;</p>
<p> </p>
<p>    /*</p>
<p>    * Verify that there are actually ads to display.</p>
<p>    */</p>
<p>    if (google_ads.length == 0) {</p>
<p>    return;</p>
<p>    }</p>
<p> </p>
<p>    s+='<table align="left" width="661px" bgcolor="#ffffff" style="border:0px solid #D3D3D3;7;width:100%;text-align:left; " cellspacing=0;  cellpadding="2px"><tr><td>';</p>
<p> </p>
<p>    /*</p>
<p>    * If an image or flash ad is returned, display that ad.</p>
<p>    * Otherwise, build a string containing all of the ads and</p>
<p>    * then use a document.write() command to print that string.</p>
<p>    */</p>
<p>    if (google_ads[0].type == "flash")</p>
<p>    {</p>
<p>    }</p>
<p>    else if (google_ads[0].type == "image")</p>
<p>    {</p>
<p>    }</p>
<p>     else if (google_ads[0].type == "html")</p>
<p>     {</p>
<p>     }</p>
<p>     else</p>
<p>    {</p>
<p>        if (google_ads.length == 1) {</p>
<p>        /*</p>
<p>        * Partners should adjust text sizes</p>
<p>        * so ads occupy the majority of ad space.</p>
<p>        */</p>
<p>        /*</p>
<p>        s += '<a target="_blank" href=\"' +</p>
<p>        google_info.feedback_url + '\" style="color:#000;">Διαφημίσεις Google</a><a style="text-decoration:none" href="' +</p>
<p>        google_ads[0].url + '" onmouseout="window.status=\'\'" onmouseover="window.status=\'go to ' +</p>
<p>        google_ads[0].visible_url + '\';return true"> <span style="text-decoration:none;font-size:12pt"> <b>' +</p>
<p>        google_ads[0].line1 + '</b><br/></span></a> <span style="color:#000;font-size:12pt">' +</p>
<p>        google_ads[0].line2 + '&nbsp;' +</p>
<p>        google_ads[0].line3 + '<br/></span> <a style="color:#000;font-size:12pt;text-decoration:none" <a target="_blank" href="' +</p>
<p>        google_ads[0].url + '" onmouseout="window.status=\'\'" onmouseover="window.status=\'go to ' +</p>
<p>        google_ads[0].visible_url + '\';return true"><span>' +</p>
<p>        google_ads[0].visible_url + '</span></a>';</p>
<p>        */</p>
<p>        }</p>
<p>        else if (google_ads.length > 1)</p>
<p>        {</p>
<p> </p>
<p> </p>
<p>              /*</p>
<p>              * For text ads, append each ad to the string.</p>
<p>              */</p>
<p> </p>
<p>            for(i = 0; i < google_ads.length; ++i)</p>
<p>            {</p>
<p></p>
<p>                if (i==google_ads.length-1){</p>
<p>                  </p>
<p>                s+= "<div class='hover1'  style='border-bottom:1px solid #ccc;;padding-top:10px;padding-bottom:8px; padding-left:5px; padding-right:5px;'>";</p>
<p>                s+='<a   target="_blank" style="text-decoration:none;font-size:11pt" href="' +</p>
<p>                google_ads[i].url + '" onmouseout="window.status=\'\'" onmouseover="window.status=\'go to ' +</p>
<p>                google_ads[i].visible_url + '\';return true"> <div style="text-decoration:none;font:Georgia, Times New Roman, Times, serif;color:#004A80"> <b>' +</p>
<p>                google_ads[i].line1 + '</b></div></a> <a style="color:#004A80;font:Georgia, Times New Roman, Times, serif;text-decoration:none;font-size:11pt" <a target="_blank" href="' +</p>
<p>                google_ads[i].url + '" onmouseout="window.status=\'\'" onmouseover="window.status=\'go to ' +</p>
<p>                google_ads[i].visible_url + '\';return true"><div style="font-size:10pt;color:#115688;">' +</p>
<p>                google_ads[i].visible_url + '</div></a><div style="color:#404040;font-size:10pt;font:Georgia, Times New Roman, Times, serif;">' +</p>
<p>                google_ads[i].line2 + '&nbsp;' +</p>
<p>                google_ads[i].line3 + '</div></div>';</p>
<p></p>
<p></p>
<p>}else{</p>
<p></p>
<p></p>
<p>                s+= "<div class='hover1' style='border-bottom:1px solid #ccc;padding:10px 0; padding-left:5px !important; padding-right:5px; font:Georgia, Times New Roman, Times, serif;'>";</p>
<p>                s+='<a target="_blank" style="text-decoration:none;font-size:11pt; font:Georgia, Times New Roman, Times, serif;" href="' +</p>
<p>                google_ads[i].url + '" onmouseout="window.status=\'\'" onmouseover="window.status=\'go to ' +</p>
<p>                google_ads[i].visible_url + '\';return true"> <div style="text-decoration:none;color:#004A80;font:Georgia, Times New Roman, Times, serif;"> <b>' +</p>
<p>                google_ads[i].line1 + '</b></div></a> <a style="color:#004A80;text-decoration:none;font-size:11pt" <a target="_blank" href="' +</p>
<p>                google_ads[i].url + '" onmouseout="window.status=\'\'" onmouseover="window.status=\'go to ' +</p>
<p>                google_ads[i].visible_url + '\';return true"><div style="font-size:10pt;color:#115688;">' +</p>
<p>                google_ads[i].visible_url + '</div></a><div style="color:#404040;font-size:10pt;font:Georgia, Times New Roman, Times, serif;">' +</p>
<p>                google_ads[i].line2 + '&nbsp;' +</p>
<p>                google_ads[i].line3 + '</div></div>';</p>
<p>}</p>
<p>            }</p>
<p>        }</p>
<p> </p>
<p></p>
<p>               s += '<div  style="background-color:#ffffff; padding-left:5px ; text-align:right; padding-right:5px;"><a href=\"' + google_info.feedback_url + '\" style="color:#999999;font-size:1.1em; font:normal normal normal 13px/1.5em Georgia, Times New Roman, Times, serif; text-decoration:none">Διαφημίσεις Google</a></div>'</p>
<p></p>
<p>       s+='</td></tr></table>';</p>
<p>  </p>
<p> </p>
<p>    document.write(s);</p>
<p>    return;</p>
<p>  }</p>
<p>}</p>
<p> </p>
