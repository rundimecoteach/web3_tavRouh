<p>    function SponsorLink() {</p>
<p>        window.open('/googleAdsCode/sponsorLinks.htm', 'winSelectCategory', 'status=no,toolbars=no,scrollbars=no,location=no,menubar=no,resizable=no,width=400,height=440');</p>
<p>    }</p>
<p></p>
<p>    function google_ad_request_done(google_ads) {</p>
<p></p>
<p>        var s;</p>
<p>        var i;</p>
<p></p>
<p>        if (google_ads.length == 0) {</p>
<p></p>
<p>            return;</p>
<p>        }</p>
<p></p>
<p>        if (google_ads[0].type == "text") {</p>
<p></p>
<p>            if (google_ads.length > 0) {</p>
<p>                s = '<div class="promoTitle"><a href="#" onclick="SponsorLink();return false;" target="_blank">ΠΡΟΒΕΒΛΗΜΕΝΕΣ ΚΑΤΑΧΩΡΗΣΕΙΣ</a></div>'; for (i = 0; i < google_ads.length; ++i) {</p>
<p>                    s += '<div class="manyAds">' +</p>
<p>				    '<div class="addTitle">' +</p>
<p>					    '<a target="_blank" href="' + google_ads[i].url + '"onmouseout="window.status=\'\';return true"' +</p>
<p>					    'onmouseover="window.status=\'go to ' + google_ads[i].visible_url + '\';return true" style="text-decoration:none">' +</p>
<p>					    google_ads[i].line1 +</p>
<p>					    '</a>' +</p>
<p>				    '</div>' +</p>
<p>				    '<div class="addText">' +</p>
<p>					    '<span>' + google_ads[i].line2 + '&nbsp;' + google_ads[i].line3 + '</span>' +</p>
<p>				    '</div>' +</p>
<p>				    '<div class="addUrl">' +</p>
<p>					    '<a target="_blank" href="' + google_ads[i].url + '"onmouseout="window.status=\'\';return true"' +</p>
<p>					    'onmouseover="window.status=\'go to ' + google_ads[i].visible_url + '\';return true" style="text-decoration:none">' +</p>
<p>					    google_ads[i].visible_url +</p>
<p>					    '</a>' +</p>
<p>				    '</div>' +</p>
<p>			    '</div>';</p>
<p>                }</p>
<p></p>
<p>                var gAdsDiv = document.getElementById('gAdsDiv');</p>
<p>                if (gAdsDiv == null)</p>
<p>                    return;</p>
<p>                gAdsDiv.innerHTML = s;</p>
<p>            }</p>
<p>        }</p>
<p></p>
<p></p>
<p>        return;</p>
<p>    }</p>
