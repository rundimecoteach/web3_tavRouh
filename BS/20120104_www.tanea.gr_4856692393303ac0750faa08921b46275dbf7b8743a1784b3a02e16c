<p>    $(document).ready(function () {</p>
<p>        $("select[name='moreUrlNavigateSelect']").change(function () {</p>
<p>            $("select option:selected").each(function () {</p>
<p>                if ($(this).attr('value') != 'null') {</p>
<p>                    window.open($(this).attr('value'), "newwindow");</p>
<p></p>
<p>                }</p>
<p>            });</p>
<p>        });</p>
<p></p>
<p></p>
<p>        ///Search/?area=0&author=&cid=-1&so=1&sa=0&pt=5&words=</p>
<p></p>
<p>        $("input[id=searchInputFooter]").keypress(function (e) {</p>
<p>            var code = (e.keyCode ? e.keyCode : e.which);</p>
<p>            if (code == 13 || code == 10) {</p>
<p>                searchFromFooter();</p>
<p>                return false;</p>
<p>            }</p>
<p>            return true;</p>
<p>        });</p>
<p></p>
<p>        $("img[id=searchButtonImage]").click(function () {</p>
<p>            searchFromFooter();</p>
<p>        });</p>
<p></p>
<p>    });</p>
<p></p>
<p>    function searchFromFooter() {</p>
<p>        var words = "";</p>
<p>        $("input[id=searchInputFooter]").each(function () {</p>
<p>            words += $(this).val();</p>
<p>        });</p>
<p>        window.location.href = "/Search/?area=0&author=&cid=-1&so=1&sa=0&pt=3&words=" + decodeURI(words);</p>
<p>    }</p>
