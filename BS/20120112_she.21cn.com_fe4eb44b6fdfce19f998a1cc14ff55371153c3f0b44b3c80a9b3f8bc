<p>//RenderPicList</p>
<p>function RenderPicList(dataId){</p>
<p>    var count = 15;</p>
<p>    var span = 5;</p>
<p>    var url = 'http://picture.21cn.com/script/' + dataId + ',0,' + count + ',2.js?func=jQuery&jsoncallback=?';</p>
<p>    var tmpHtml = '';</p>
<p>    $.getJSON(url, function(data){</p>
<p>        var ds = data.resultList.list;</p>
<p>        tmpHtml = '<div>';</p>
<p>        </p>
<p>        $.each(ds, function(i){</p>
<p>            if (i !== 0 && i % span === 0) {</p>
<p>                tmpHtml = tmpHtml + '</div><div>';</p>
<p>            }</p>
<p>			var ds_url = 'http://picture.et.21cn.com/slide/'+ds[i].channelId+','+ds[i].folderId+'.shtml'</p>
<p>            tmpHtml = tmpHtml + '<a href="'+ds_url+'" target="_blank" ><img src="' + ds[i].coverUrl + '" alt="" width="100" height="75" /><em>' + ds[i].folderName + '</em></a>';</p>
<p>        });</p>
<p>        tmpHtml = tmpHtml + '</div>';</p>
<p>        $('#picList').html(tmpHtml);</p>
<p>        $(".scrollable2").scrollable({</p>
<p>            circular: true</p>
<p>        }).autoscroll({</p>
<p>            interval: 4000</p>
<p>        }).navigator();</p>
<p>    });    </p>
<p>}</p>
<p>	$(function(){					  </p>
<p>            RenderPicList(106);</p>
<p>			});</p>
