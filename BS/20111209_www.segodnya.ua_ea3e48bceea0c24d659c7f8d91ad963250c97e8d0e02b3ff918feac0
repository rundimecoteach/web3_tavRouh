<p>    $('#register_via_email').click(function(){</p>
<p>        var reg_emailre = /^[\w]{1}[\w\.\-_]*@[\w]{1}[\w\-_\.]*\.[\w]{2,9}$/i;</p>
<p>        if ($('#reg_nick').val() =='')</p>
<p>        {</p>
<p>            show_error('#register_email_error','Вы не указали имя на сайте');</p>
<p>            $('#reg_nick').focus();</p>
<p>            return false;</p>
<p>        }</p>
<p>        if (!$('#reg_email').val().match(reg_emailre))</p>
<p>        {</p>
<p>            show_error('#register_email_error','Введите пожалуйста свой E-mail');</p>
<p>            $('#reg_email').focus();</p>
<p>            return false;</p>
<p>        }</p>
<p>        </p>
<p>        var reg_phone = /^[0-9]{10}$/i;</p>
<p>        if (!$('#reg_phone').val().match(reg_phone))</p>
<p>        {</p>
<p>            show_error('#register_email_error','Укажите корректный номер телефона');</p>
<p>            $('#reg_phone').focus();</p>
<p>            return false;</p>
<p>        }</p>
<p>        </p>
<p>        if ($('#reg_passwd').val() == '')</p>
<p>        {</p>
<p>            show_error('#register_email_error','Введите пожалуйста пароль');</p>
<p>            $('#reg_passwd').focus();</p>
<p>            return false;</p>
<p>        }</p>
<p>        if ($('#reg_code').val()=='')</p>
<p>        {</p>
<p>            show_error('#register_email_error','Введите пожалуйста защитный код');</p>
<p>            $('#reg_code').focus();</p>
<p>            return false;  </p>
<p>        }</p>
<p>        $('#register_form_email').hide();</p>
<p>        $('#loading').show();</p>
<p>        $.getJSON('/auth/email/register.html',$('#register_form_email').serialize(),function(data){</p>
<p>            </p>
<p>            if (data.error !='' && data.error != undefined)</p>
<p>            {</p>
<p>                $('#loading').hide();        </p>
<p>                $('#register_form_email').show();</p>
<p>                show_error('#register_email_error',data.error);</p>
<p>                </p>
<p>                $('#reg_code').val('');</p>
<p>                $('#reg_code_captcha').attr('src','/exec/code.php?'+(new Date()).getTime());</p>
<p>                </p>
<p>            }</p>
<p>            else</p>
<p>            {</p>
<p>                $('#loading').hide();</p>
<p>                //$('#register_form_email_result').show();</p>
<p>                $('#main_header').hide();</p>
<p>                $('#main_body').hide();</p>
<p>                </p>
<p>                //$('#check_email_header').show();</p>
<p>                $('#check_email_body').show();</p>
<p>                </p>
<p>                show_error('#register_email_error',data.msg);</p>
<p>            }</p>
<p>        })</p>
<p>        </p>
<p>        return false;</p>
<p>    })</p>
<p>    </p>
<p>    </p>
<p>    // email login</p>
<p>    $('#login_form_submit').click(function(){</p>
<p>        /*</p>
<p>        var reg_emailre = /^[\w]{1}[\w\.\-_]*@[\w]{1}[\w\-_\.]*\.[\w]{2,9}$/i;</p>
<p>        if (!$('#email').val().match(reg_emailre) && ($('#email').val()!='root'))</p>
<p>        {</p>
<p>            show_error('#login_email_error','Введите пожалуйста свой E-mail');</p>
<p>            $('#email').focus();</p>
<p>            return false;</p>
<p>        }</p>
<p>        */</p>
<p>        if ($('#password').val() == '')</p>
<p>        {</p>
<p>            show_error('#login_email_error','Введите пожалуйста пароль');</p>
<p>            $('#pass_field_fake').focus();</p>
<p>            return false;</p>
<p>        }</p>
<p>        hide_error('#login_email_error');</p>
<p>        alax_preload(this,1);</p>
<p>        $.post('/passport/login.html',{</p>
<p>                'email':$('#email').val(),</p>
<p>                'passwd':$('#password').val(),</p>
<p>                mode:'ajax',</p>
<p>                captcha: $('#f_code').val()</p>
<p>        }, function(data){</p>
<p>            eval(data);</p>
<p>            if (typeof(data) !='' && data.success == true) {</p>
<p>                $('#login_form_submit span').text('Вошли');</p>
<p>                document.location.href = document.location.href+(sunsite.anchorhref()?'#'+sunsite.anchorhref():'');</p>
<p>                document.location.reload();</p>
<p>            } else if (data.error != null) {</p>
<p>                show_error('#login_email_error',data.error);</p>
<p>                var isrc = $('#code_img').attr('src');</p>
<p>                $('#code_img').attr('src',isrc+'?'+(new Date()).getTime())</p>
<p>                if(data.login_attempt >= 2) $('#code_holder').show();</p>
<p>                </p>
<p>            }</p>
<p>            </p>
<p>            alax_preload('#login_form_submit',0);</p>
<p>                        </p>
<p>        }) </p>
<p>        </p>
<p>        return false;</p>
<p>    });</p>
<p>    $('#button_facebook a, #button_twitter a, #button_google a, #button_vkontakte a').click(function(){</p>
<p>        location.href = $(this).attr('href')+"?redirect="+encodeURIComponent(document.location.pathname)+"&anchor="+sunsite.anchorhref();</p>
<p>        $('#soc_net').html('Подключаемся...');</p>
<p>		return false;</p>
<p>    })</p>
<p>    function show_error(selector, text) {</p>
<p>        $(selector).show().text(text);</p>
<p>    }</p>
<p>    function hide_error(selector) {</p>
<p>        $(selector).hide();</p>
<p>    }</p>
<p>    function alax_preload(selector, mode) {</p>
<p>        if (mode == 1) {</p>
<p>            if (!$(selector).prev().hasClass("ajax_preload")) {</p>
<p>                $(selector).before("<div class=\"ajax_preload\">Подождите несколько секунд <img width=\"16\" height=\"16\" alt=\"\" src=\"/user/img/loader.gif\" /></div>");</p>
<p>            }</p>
<p>        } else {</p>
<p>            $(selector).prev(".ajax_preload").remove();</p>
<p>        }</p>
<p>    }</p>
<p>    $('#reminder_show').click(function(){</p>
<p>        $('#email_login').hide();</p>
<p>        $('#email_reminder').show();</p>
<p>        return false;</p>
<p>    })</p>
<p>    $('#decline_email_reminder').click(function(){</p>
<p>        $('#email_login').show();</p>
<p>        $('#email_reminder').hide();</p>
<p>        return false;</p>
<p>    })</p>
<p>    // email reminder</p>
<p>    $('#login_form_reminder').click(function(){</p>
<p>        var reg_emailre = /^[\w]{1}[\w\.\-_]*@[\w]{1}[\w\-_\.]*\.[\w]{2,9}$/i;</p>
<p>        if (!$('#login_reminder').val().match(reg_emailre))</p>
<p>        {</p>
<p>            show_error('#login_email_reminder_error','Введите пожалуйста свой E-mail');    </p>
<p>            $('#login_reminder').focus();</p>
<p>            return false;</p>
<p>        }</p>
<p>        </p>
<p>        hide_error('#login_email_reminder_error');</p>
<p>        alax_preload(this,1);</p>
<p>        $.getJSON('/auth/email/reset.html',{'f[email]':$('#login_reminder').val()},function(data)    {</p>
<p>            //eval(data);</p>
<p>            if (data.msg != null && data.msg != 'undefined' )</p>
<p>            {</p>
<p>                show_error('#login_email_error',data.msg);</p>
<p>                $('#decline_email_reminder').click();</p>
<p>                $('#email').val($('#login_reminder').val());</p>
<p>                $('#password').focus()</p>
<p>            }</p>
<p>            else if (data.error != null)</p>
<p>                show_error('#login_email_reminder_error',data.error);</p>
<p>            alax_preload('#login_form_reminder',0);</p>
<p>        }) </p>
<p>        </p>
<p>        return false;</p>
<p>    })</p>
<p>    </p>
<p>    $('#confirm_sms_btn').click(function(){</p>
<p>        if($('#sms_code').val() == '') {</p>
<p>            show_error('#login_email_error','Введите код из смс');</p>
<p>            $('#sms_code').focus();</p>
<p>            return false;</p>
<p>        }</p>
<p>        $.getJSON('/auth/confirm_sms.html',{ 'phone_num': $('#reg_phone').val(), 'sms_code': $('#sms_code').val() },function(data)    {</p>
<p>            if (data.error != null) {</p>
<p>                show_error('#login_email_reminder_error','Вы ввели неверный код. Код отправлен повторно');</p>
<p>            } else {</p>
<p>                document.location.href = document.location.pathname+(sunsite.anchorhref()?'#'+sunsite.anchorhref():false);</p>
<p>            }</p>
<p>        });</p>
<p>        return false;</p>
<p>    });</p>
<p>    $('#repeat_sms_code').click(function(){</p>
<p>        $.getJSON('/auth/confirm_sms.html',{ 'phone_num': $('#reg_phone').val(), 'repeat': true },function(data)    {</p>
<p>            if (data.error != null) {</p>
<p>                alert(data.error);</p>
<p>            } else {</p>
<p>                alert('Код отправлен повторно');</p>
<p>            }</p>
<p>        });</p>
<p>        return false;</p>
<p>    });</p>
