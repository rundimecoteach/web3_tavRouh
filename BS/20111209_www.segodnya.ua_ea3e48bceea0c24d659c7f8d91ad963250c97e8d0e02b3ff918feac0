<p>    sunsite.addfunc('top_brand_link', function(data){</p>
<p>        </p>
<p>        var d_brand = new Date(sunsite.time);</p>
<p>        var h_brand = d_brand.getHours();</p>
<p>        var m_brand = d_brand.getMinutes();</p>
<p>        </p>
<p>        </p>
<p>        </p>
<p>        if(</p>
<p>            (h_brand == 14 && m_brand >= 40 || h_brand > 14)</p>
<p>            &&</p>
<p>            (h_brand == 16 && m_brand < 40 || h_brand < 16)</p>
<p>        ) {</p>
<p>            $('body').css('background', 'url(/user/img/b2fon.jpg) top center no-repeat');</p>
<p>            $('body').prepend('<a style="display: block; height: 100px;" href="http://www.segodnya.ua/regions/kiev/Bilety-v-obmen-na-novosti-Segodnyayua-obyavlyaet-nebolshoy-konkurs.html"></a>');</p>
<p>        } else {</p>
<p>            $('body').css('background', 'url(/user/img/bg6years.jpg) top center no-repeat');</p>
<p>            $('body').prepend('<a style="display: block; height: 115px;" href="http://video.segodnya.ua/Saytu-Segodnya-ispolnilos-6-let-Uznayte-pro-nashi-uspehi.html"></a>');</p>
<p>        }</p>
<p>        </p>
<p>    });</p>
