<p>		sunsite.addfunc('after_banners', function(data){</p>
<p>                var w = window, d = document;</p>
<p>            w.admixZArr = (w.admixZArr || []);</p>
<p>            w.admixerSmOptions = (w.admixerSmOptions || {});</p>
<p>            w.admixerSmOptions.showAdsOnLoad = true;</p>
<p>            if (!w.admixerSm) {</p>
<p>                var adms = document.createElement('script');</p>
<p>                adms.async = true;</p>
<p>                adms.type = 'text/javascript';</p>
<p>                adms.src = 'http://cdn.admixer.net/scriptlib/asm2.js?v=3';</p>
<p>                var node = d.getElementsByTagName('script')[0];</p>
<p>                node.parentNode.insertBefore(adms, node);</p>
<p>            }</p>
<p>            </p>
<p>        });</p>
<p>        </p>
<p>        sunsite.start();</p>
<p>        </p>
<p>    </p>
