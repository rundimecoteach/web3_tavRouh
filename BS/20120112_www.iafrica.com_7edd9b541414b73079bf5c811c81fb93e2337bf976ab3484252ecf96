<p>    </p>
<p>	nxtblock[1] = 1;</p>
<p>	nxtblock[2] = 4;</p>
<p></p>
<p>    function hide_content ( v_start, v_end ) </p>
<p>    {</p>
<p>        //alert(\'function: hide_content\');</p>
<p>        for ( i = v_start; i < v_end; i ++ ) </p>
<p>		{</p>
<p>           	//alert("#c_userBlock_tag" + i);</p>
<p>			$("#c_userBlock_tag" + i).hide();</p>
<p>        }</p>
<p>    }</p>
<p>    function fade_next( blk ) </p>
<p>    {</p>
<p>    	//alert(\'block: \' + blk);</p>
<p>    </p>
<p>        setuserBlock(vc_arr[nxtblock[blk]],'c_userBlock_tag' + nxtblock[blk], blk);</p>
<p>        //setuserBlock(vc_arr[nxtblock[blk]],\'c_userBlock_tag\' + blk, blk)</p>
<p>        //alert(\'function: fade_next\');</p>
<p>    }</p>
<p></p>
<p>    function setuserBlock (theTarget,theContent,theBlock)</p>
<p>    {</p>
<p>        v_start = (theBlock-1) * 3 + 1;</p>
<p>        v_end = theBlock * 3 + 1;</p>
<p>        clearTimeout(tmout_tab[theBlock]);</p>
<p>        cur_tab = theContent;</p>
<p>        </p>
<p>        nxtblock[theBlock]++;</p>
<p>        if ( nxtblock[theBlock] >= v_end )</p>
<p>            nxtblock[theBlock] = v_start;</p>
<p></p>
<p>        for ( i = v_start; i < v_end; i ++ ) </p>
<p>		{</p>
<p>            for ( b = 1; b < 4; b ++ ) </p>
<p>             {</p>
<p>                </p>
<p>                v_str = "#" + vc_arr[i] + "_hd_" + b;</p>
<p>		    //	alert(v_str);</p>
<p>                $(v_str).css("background", "none").css("color", "#878787");</p>
<p>                //document.getElementById(v_str).style.background =  "#878787";</p>
<p>            }</p>
<p>        }</p>
<p></p>
<p>        for ( b = 1; b < 4; b ++ ) </p>
<p>		{</p>
<p>            v_str = "#" + theTarget + "_hd_" + b;</p>
<p>            </p>
<p>            $(v_str).css("background", vimg_arr[b]).css("color", "#00508e");</p>
<p>            </p>
<p>            //alert(v_str + vimg_arr[b]);</p>
<p>            //document.getElementById(v_str).style.background =  "#00508e";</p>
<p>        }</p>
<p></p>
<p>        var item = $("#c_userBlock_display" + theBlock);</p>
<p>        var v_flt = item.css("filter");</p>
<p>        </p>
<p>        item.fadeOut("slow", function() </p>
<p>        {</p>
<p>            item.html($("#"+theContent).html());</p>
<p>            item.fadeIn("slow", function() </p>
<p>            {</p>
<p>                item.css("filter", v_flt);</p>
<p>                </p>
<p>                tmout_tab[theBlock] = setTimeout("fade_next(" + theBlock + ")", v_timeout_tab);</p>
<p>            });</p>
<p>        });</p>
<p>    }</p>
<p>    </p>
