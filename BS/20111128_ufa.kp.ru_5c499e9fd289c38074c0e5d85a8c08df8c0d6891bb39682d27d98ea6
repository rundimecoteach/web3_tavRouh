<p>    function initZeroClipboard() {</p>
<p>            clip = new ZeroClipboard.Client();</p>
<p>            clip.setHandCursor( true );</p>
<p>            clip.addEventListener('load', my_load);</p>
<p>            clip.addEventListener('mouseOver', my_mouse_over);</p>
<p>            clip.addEventListener('complete', my_complete);</p>
<p>            clip.glue( 'd_clip_button' );</p>
<p>    }</p>
<p></p>
<p>    function my_load(client) {</p>
<p>            $('d_clip_button').innerHTML = "<u>Скопировать код</u>";</p>
<p>    }</p>
<p>    function my_mouse_over(client) {</p>
<p>            clip.setText( $('fe_text').value );</p>
<p>    }</p>
<p></p>
<p>    function my_complete(client, text) {</p>
<p>            $('d_clip_button').innerHTML = "<span class=\"bold_red\">Код скопирован</span>";</p>
<p>    }</p>
