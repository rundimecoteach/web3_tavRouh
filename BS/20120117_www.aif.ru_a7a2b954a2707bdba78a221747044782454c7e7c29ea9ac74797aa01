<p>		//$('#comments_inp').val(cmt + author + " писал(а):\n<i>" + comment + "</i>\n---\n");</p>
<p>        $('#reply_text textarea').val(cmt + author + " писал(а):\n<i>" + comment + "</i>\n---\n");</p>
<p></p>
<p>        $('#reply_text').show();</p>
<p>        document.location.href='#reply_text';</p>
<p>		$('#comments_inp').focus();</p>
<p>    }</p>
<p></p>
<p>	$('#show_comment_button_a, .to-comment-form').click(function(){</p>
<p>		$('#comment_form_block').show(); </p>
<p>		$('#show_comment_button').hide();</p>
<p>	});</p>
<p>    </p>
<p>    $("a.reply").click(function()</p>
<p>	{</p>
<p>		var comment_id = $(this).attr('rel').replace(/[^0-9]+/, '');</p>
<p></p>
<p>		var comment = $('blockquote', '#comment'+ comment_id).text();</p>
<p>        text_block = $('blockquote', '#comment'+ comment_id).hide();</p>
<p></p>
<p></p>
<p>        if(!$('#comment'+ comment_id + ' textarea')[0]) {</p>
<p>            ta = $('<textarea readonly="readonly" style="width: 457px; overflow:auto; border:1px solid #B4B4B4;background:transparent;">');</p>
<p>            ta.height(text_block.height()+40);</p>
<p>            buttons = $('<div class="reply_buttons"><a href="javascript:void(0);" rel="#comment'+comment_id+'" class="reply_full">Цитировать полностью</a>  <a href="javascript:void(0);" rel="#comment'+comment_id+'" class="reply_selected">Цитировать выделенное</a> <a href="javascript:void(0);" rel="#comment'+comment_id+'" class="reply_cancel">Отмена</a></div>');</p>
<p>            ta.val($.trim(comment));</p>
<p></p>
<p></p>
<p>            $('#comment'+ comment_id).append(buttons);</p>
<p>            $('#comment'+ comment_id).append('<div id="reply_info_text">Для цитирования части комментария выделите нужную область текста и нажмите "Цитировать выделенное"</div>');</p>
<p>            $('#comment'+ comment_id).append(ta);</p>
<p></p>
<p></p>
<p>            $("a.reply_full").click(function()</p>
<p>            {</p>
<p>                var comment_id = $(this).attr('rel').replace(/[^0-9]+/, '');</p>
<p></p>
<p>                var comment = $('blockquote', '#comment'+ comment_id).text();</p>
<p></p>
<p>                $('#comment_form_block').show();</p>
<p>                $('#show_comment_button').hide();</p>
<p>                reply(comment,comment_id);</p>
<p>            });</p>
<p></p>
<p>            $("a.reply_selected").click(function() {</p>
<p>                var comment_id = $(this).attr('rel').replace(/[^0-9]+/, '');</p>
<p>                text = selectedText($('#comment'+ comment_id + ' textarea'));</p>
<p>                if(text.length > 0) {</p>
<p>                    $('#comment_form_block').show();</p>
<p>                    $('#show_comment_button').hide();</p>
<p>                    reply(text,comment_id);</p>
<p>                } else {</p>
<p>                    aif.showError($(this),'Выделите текст для цитирования');</p>
<p>                }</p>
<p>            });</p>
<p>            $("a.reply_cancel").click(function()</p>
<p>            {</p>
<p>                var comment_id = $(this).attr('rel').replace(/[^0-9]+/, '');</p>
<p></p>
<p>                $('div.reply_buttons', '#comment'+ comment_id).remove();</p>
<p>                $('textarea', '#comment'+ comment_id).remove();</p>
<p>                $('blockquote', '#comment'+ comment_id).show();</p>
<p>                $('#reply_info_text').remove();</p>
<p>                $('#reply_text').hide();</p>
<p>            });</p>
<p>        }</p>
<p>    });</p>
<p></p>
<p>    $("a.open").click(function() {</p>
<p>        var li = $(this).parents("li.comment_closed");</p>
<p>        li.attr("class","comment_opened");</p>
<p>        cookie_id = cookie_prefix + li.find("div.comment-meta").attr("id");</p>
<p>        $.cookie(cookie_id,'opened');</p>
<p>    });</p>
<p>    $("a.close").click(function() {</p>
<p>        li = $(this).parents("li.comment_opened")</p>
<p>        li.attr("class","comment_closed");</p>
<p>        cookie_id = cookie_prefix + li.find("div.comment-meta").attr("id");</p>
<p>        $.cookie(cookie_id,'closed');</p>
<p>    });</p>
<p></p>
<p>    $("a.complain").click(function() {</p>
<p>        li = $(this).parents("li.comment_opened");</p>
<p>        id = li.find("div.comment-meta").attr("id").replace('meta','');</p>
<p>        var _self = this;</p>
<p></p>
<p>        if($.cookie('comment'+id) != null) {</p>
<p>            aif.showError($(_self),'Вы уже жаловались на данный комментарий!');</p>
<p>            return;</p>
<p>        }</p>
<p></p>
<p></p>
<p>        $.ajax({</p>
<p>            url: '/complaintadd/comment_id/' + id,</p>
<p>            data: {},</p>
<p>            type: 'POST',</p>
<p>            cache: false,</p>
<p>            success: function(response) {</p>
<p>                if(typeof response == "string" && response.charAt(0) == "{") response = jQuery.parseJSON(response);</p>
<p>                if(response.result) {</p>
<p>                    /*if(response.content) {</p>
<p>                        complaint_count = $(_self).parents("div.comment-meta").find("span.complaint_count");</p>
<p>                        complaint_count.html(response.content);</p>
<p>                    }*/</p>
<p>                    aif.showError($(_self),response.result);</p>
<p>                }</p>
<p>            },</p>
<p>            error: function() {</p>
<p>                aif.showError($(_self),'Ошибка сервера');</p>
<p>            }</p>
<p>        });</p>
<p></p>
<p>    });</p>
<p></p>
<p>    $("a.full").click(function() {</p>
<p>        //li = $(this).parents("li.comment_opened");</p>
<p>        //li.find("span.comment-etc").hide();</p>
<p>        //li.find("span.comment-part2").show();</p>
<p>        $(this).prev().hide();</p>
<p>        $(this).next().show();</p>
<p>        $(this).hide();</p>
<p>    });</p>
<p></p>
