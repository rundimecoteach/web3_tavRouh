<p> function selectedText(textArea) {</p>
<p></p>
<p> }</p>
<p></p>
<p>function selectedText(textArea) {</p>
<p></p>
<p>//IE</p>
<p>    if(document.selection)</p>
<p>    {</p>
<p>        var s = document.selection.createRange();</p>
<p>        return s.text;</p>
<p>    }</p>
<p>    if (window.opera && document.getSelection()) { return document.getSelection();}</p>
<p>    // FireFox</p>
<p>    else if (typeof(textArea[0].selectionStart) == "number")</p>
<p>    {</p>
<p>        if (textArea[0].selectionStart != textArea[0].selectionEnd)</p>
<p>        {</p>
<p>            var len = textArea.val().length;</p>
<p>            var start = textArea[0].selectionStart;</p>
<p>            var end = textArea[0].selectionEnd;</p>
<p>            return textArea.val().substring(start, end);</p>
<p>        }</p>
<p>        </p>
<p>        return '';</p>
<p>        </p>
<p>    }</p>
<p>}</p>
