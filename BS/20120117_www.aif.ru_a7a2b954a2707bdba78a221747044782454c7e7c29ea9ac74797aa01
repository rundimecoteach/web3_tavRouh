<p>	$('#ajaxform').submit(function(e){</p>
<p>		e.preventDefault();    </p>
<p>        //comment = $('<div>').html($('#comments_inp').val()).text();</p>
<p>        comment = $('#comments_inp').val();</p>
<p>        $('#comments_inp').val($('#reply_text textarea').val() + comment);</p>
<p>		var action = $(this).attr('action');</p>
<p>		var data = $(this).serialize();</p>
<p>        $('#comments_inp').val(comment);</p>
<p>		$.post(action, data, function(r){</p>
<p>			if('ok'==r.result) {</p>
<p>                            </p>
<p>                            				</p>
<p>                                $('#article-comments').html(r.content);</p>
<p>				$('#comments_form').load('http://www.aif.ru/aforism/?'+Math.random());</p>
<p>                            init_comment_actions();</p>
<p>                                                         </p>
<p>			} else {</p>
<p>				alert(r.result);</p>
<p>			}</p>
<p>		}, 'json');</p>
<p>               </p>
<p>	});</p>
<p></p>
<p>    //инициализируем действия для кнопок комментариев</p>
<p>    init_comment_actions();</p>
<p></p>
<p>});</p>
