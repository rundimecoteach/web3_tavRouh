<p>(function()</p>
<p>{</p>
<p>    var pbname = $.cookie('pbname');</p>
<p>    if( pbname )</p>
<p>    {</p>
<p>        var name = '';</p>
<p>        if( pbname.search(/^[-0-9]+$/)>=0 )</p>
<p>        {</p>
<p>            var aname = pbname.split("-");</p>
<p>            for(var i in aname)</p>
<p>                name += String.fromCharCode(aname[i]);</p>
<p>        }</p>
<p>        else</p>
<p>            name = pbname.replace(/\"/g,'');</p>
<p>        if( name )</p>
<p>        {</p>
<p>            $('#profile_text').hide();</p>
<p>            $('#username').html(name);</p>
<p>        }</p>
<p>    }</p>
<p>})();</p>
