<p>	YUI(_CBC_LOADER).use('cbc_core', 'node', 'gigya', function(Y, result){</p>
<p>		if (result.success){</p>
<p>			var conf = {</p>
<p>				APIKey:CBC.SETTINGS.getVal('gigyaApiKey')</p>
<p>			};</p>
<p>				</p>
<p>			var ua = new gigya.services.socialize.UserAction();</p>
<p>			ua.setUserMessage("Share this CBC content");</p>
<p>			ua.setLinkBack(document.location.href);</p>
<p>			</p>
<p>			ua.setTitle("Norovirus outbreak suspected at B.C. student conference");</p>
<p>			</p>
<p>			var cid = '';</p>
<p>			</p>
<p>				cid = "Canada";</p>
<p>			</p>
<p>			var params1 ={</p>
<p>				userAction:ua,</p>
<p>				shareButtons:'facebook, twitter, google-plusone, share, email',</p>
<p>				containerID: "sharecount",</p>
<p>				cid:cid,</p>
<p>				layout:'vertical'</p>
<p>			};</p>
<p>			var params2 ={</p>
<p>				userAction:ua,</p>
<p>				shareButtons:'facebook, twitter, google-plusone, share, email',</p>
<p>				containerID: "sharetools",</p>
<p>				showCounts: "none",</p>
<p>				cid:cid,</p>
<p>				layout:'horizontal'</p>
<p>			};</p>
<p>			var params3 ={</p>
<p>				userAction:ua,</p>
<p>				shareButtons:'facebook, twitter, google-plusone, share, email',</p>
<p>				containerID: "sharetoolstwo",</p>
<p>				showCounts: "none",</p>
<p>				cid:'',</p>
<p>				layout:'horizontal'</p>
<p>			};</p>
<p>			if(Y.one("#sharecount"))</p>
<p>				gigya.services.socialize.showShareBarUI(conf,params1);</p>
<p>			if (Y.one("#sharetools"))</p>
<p>				gigya.services.socialize.showShareBarUI(conf,params2);</p>
<p>			if (Y.one("#sharetoolstwo"))</p>
<p>				gigya.services.socialize.showShareBarUI(conf,params3);</p>
<p>		}</p>
<p>	});</p>
