<p>	//2010/11/25 begin</p>
<p>	function chgSearchType(Id){</p>
<p>		for(var i=1;i<=6;i++){</p>
<p>			if(GetObj("sTb"+i)){</p>
<p>				GetObj("sTb"+i).style.display = "none";</p>
<p>			}</p>
<p>		}</p>
<p>		if(GetObj("sTb"+Id)){</p>
<p>			GetObj("sTb"+Id).style.display = "block";</p>
<p>			if(Id==6){</p>
<p>			  GetObj("hotwords").style.display = "none";</p>
<p>			  document.fm_06.k.value =theSearchinputValue;    </p>
<p>			}else if(Id==5){</p>
<p>			  GetObj("hotwords").style.display = "none";</p>
<p>			  document.fm_05.k.value =theSearchinputValue;    </p>
<p>			}else if(Id==4){</p>
<p>			  GetObj("hotwords").style.display = "none";</p>
<p>			  document.fm_04.k.value =theSearchinputValue;    </p>
<p>			}else if(Id==3){</p>
<p>			  GetObj("hotwords").style.display = "none";</p>
<p>			  document.picsb02_2.q.value =theSearchinputValue;    </p>
<p>			}else   if(Id==2){</p>
<p>				GetObj("hotwords").style.display = "none";</p>
<p>				document.sb02_2.q.value =theSearchinputValue; </p>
<p>			}else if(Id==1){</p>
<p>				document.gform2.q.value =theSearchinputValue; </p>
<p>				if(GetObj("hotwords_link").innerHTML == ""){</p>
<p>					GetObj("hotwords").style.display = "none";</p>
<p>				}else{</p>
<p>					GetObj("hotwords").style.display = "block";</p>
<p>				}</p>
<p>				GetObj("pbg").innerHTML = '';</p>
<p>			}</p>
<p>		}</p>
<p>	}</p>
<p>	//2010/11/25end</p>
