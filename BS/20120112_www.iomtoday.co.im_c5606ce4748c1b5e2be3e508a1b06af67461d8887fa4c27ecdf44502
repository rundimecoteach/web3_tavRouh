<p>                $('#logoutPanel').show();</p>
<p>                $('#loginRegisterPanel').hide();</p>
<p>                $('#username2').text( json.loginStatus.screenname );</p>
<p>            }</p>
<p>                        $('#subscribeNLWidgetLoggedOut').hide();</p>
<p>            $('#subscribeNLWidgetLoggedIn').show();</p>
<p>        } else {</p>
<p>            $('#loginRegisterPanel').show();</p>
<p>            $('#logoutPanel').hide();</p>
<p>            $('#profileLogoutPanel').hide();</p>
<p>            $('#subscribeNLWidgetLoggedIn').hide();</p>
<p>            $('#subscribeNLWidgetLoggedOut').show();</p>
<p>        }</p>
