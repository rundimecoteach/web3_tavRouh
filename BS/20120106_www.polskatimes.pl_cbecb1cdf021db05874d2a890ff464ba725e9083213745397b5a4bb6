<p>var oLi = GR("$", "menu-regiony").getElementsByTagName("li");</p>
<p>var dlugoscOli = oLi.length;</p>
<p>for (var i = 0; i < dlugoscOli; i++) {</p>
<p>    var kat = oLi[i].id.split("-").shift();</p>
<p>    if (kat == "kategoria") {</p>
<p>        if (i == 0 && !GR("$", oG["ostatnieID"])) {</p>
<p>            oG["ostatnieID"] = 0;</p>
<p>            oG["aktywny"] = oLi[i].id;</p>
<p>        }</p>
<p>        oLi[i].onmouseover = function() { zmien(this.id, true); return false }</p>
<p>        oLi[i].onmouseout = function() { zmien(this.id, false); return false }</p>
<p>    }</p>
<p>}</p>
<p></p>
<p>function zmien(id, focus) {</p>
<p>    if (oG["ostatnieID"] == 0) {</p>
