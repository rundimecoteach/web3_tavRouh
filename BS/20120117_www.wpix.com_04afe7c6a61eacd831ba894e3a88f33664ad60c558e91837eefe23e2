<p>function readCookie(name) {   var nameEQ = name + "=";   var ca = document.cookie.split(';');   for(var i=0;i < ca.length;i++) {   var c = ca[i];   while (c.charAt(0)==' ') c = c.substring(1,c.length);   if (c.indexOf(nameEQ) == 0) return c.substring(nameEQ.length,c.length);   }   return null;   }   var x = readCookie('c_mId');   if (x)  {   try {if (typeof adTargetConsumerInfo == 'undefined') {document.write('<scr'+'ipt type="text/javascript" src="include-member-ad.javascript"></scr'+'ipt>');   }   } catch(err) {}}      var scriptSrc='http://ad.doubleclick.net/adj/trb.wpix2/news;;ptype=sbrd;slug=wpix-hospital-patient-gets-40-million-bill;pos=Tbrd;dcopt=ist;sz=728x90;tile=1;ord=27219516?';   var noscriptHref='http://ad.doubleclick.net/jump/trb.wpix2/news;;ptype=sbrd;slug=wpix-hospital-patient-gets-40-million-bill;pos=Tbrd;sz=728x90;tile=1;ord=27219516?" target="_blank" rel="nofollow"';   var noscriptSrc='http://ad.doubleclick.net/ad/trb.wpix2/news;;ptype=sbrd;slug=wpix-hospital-patient-gets-40-million-bill;pos=Tbrd;dcopt=ist;sz=728x90;tile=1;ord=27219516?" width="728" height="90" border="0" alt=""';   indexOfHrefQuestion='144';   indexOfLastImgSrcQuestion='152';   try {       if (adTargetConsumerInfo != undefined) {           scriptSrc = scriptSrc.substring(0, scriptSrc.length -1) + adTargetConsumerInfo + '?';           noscriptHref = noscriptHref.substring(0, indexOfHrefQuestion) + adTargetConsumerInfo + '?';           noscriptSrc = noscriptSrc.substring(0, indexOfLastImgSrcQuestion) + adTargetConsumerInfo + '?';       }   } catch(err) {}   document.write('<scr'+'ipt type="text/javascript" src="'+scriptSrc+'"></scr'+'ipt>');   document.write('<noscript><a href="'+noscriptHref+'><img scr="'+noscriptSrc+'></a></noscript>');</p>
