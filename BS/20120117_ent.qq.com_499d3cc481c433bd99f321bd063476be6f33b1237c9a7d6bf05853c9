<p>mbAssistantTool.prototype.initTalkBox = function(){</p>
<p>	var _this = this;</p>
<p>	if(this._msgTxt.value.indexOf(window.location)==-1)</p>
<p>		this._talkBoxObj.txtMax = 140-11;</p>
<p>	this._talkBoxObj.success = function(data){</p>
<p>		var afterSendObj = _$("mbAfterSend");</p>
<p>		var backgroundObj = _$("afterBack_mod");</p>
<p>		if(data.result == 0){</p>
<p>			afterSendObj.style.display = "block";</p>
<p>			backgroundObj.style.display = "block";</p>
<p>			setTimeout(function(){</p>
<p>				afterSendObj.style.display = "none";</p>
<p>				backgroundObj.style.display =  "none";},  3000);</p>
<p>			if(sosoArticlKey!=""&&!(/&#[0-9]+;/.test(sosoArticlKey))) </p>
<p>				_this._msgTxt.value = "#"+sosoArticlKey+"#";</p>
<p>			_this.focusTextBox();</p>
<p>			_this.addLinkClick();</p>
<p>		}</p>
<p>	};</p>
<p>	this._talkBoxObj.getContentFunc = function(){</p>
<p>		if(_MUI.G('addArticleLink').checked && _this._msgTxt.value.indexOf(window.location)==-1)</p>
<p>			return _this._msgTxt.value + window.location;</p>
<p>		else</p>
<p>			return _this._msgTxt.value;</p>
<p>	};</p>
<p>}</p>
