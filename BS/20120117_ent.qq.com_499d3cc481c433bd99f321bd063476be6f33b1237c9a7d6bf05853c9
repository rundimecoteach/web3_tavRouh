<p>	function _cbSum(data){</p>
<p>		if(typeof data !== "undefined"){</p>
<p>					TencentArticl.$("comNum").innerHTML = TencentArticl.$("comNum2").innerHTML = TencentArticl.$("comNum3").innerHTML=arguments[0];//总贴数</p>
<p>					TencentArticl.$("comNum4").innerHTML =arguments[1];//参与</p>
<p>					TencentArticl.$("cmt_1").href = TencentArticl.$("cmt_2").href = TencentArticl.$("cmt_3").href="http://"+TencentArticl.cmtBase+"."+TencentArticl.site+".qq.com/comment.htm?site="+TencentArticl.site+"&id="+TencentArticl.cmtId+"";</p>
<p>					TencentArticl.$("cmt_1").target = TencentArticl.$("cmt_2").target = TencentArticl.$("cmt_3").target = "_blank";</p>
<p>				if(arguments[2]>0&&arguments[3]!==0){//如果精华帖不为空</p>
<p>		TencntART.JsLoader.load("http://"+TencentArticl.cmtBase+"."+TencentArticl.site+".qq.com/php_qqcom/greplyBycid.php?site="+TencentArticl.site+"&c_id="+TencentArticl.cmtId+"&t_c=5&o_c=0&t="+Math.round(Math.random()*10000), function(){</p>
<p>					setTimeout("_replyBycid",0);</p>
<p>				});</p>
<p>				}else{</p>
<p>   /*getSibling(TencentArticl.$("cmt_3")).style.display = "none";*/</p>
<p>					TencentArticl.$(TencentArticl.comtList).style.display = "none";</p>
<p>				}</p>
<p>		}</p>
<p>	}</p>
<p>            function getSibling(el){//FF</p>
<p>		var prev =  el.previousSibling;</p>
<p>		while(prev.nodeType != 1){</p>
<p>			prev = prev.previousSibling;</p>
<p>		}</p>
<p>		return prev;</p>
<p>	};</p>
<p>	function _replyBycid(data){</p>
<p>		var tempHTML = "";</p>
<p>		if(typeof data !== "undefined"){</p>
<p>			</p>
<p>			var l = data.hot_list.length;</p>
<p>			l = l > 5 ? 5 : l;</p>
<p>			for(var i=0; i < l; i++){</p>
<p>				if(i==l-1){</p>
<p>				tempHTML+="<li style='border-bottom:0px;'><span class='member'>腾讯"+data.hot_list[i].province_city+"网友 <a href='http://"+TencentArticl.cmtBase+"."+TencentArticl.site+".qq.com/comment_user2.htm?uin="+data.hot_list[i].uin+"' target='_blank'>"+data.hot_list[i].nick+"</a></span>";</p>
<p>				if(data.hot_list[i].subflag==1){</p>
<p>						tempHTML+="<p>"+data.hot_list[i].reply_content+"<a href=http://"+TencentArticl.cmtBase+"."+TencentArticl.site+".qq.com/comment.htm?site="+TencentArticl.site+"&id="+TencentArticl.cmtId+" target='_blank'>[详细]</a></p>";</p>
<p>					}else{</p>
<p>						tempHTML+="<p>"+data.hot_list[i].reply_content+"</p>";</p>
<p>					}</p>
<p>				}else{</p>
<p>								tempHTML+="<li><span class='member'>腾讯"+data.hot_list[i].province_city+"网友 <a href='http://"+TencentArticl.cmtBase+"."+TencentArticl.site+".qq.com/comment_user2.htm?uin="+data.hot_list[i].uin+"' target='_blank'>"+data.hot_list[i].nick+"</a></span>";</p>
<p>				if(data.hot_list[i].subflag==1){</p>
<p>						tempHTML+="<p>"+data.hot_list[i].reply_content+"<a href=http://"+TencentArticl.cmtBase+"."+TencentArticl.site+".qq.com/comment.htm?site="+TencentArticl.site+"&id="+TencentArticl.cmtId+" target='_blank'>[详细]</a></p>";</p>
<p>					}else{</p>
<p>						tempHTML+="<p>"+data.hot_list[i].reply_content+"</p>";</p>
<p>					}</p>
<p>				}</p>
<p>			}</p>
<p>		}</p>
<p>	TencentArticl.$(TencentArticl.comtList).innerHTML = tempHTML;</p>
<p>	}</p>
