<p>mbAssistantTool.prototype.addArticleLink = function(){</p>
<p>	_MI.Bos('btnMbAssistantB','','qqcom.dp.btnMbAssistantB');</p>
<p>	this._talkBoxObj.sourcePic = '';</p>
<p>	var imgs = _$$(document.getElementById("Cnt-Main-Article-QQ"), 'img');</p>
<p>	for(var i in imgs){</p>
<p>		var o = imgs[i], w = _MUI.width(o), h = _MUI.height(o);</p>
<p>		if(w>100 || h>100){</p>
<p>			this._talkBoxObj.sourcePic = o.src;</p>
<p>			break;</p>
<p>		}	</p>
<p>	}</p>
<p>}</p>
