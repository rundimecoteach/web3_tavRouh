<p>mbAssistantTool.prototype.addEvent =function(obj, event, func){</p>
<p>	if(typeof obj.onclick != "function"){</p>
<p>		if(window.attachEvent)	{</p>
<p>			obj.attachEvent("on"+event,func);</p>
<p>		}</p>
<p>		else if(window.addEventListener) {</p>
<p>			obj.addEventListener(event,func,false);</p>
<p>		}</p>
<p>	}</p>
<p>	else{</p>
<p>		var funcOld = obj.onclick;</p>
<p>		obj.onclick = function(){</p>
<p>			func();</p>
<p>			funcOld();</p>
<p>		}</p>
<p>	}</p>
<p>}</p>
