<p>	MI.Login.setCallback("broad_login", UI.bind(function(){</p>
<p>		document.getElementById("talkBox").style.display = "block";</p>
<p>		_this.focusTextBox();</p>
<p>	}));</p>
<p>	MI.AccountInfo.setCallback("broad_login", UI.bind(function(a){</p>
<p>		var nickname = a.mbnick ? a.mbnick : a.nick;</p>
<p>		document.getElementById("talkbox_title").innerHTML = '<a onclick="MI.Bos(\'btnPortalJump1\')" href="http://t.qq.com/'+a.mbid+'/?pref=qqcom.dp.mbassistant" target="_blank">您好，'+nickname+'</a>';</p>
<p>	}));</p>
<p>	if (!c || !e) {	</p>
<p>		MI.Login.showPopup("broad_login", this)</p>
<p>	}</p>
<p>	else{</p>
<p>		this.SetUserNick();</p>
<p>		_$("talkBox").style.display = "block";</p>
<p>	}</p>
<p>	this.focusTextBox();</p>
<p>}</p>
