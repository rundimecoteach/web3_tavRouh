<p>mbAssistantTool.prototype.addLinkClick = function(){</p>
<p>	if(_MUI.G('addArticleLink').checked && document.getElementById("msgTxt").value.indexOf(window.location)==-1){</p>
<p>		this._talkBoxObj.txtMax = 140-11;</p>
<p>		this._talkBoxObj.countTxt();</p>
<p>	}else{</p>
<p>		this._talkBoxObj.txtMax = 140;</p>
<p>		this._talkBoxObj.countTxt();</p>
<p>	}</p>
<p>}</p>
