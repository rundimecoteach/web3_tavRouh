<p>mbAssistantTool.prototype.showMessage = function(page){</p>
<p>	for(var i=0; i<this.comments.length; ++i){</p>
<p>		var msg = _MUI.DC('div');</p>
<p>		msg.innerHTML = msg.innerHTML + '<a class="comment_account" href="http://t.qq.com/'+this.comments[i].userName +'" target="_blank">'+this.comments[i].userNick+'</a><span class="comment_content">：'+ this.comments[i].content+'</span><a class="time_zone" href="http://t.qq.com/p/t/'+ this.comments[i].id + '" target="_blank">(' + this.comments[i].time+')</a>';</p>
<p>		_MUI.addClass(msg, 'msg_content');</p>
<p>		_MUI.append(msg, this._msg_area);</p>
<p>		if(i != (this.comments.length-1)){</p>
<p>			var div_line = _MUI.DC('div');</p>
<p>			_MUI.addClass(div_line, 'div_line');</p>
<p>			_MUI.append(div_line, this._msg_area);</p>
<p>		}</p>
<p>	}</p>
<p>}</p>
