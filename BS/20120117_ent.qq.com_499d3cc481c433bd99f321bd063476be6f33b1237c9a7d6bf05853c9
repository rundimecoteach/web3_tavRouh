<p>mbAssistantTool.prototype.getCommentMsgs = function(page){</p>
<p>	var _this = this;</p>
<p>	var url = 'http://public.open.t.qq.com/api/other/url_converge';</p>
<p>	var params = {</p>
<p>		format: 'getCommentMsgCallback',</p>
<p>		pageflag: '0',</p>
<p>		url: ARTICLE_INFO.article_url,</p>
<p>		pagetime: 'xxx',</p>
<p>		tweetid: 'xxx',</p>
<p>		type: '1',</p>
<p>		words: '5',</p>
<p>		falg: '1',</p>
<p>		reqnum: '10',</p>
<p>		detaillevel: '3'</p>
<p>	};</p>
<p>	var data = [];</p>
<p>	for(i in params)</p>
<p>		data.push(i + '=' + encodeURIComponent(params[i]));</p>
<p>	url += ('?'+data.join('&'));</p>
<p>	_MUI.crossAsynJson(url,'getCommentMsgCallback',function(data){</p>
<p>		if(!data)</p>
<p>			return;</p>
<p>		if(data.ret == 0){</p>
<p>			_$$(_this._body, '.msg_loading')[0].style.display = "none";</p>
<p>			var msgCount = 0;</p>
<p>			if(data.data.countinfo != null){</p>
<p>				for(var i=0; i<data.data.countinfo.length; ++i){</p>
<p>					if(data.data.countinfo[i].counttype==1){</p>
<p>						msgCount = data.data.countinfo[i].countnum;</p>
<p>						break;</p>
<p>					}</p>
<p>				}</p>
<p>			}</p>
<p>			if(msgCount > 99)</p>
<p>				msgCount = 99;</p>
<p>			//Message Count Setting</p>
<p>			if(msgCount > 0){</p>
<p>				_$$(_this._body, '.mbNum')[0].innerHTML = '<span>' + msgCount + '</span>';</p>
<p>				_$$(_this._body, '.mbNumZone')[0].style.display = "";</p>
<p>			}</p>
<p>			if(data.data.tweetinfo){</p>
<p>				for(var i=0; i<data.data.info.length; ++i){</p>
<p>					if(data.data.tweetinfo[i].type != 1)</p>
<p>						continue;</p>
<p>					var msgObj = {</p>
<p>						content: data.data.info[i].text, </p>
<p>						userName: data.data.tweetinfo[i].name, </p>
<p>						userNick: data.data.tweetinfo[i].nick, </p>
<p>						time: _this.formatTime(data.data.tweetinfo[i].time),</p>
<p>						id: data.data.tweetinfo[i].tweetid</p>
<p>					};</p>
<p>					_this.comments.push(msgObj);</p>
<p>				}</p>
<p>			}</p>
<p>			if(msgCount == 0){</p>
<p>				_$$(_this._body, '.send_box')[0].style.display = "none";</p>
<p>				_this._wbTalkBox.style.display = "";</p>
<p>				_$("msgTxt").style.height = "175px";</p>
<p>				_$$(_this._body, '.text_box')[0].style.height = "185px";</p>
<p>				_$$(_this._body, '.comment_msg')[0].style.display = "none";</p>
<p>			}</p>
<p>			_this.showMessage();</p>
<p>		}</p>
<p>	}, 'utf-8')</p>
<p>}</p>
<p></p>
<p>/**</p>
<p>	time: 消息发表时间</p>
<p>	return: 刚刚， xx分钟前，xx小时前，xx天前</p>
<p>*/</p>
<p>mbAssistantTool.prototype.formatTime = function(time){</p>
<p>	var now = Math.floor(new Date().getTime()/1000);</p>
<p>	var diff = now - time;</p>
<p>	if(diff < 60)</p>
<p>		return '刚刚';</p>
<p>	else if(diff >= 60  && diff < 60*60)</p>
<p>		return Math.floor(diff/60) + '分钟前';</p>
<p>	else if(diff >= 60*60 && diff < 60*60*24)</p>
<p>		return Math.floor(diff/3600) + '小时前';</p>
<p>	else{</p>
<p>		var postTime = new Date();</p>
<p>		postTime.setTime(time*1000);</p>
<p>		var m = postTime.getMonth()+1;</p>
<p>		m = m > 9 ? m : ("0" + m);</p>
<p>		var d = postTime.getDate() > 9 ? postTime.getDate() : "0" + postTime.getDate();</p>
<p>		var h = postTime.getHours() > 9 ? postTime.getHours() : "0" + postTime.getHours();</p>
<p>		var min = postTime.getMinutes() > 9 ? postTime.getMinutes() : "0" + postTime.getMinutes();</p>
<p>		return  (m + "月" + d + "日 " + h + ":" + min);</p>
<p>	}</p>
<p>}</p>
<p></p>
<p>mbAssistantTool.prototype.showrelayArea = function(){</p>
<p>	var _this = this;</p>
<p>	var c = MI.Login.getUin();</p>
