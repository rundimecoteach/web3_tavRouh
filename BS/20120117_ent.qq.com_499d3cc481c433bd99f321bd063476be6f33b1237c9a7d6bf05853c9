<p>_MI.GoTop1 = {</p>
<p>	_body : null,</p>
<p>	delay : {},</p>
<p>	resize : 0,</p>
<p>	position : function(){</p>
<p>		this.build();</p>
<p>	},</p>
<p>	build : function(main,bar) {</p>
<p>		var _main = _$(main);</p>
<p>		if (!_main) {</p>
<p>			return;</p>
<p>		}</p>
<p>		var Self = this,</p>
<p>		leftFix = 5,</p>
<p>		left = _MUI.getX(_main) + _MUI.width(_main) - leftFix,</p>
<p>		foot,</p>
<p>		windowHeight = _MUI.windowHeight(),</p>
<p>		pageHeight = _MUI.pageHeight(),</p>
<p>		scrollY,</p>
<p>		resize,</p>
<p>		y;</p>
<p>		Self._body = _$(bar);</p>
<p>		if (_main) {</p>
<p>			foot = pageHeight - (_MUI.getY(_main) + _MUI.height(_main));</p>
<p>			position();</p>
<p>			Self._body.onfocus = function(){</p>
<p>				this.blur();</p>
<p>			}</p>
<p>			_MUI.EA(window,'scroll',position);</p>
<p>			_MUI.EA(window,'resize',position);</p>
<p></p>
<p>		}</p>
<p>		function position(e){</p>
<p>			resize = (e && _MUI.E(e).type == 'resize') || Self.resize;</p>
<p>			clearTimeout(Self.delay.position);</p>
<p>			if (_MUI.B.ie6) {</p>
<p>				_MUI.hide(Self._body);</p>
<p>			}</p>
<p>			Self.delay.position = setTimeout(set,_MUI.B.ie6 ? 400 : 0);</p>
<p>		}</p>
<p>		function set(){</p>
<p>			var pageHeight = _MUI.pageHeight();</p>
<p>			if (resize) {</p>
<p>				windowHeight = _MUI.windowHeight()</p>
<p>				left = _MUI.getX(_main) + _MUI.width(_main) - leftFix;</p>
<p>				Self.resize = 0;</p>
<p>			}</p>
<p>			scrollY = _MUI.scrollY();</p>
<p>			Self._body.style.cssText = 'left:' + left + 'px;';</p>
<p>				_MUI.show(Self._body);</p>
<p>			y = scrollY + windowHeight - pageHeight;</p>
<p>			if (y >= -foot) {</p>
<p>				if(!_MUI.B.ie)</p>
<p>					Self._body.style.cssText += ';position:absolute;top:' + (pageHeight - 45 - foot) + 'px';</p>
<p>				Self._body.style.cssText += ';position:absolute;top:' + (pageHeight - 30 - foot) + 'px';</p>
<p>			}</p>
<p>		}</p>
<p>	}</p>
<p>}</p>
