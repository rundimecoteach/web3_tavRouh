<p>mbAssistantTool.prototype.focusTextBox = function(){</p>
<p>	if(_MUI.B.ie){</p>
<p>		var range = this._msgTxt.createTextRange();</p>
<p>		range.collapse(true);</p>
<p>		range.moveStart('character', this._msgTxt.value.length);</p>
<p>		range.select();</p>
<p>	}else{</p>
<p>		this._msgTxt.focus();</p>
<p>	}</p>
<p>}</p>
