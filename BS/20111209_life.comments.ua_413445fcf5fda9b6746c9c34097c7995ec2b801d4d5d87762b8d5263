<p>	function getScrollTop() {</p>
<p>	   var scrOfY = 0;</p>
<p>	   if(typeof(window.pageYOffset) == "number"){</p>
<p>		   //Netscape compliant</p>
<p>		   scrOfY = window.pageYOffset;</p>
<p>	   }else if(document.body && (document.body.scrollLeft || document.body.scrollTop)){</p>
<p>		   //DOM compliant</p>
<p>		   scrOfY = document.body.scrollTop;</p>
<p>	   }else if(document.documentElement && (document.documentElement.scrollLeft || document.documentElement.scrollTop)){</p>
<p>		   //IE6 Strict</p>
<p>		   scrOfY = document.documentElement.scrollTop;</p>
<p>	   }</p>
<p>	   return scrOfY;</p>
