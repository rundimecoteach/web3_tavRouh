<p>	function SwapTabs(tabName)</p>
<p>	{</p>
<p>		var tabs=['Today','Week','Month'];</p>
<p>		for(var i=0;i<3;i++)</p>
<p>		{</p>
<p>			if(tabs[i]==tabName)</p>
<p>			{</p>
<p>				document.getElementById('tab'+tabs[i]).className='tab_on';</p>
<p>				document.getElementById('div'+tabs[i]).style.display='';</p>
<p>			}</p>
<p>			else</p>
<p>			{</p>
<p>				document.getElementById('tab'+tabs[i]).className='tab';</p>
<p>				document.getElementById('div'+tabs[i]).style.display='none';</p>
<p>			}</p>
<p>		}</p>
<p>	}</p>
