<p>function show_responses() {var x = ((window.ActiveXObject) ? new ActiveXObject("Microsoft.XMLHTTP") : new XMLHttpRequest());</p>
<p>x.open("GET","responses.ashx?did=1000716239&t=" + (new Date()).getTime() ,true);</p>
<p>x.onreadystatechange = function() {</p>
<p>if (x.readyState == 4 && x.status == 200) {</p>
<p>try {	var t = document.getElementById("responses_table");</p>
<p>var responses = x.responseText.split("\n");</p>
<p>for(var i in responses) {</p>
<p>var ri = responses[i].split(",");</p>
<p>if (ri.length < 4) {continue}</p>
<p>var row = t.insertRow(t.rows.length);</p>
<p>row.setAttribute("valign","top");</p>
<p>row.insertCell(0).innerHTML = "<span style='font-family:arial;font-size:11px; '>"+(parseInt(i)+1).toString()+".&nbsp;</span>"; </p>
<p>row.insertCell(1).innerHTML = '<a style="font-size:11px;" href="javascript:readResponse(1000716239, ' + ri[0] + ')">' + (ri[3] ? ri[3] : "no subject") + '</a>' // subject</p>
<p>row.insertCell(2).innerHTML = "&nbsp;<span style='font-family:arial;font-size:11px;color:#cc0000; '>" + (ri[2] ? ri[2] : "no name")+"</span>" // username</p>
<p>row.insertCell(3).innerHTML = "&nbsp;<span style='font-family:arial;font-size:11px;color:#6d6d6d; '>" + ri[1].substring(0,(ri[1].length-6)).replace(/ /g,"&nbsp;") // datetime</p>
<p>}</p>
<p>} catch(ex) {}</p>
<p>}</p>
<p>}</p>
<p>x.send(null)</p>
<p>} show_responses()</p>
