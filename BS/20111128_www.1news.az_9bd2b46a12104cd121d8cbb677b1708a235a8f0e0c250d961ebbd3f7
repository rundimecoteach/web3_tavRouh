<p>function sendOpechatka()</p>
<p>{</p>
<p>	$('#opechatkaT').hide();</p>
<p></p>
<p>	$('#opechatkaMsgs').html('<div style="text-align:center;"><img src="/i/loading.gif" width="32" height="32" alt="loading" style="margin:40px auto 15px auto;"></div>');</p>
<p>	$('#opechatkaMsgs').show();</p>
<p></p>
<p>	var opechatkaText = $('#opechatkaText').val();</p>
<p>	var comments = $('#opechatkaComments').val();</p>
<p></p>
<p>	$.ajax(</p>
<p>	{</p>
<p>		type: "POST",</p>
<p>		url: "/opechatka.php",</p>
<p>		data: "sec_id=4&item_id=20111128061836438&opechatkaText="+opechatkaText+"&comments="+comments,</p>
<p>		success: function(msg)</p>
<p>		{</p>
<p>			if (msg) $('#opechatkaMsgs').html('<div style="text-align:center; font-size:12px; padding:20px;">Информация отослана. Спасибо.</div>');</p>
<p>			else $('#opechatkaMsgs').html('<div style="text-align:center; font-size:12px; padding:20px;">Возникла ошибка.</div>');</p>
<p>		}</p>
<p>	});</p>
<p></p>
<p>	$('#opechatkaComments').val('');</p>
<p>	return false;</p>
<p>}</p>
<p></p>
<p>function wordwrap( str, width, brk, cut )</p>
<p>{</p>
<p>	brk = brk || '\n';</p>
<p>	width = width || 75;</p>
<p>    cut = cut || false;</p>
<p> </p>
<p>    if (!str) { return str; }</p>
<p> </p>
<p>    var regex = '.{1,' +width+ '}(</p>|$)' + (cut ? '|.{' +width+ '}|.+$' : '|\\S+?(</p>|$)');</p>
<p></p>
<p>	return str.match( RegExp(regex, 'gi') );//.join( brk );</p>
<p>}</p>
<p></p>
<p>var symbolsNotWrapped = 6000;</p>
<p>var symbolsMiddleWrapped = 8500;</p>
<p>var symbolsLastPage = 500;</p>
<p>var symbolsperpage = 4000;</p>
<p>var pages = 0;</p>
<p>var curPage = 0;</p>
<p>var content = '';</p>
<p>var animate = false;</p>
<p></p>
<p>function rebuildContent()</p>
<p>{</p>
<p>	content = content.replace(/\n/g, "");</p>
<p>	content2 = content.replace(/<object .+<\/object>/gi, "");</p>
<p></p>
<p>	var conLen = content2.length;</p>
<p>//	var conLen = content.length;</p>
<p>//	console.log(conLen);</p>
<p></p>
<p>	var Opera = /opera/i.test(navigator.userAgent);</p>
<p>	if (Opera) conLen = 0;</p>
<p></p>
<p>	if (conLen <= symbolsNotWrapped)</p>
<p>	{</p>
<p>	}</p>
<p>	else if (conLen > symbolsNotWrapped && conLen < symbolsMiddleWrapped)</p>
<p>	{</p>
<p>		pages = 2;</p>
<p></p>
<p>		textParts = wordwrap(content, symbolsperpage, "%%", false);</p>
<p>//		textParts = contentWrapped.split("%%");</p>
<p></p>
<p>		if (curPage == 0) $('#content').html(textParts[curPage]);</p>
<p>		else </p>
<p>		{</p>
<p>			var text = textParts[curPage];</p>
<p>			if (textParts[curPage+1] != undefined)</p>
<p>			{</p>
<p>				text += textParts[curPage+1];</p>
<p>			}</p>
<p>			$('#content').html(text);</p>
<p>		}</p>
<p>		</p>
<p>		if (animate) $('html,body').animate({scrollTop: $("#artc_title").offset().top}, 400);</p>
<p></p>
<p>		pagesHtml = '';</p>
<p>		for (i=1;i<=pages ;++i )</p>
<p>		{</p>
<p>			if (i == (curPage+1))</p>
<p>			{</p>
<p>				pagesHtml += '<strong>'+i+'</strong>';</p>
<p>			}else</p>
<p>			{</p>
<p>				pagesHtml += '<a href="#page'+i+'" onClick="changePage('+i+');">'+i+'</a>';</p>
<p>			}</p>
<p>		}</p>
<p>		$('#pagesHolder').show();</p>
<p>		$('#pages').html(pagesHtml);</p>
<p></p>
<p>		curPage = parseInt(curPage);</p>
<p>		if (curPage > 0) $('#pageofpage').html('Страница '+(curPage+1)+' из '+pages).css('margin-bottom', '10px');</p>
<p>		else $('#pageofpage').html('').css('margin-bottom', '0');</p>
<p></p>
<p>	}else</p>
<p>	{</p>
<p>		textParts = wordwrap(content, symbolsperpage, "%%", false); 		</p>
<p>//		textParts = contentWrapped.split("%%");</p>
<p>		textPartsLen = textParts.length;</p>
<p>		</p>
<p>		pages = textPartsLen;</p>
<p></p>
<p>		var lastPageText = '';</p>
<p>		if (textParts[pages-1].length < 500)</p>
<p>		{</p>
<p>			pages -= 1;</p>
<p>			if (curPage == pages-1)</p>
<p>			{</p>
<p>				lastPageText = textParts[pages];</p>
<p>			}</p>
<p>		}</p>
<p></p>
<p>		$('#content').html(textParts[curPage]+lastPageText);</p>
<p></p>
<p>		if (animate) $('html,body').animate({scrollTop: $("#artc_title").offset().top}, 400);</p>
<p></p>
<p>		pagesHtml = '';</p>
<p>		for (i=1;i<=pages ;++i )</p>
<p>		{</p>
<p>			if (i == (curPage+1))</p>
<p>			{</p>
<p>				pagesHtml += '<strong>'+i+'</strong>';</p>
<p>			}else</p>
<p>			{</p>
<p>				pagesHtml += '<a href="#page'+i+'" onClick="changePage('+i+');">'+i+'</a>';</p>
<p>			}</p>
<p>		}</p>
<p>		$('#pagesHolder').show();</p>
<p>		$('#pages').html(pagesHtml);</p>
<p></p>
<p>		curPage = parseInt(curPage);</p>
<p>		if (curPage > 0) $('#pageofpage').html('Страница '+(curPage+1)+' из '+pages).css('margin-bottom', '10px');</p>
<p>		else $('#pageofpage').html('').css('margin-bottom', '0');</p>
<p>	}</p>
<p>}</p>
<p></p>
<p>function changePage(pageN)</p>
<p>{</p>
<p>	curPage = pageN-1;</p>
<p>	animate = true;</p>
<p>	rebuildContent();</p>
<p>}</p>
<p></p>
<p>function addLink()</p>
<p>{</p>
<p>	var body_element = document.getElementsByTagName('body')[0];</p>
<p></p>
<p>	var pagelink = "<br><br> Подробности: <a href='"+location.href+"'>"+location.href+"</a><br>Любое использование материалов допускается только при наличии гиперссылки на 1news.az";</p>
<p></p>
<p>	var selection;</p>
<p>	if (Browser.Version() <= 9)</p>
<p>	{</p>
<p>		selection = document.selection.createRange();</p>
<p>		var copytext = selection.htmlText + pagelink;</p>
<p>	}else</p>
<p>	{</p>
<p>		selection = window.getSelection();</p>
<p>		var copytext = selection + pagelink;</p>
<p>	}</p>
<p></p>
<p>	var newdiv = document.createElement('div');</p>
<p>	var newdiv2 = document.createElement('div');</p>
<p></p>
<p>	newdiv.style.position='absolute';</p>
<p>	newdiv.style.left='-99999px';</p>
<p>	newdiv.style.backgroundColor='white';</p>
<p>	newdiv.style.color='black';</p>
<p>	newdiv.style.textAlign='left';</p>
<p>	newdiv.style.fontSize='12px';</p>
<p></p>
<p>	newdiv2.style.position='absolute';</p>
<p>	newdiv2.style.left='-99999px';</p>
<p></p>
<p>	document.body.insertBefore(newdiv2, document.body.firstChild);</p>
<p></p>
<p>	newdiv.innerHTML= copytext;</p>
<p></p>
<p>	newdiv2.appendChild(document.createElement("br"));</p>
<p>	newdiv2.appendChild(newdiv);</p>
<p></p>
<p>	if (Browser.Version() <= 9)</p>
<p>	{</p>
<p>		pagelink=document.body.createTextRange();</p>
<p>		pagelink.moveToElementText(newdiv);</p>
<p>		pagelink.select();</p>
<p>	}else</p>
<p>	{</p>
<p>		selection.selectAllChildren(newdiv);</p>
<p>	}</p>
<p></p>
<p>//	setTimeout(function() {body_element.removeChild(newdiv);},0);</p>
<p>}</p>
<p></p>
<p>var Browser = {</p>
<p>  Version: function() {</p>
<p>    var version = 999; // we assume a sane browser</p>
<p>    if (navigator.appVersion.indexOf("MSIE") != -1)</p>
<p>      // bah, IE again, lets downgrade version number</p>
<p>      version = parseFloat(navigator.appVersion.split("MSIE")[1]);</p>
<p>    return version;</p>
<p>  }</p>
<p>}</p>
