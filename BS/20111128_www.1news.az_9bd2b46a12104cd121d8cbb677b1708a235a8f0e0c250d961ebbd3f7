<p>function GetSelectedText()</p>
<p>{</p>
<p>	var selectedtext = '';</p>
<p></p>
<p>	if (window.getSelection)</p>
<p>	{</p>
<p>		var range = window.getSelection ();                                        </p>
<p>		selectedtext = range.toString();</p>
<p>	}else if (document.selection.createRange)</p>
<p>	{</p>
<p>		var range = document.selection.createRange ();</p>
<p>		selectedtext = range.text;</p>
<p>	}</p>
<p></p>
<p>	return selectedtext;</p>
<p>}</p>
