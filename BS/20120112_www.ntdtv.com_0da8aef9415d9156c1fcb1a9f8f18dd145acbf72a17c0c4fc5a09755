<p>  function setautonext(boxName ) {</p>
<p>		if(boxName.checked == true) {</p>
<p>			createCookie("autonext","1",100);</p>
<p>		}</p>
<p>		else {</p>
<p>			createCookie("autonext","0",100);</p>
<p>		}</p>
