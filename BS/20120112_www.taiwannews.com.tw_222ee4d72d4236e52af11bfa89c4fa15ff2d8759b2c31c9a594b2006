<p>function send_mail(){</p>
<p>var obj_icon = document.getElementById('func_icon');</p>
<p>	var str = obj_icon.innerHTML;</p>
<p>	obj_icon.innerHTML = '';</p>
<p>	var obj = document.getElementById('fullstory');	</p>
<p>	document.form_mail.news_content.value = obj.innerHTML;	</p>
<p>	var obj_title = document.getElementById('mail_title1');	</p>
<p>	document.form_mail.news_title.value = obj_title.innerHTML;</p>
<p>	var obj_source = document.getElementById('mail_source');</p>
<p>	document.form_mail.news_source.value = obj_source.innerHTML;</p>
<p>	document.form_mail.action = "mailto.php";	</p>
<p>	document.form_mail.submit();</p>
<p>	obj_icon.innerHTML = str;</p>
<p>}</p>
