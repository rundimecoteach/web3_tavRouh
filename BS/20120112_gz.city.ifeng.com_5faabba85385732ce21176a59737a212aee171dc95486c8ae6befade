<p>	function _ArpGetRandomNum_639(Max){  </p>
<p>		var Range = Max-1;</p>
<p>		var Rand = Math.random();</p>
<p>		return(Math.round(Rand * Range));</p>
<p>	}</p>
<p>	var _AdNum = _ArpGetRandomNum_639(2);</p>
<p>	var _AdHtml = document.getElementById("ArpAdPro_639"+_AdNum).innerHTML;</p>
