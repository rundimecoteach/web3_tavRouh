<p>		this.line = 0;</p>
<p>		this.count = 0;</p>
<p>		document.searchForm.select_type.disabled = true;</p>
<p>		document.searchForm.select_code.disabled = true;</p>
<p>		document.searchForm.select_type.value = "";</p>
<p>		document.searchForm.select_code.value = "";</p>
