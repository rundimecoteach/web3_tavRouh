<p>		var _type_conf = {</p>
<p>			"stock": "股票",</p>
<p>			"fund": "基金",</p>
<p>			"hkstock": "港股",</p>
<p>			"forex": "外汇",</p>
<p>			"bond": "债券"</p>
<p>		};</p>
