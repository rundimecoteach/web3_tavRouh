<p>    document.dcdAdsI = new Array();</p>
<p>    document.dcdAdsU = new Array();</p>
<p>    document.dcdAdsR = new Array();</p>
<p>    document.dcdAdsEH = new Array();</p>
<p>    document.dcdAdsE = new Array();</p>
<p>    document.dcdAdsEC = new Array();</p>
<p>    document.dcdAdsAA = new Array();</p>
<p>    document.dcdAdsAI = new Array();</p>
<p>    document.dcdAdsAG = new Array();</p>
<p>    document.dcdAdsToClose = new Array();</p>
