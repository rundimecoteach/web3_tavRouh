<p>path = document.location.href.split("/");</p>
<p>var pType="ArticleModule"</p>
<p>for(x=4; x<7; x++)</p>
<p>{</p>
<p>    if (typeof path[x] == 'undefined')</p>
<p>    {</p>
<p>        path[x] = '';</p>
<p>    }</p>
<p></p>
<p>    if(path[x] == "" || (pType == "ArticleModule" && path[x].match(/[0-9]+\./)) || (path[x].indexOf('?') > -1))</p>
<p>    {</p>
<p>        path[x] = path[x-1];</p>
<p>    }</p>
<p>    else</p>
<p>    {</p>
<p>        path[x] = path[x-1] + "/" + path[x];</p>
<p>    }</p>
<p>    path[x] = path[x].replace(/\/undefined/g,"");</p>
<p>} </p>
<p>sitename = path[2].split(".");</p>
