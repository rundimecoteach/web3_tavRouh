<p>function recordLike(type, omnitureRef, omnitureName)</p>
<p>{</p>
<p>    $.ajax({</p>
<p>        url: "/misc/like/",</p>
<p>        type: "POST",</p>
<p>        data: {url: 'http://www.basingstokegazette.co.uk/news/9474198.Man_denies_infecting_women_with_HIV/', 'type': type, 'title': document.title}</p>
<p>    });</p>
<p>    s.events=omnitureRef;</p>
<p>    s.linkTrackVars='eVar2,eVar4,eVar5,eVar6,eVar10,eVar16,eVar17,eVar18,eVar19,eVar25,eVar43,events';</p>
<p>    s.linkTrackEvents=omnitureRef;</p>
<p>    s.tl(document,'o', omnitureName); </p>
<p>}</p>
