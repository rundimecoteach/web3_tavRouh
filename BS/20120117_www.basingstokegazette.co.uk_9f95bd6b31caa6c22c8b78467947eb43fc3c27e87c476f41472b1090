<p>function crtg_getCookie(c_name){</p>
<p>      var i,x,y,ARRcookies=document.cookie.split(";");</p>
<p>      for(i=0;i<ARRcookies.length;i++){</p>
<p>            x=ARRcookies[i].substr(0,ARRcookies[i].indexOf("="));</p>
<p>            y=ARRcookies[i].substr(ARRcookies[i].indexOf("=")+1);</p>
<p>            x=x.replace(/^\s+|\s+$/g,"");</p>
<p>            if(x==c_name){</p>
<p>                  return unescape(y);</p>
<p>            }</p>
<p>      }return'';</p>
<p>}</p>
