<p>	var loginResponseSet = false;</p>
<p>	var loginHandlers = new Array();</p>
<p></p>
<p>    function loadLogin(json) {</p>
<p>		loginStatusResponse = json;</p>
<p>        if (json.loginStatus.loggedIn) {</p>
<p>            if (json.loginStatus.accessProfile) {</p>
<p>                $('#profileLogoutPanel').show();</p>
<p>                $('#logoutPanel').hide();</p>
<p>                $('#loginRegisterPanel').hide();</p>
<p>                $('#username1').text( json.loginStatus.screenname );</p>
<p>            } else {</p>
<p>                $('#profileLogoutPanel').hide();</p>
<p>                $('#logoutPanel').show();</p>
<p>                $('#loginRegisterPanel').hide();</p>
<p>                $('#username2').text( json.loginStatus.screenname );</p>
<p>            }</p>
<p>                        $('#subscribeNLWidgetLoggedOut').hide();</p>
<p>            $('#subscribeNLWidgetLoggedIn').show();</p>
<p>        } else {</p>
<p>            $('#loginRegisterPanel').show();</p>
<p>            $('#logoutPanel').hide();</p>
<p>            $('#profileLogoutPanel').hide();</p>
<p>            $('#subscribeNLWidgetLoggedIn').hide();</p>
<p>            $('#subscribeNLWidgetLoggedOut').show();</p>
<p>        }</p>
<p></p>
<p>		loginResponseSet = true;</p>
<p>		$(document).trigger('loginResponse');		</p>
<p>    }</p>
<p>    </p>
<p>    				$(document).ready(function() {</p>
<p>    	if (($.cookie("sessionKey") == null) && ($.cookie("rememberMe") == null) ) {</p>
<p>    		loadLogin({loginStatus:{loggedIn:false}});</p>
<p>    	} else {</p>
<p>            $.getJSON('/ajax?action=loginStatus', loadLogin);</p>
<p>    	}</p>
<p>	});</p>
