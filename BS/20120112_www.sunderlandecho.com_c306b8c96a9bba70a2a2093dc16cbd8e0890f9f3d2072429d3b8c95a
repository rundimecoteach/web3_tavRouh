<p>}</p>
<p></p>
<p>function performCarSearch()</p>
<p>{</p>
<p>    var manufactureId = $('#carSearchFrm #make option:selected').val();</p>
<p>    var maufactureName = $('#carSearchFrm #make option:selected').text();</p>
<p>    var modelId = $('#carSearchFrm #model option:selected').val();</p>
<p>    var modelName = $('#carSearchFrm #model option:selected').text();</p>
<p>    if(modelName == "Any Model")</p>
<p>    {</p>
<p>        modelName = "";</p>
<p>    }</p>
<p>    var minPrice = $('#carSearchFrm #priceFrom option:selected').val();</p>
<p>    var maxPrice = $('#carSearchFrm #priceTo option:selected').val();</p>
<p>    </p>
<p>    var strUrl = "http://www.motorstoday.co.uk/results.aspx?location=Sunderland&keywords=&manufacturerid=" + manufactureId + "&manufacturername=" + maufactureName + "&modelid=" + modelId + "&modelname=" + modelName + "&minage=-1&maxage=-1&minprice=" + minPrice + "&maxprice=" + maxPrice + "&maxmileage=-1&radius=10&type=3&sortorder=2&page=1&searchtype=1&derivitive=&fueltype=-1&bodytype=-1&doors=-1&colour=-1&enginesize=-1&transmission=-1";</p>
<p>	strUrl = strUrl + "&SiteKey=" + siteKey;</p>
<p>	</p>
<p>    window.open(strUrl);</p>
<p>}</p>
<p></p>
<p>function performPropertySearch()</p>
<p>{</p>
<p>    var orderType = $('#propertySearchFrm input[name=buyRent]:checked').val();</p>
<p>    var maxPrice = (orderType == 0)</p>
<p>                    ? $('#propertySearchFrm #priceToBuy option:selected').val()</p>
<p>                    : $('#propertySearchFrm #priceToRent option:selected').val();</p>
<p>    var minPrice = (orderType == 0)</p>
<p>                    ? $('#propertySearchFrm #priceFromBuy option:selected').val()</p>
<p>                    : $('#propertySearchFrm #priceFromRent option:selected').val();</p>
<p>    var bedrooms = $('#propertySearchFrm #bedrooms option:selected').val();</p>
<p>    </p>
<p>    var strUrl = "http://www.propertytoday.co.uk/propertyresults.aspx?Town=Sunderland&MaxPrice=" + maxPrice + "&MinPrice=" + minPrice + "&Radius=5&OrderType=1&Bedrooms=" + bedrooms;</p>
<p>    if(orderType == 1)</p>
<p>    {</p>
<p>        strUrl += "&Rent=true";</p>
<p>    }</p>
<p>    strUrl = strUrl + "&SiteKey=" + siteKey;</p>
<p>	</p>
<p>    window.open(strUrl);</p>
<p>}</p>
