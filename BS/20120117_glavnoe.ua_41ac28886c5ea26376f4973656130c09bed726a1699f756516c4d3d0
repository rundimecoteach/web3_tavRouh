<p>function checksearch(form) {</p>
<p>	if(form.sword.value.match(/^\s*$/) || form.sword.value=='поиск') {</p>
<p>		alert('Не заполненно поле для поиска');</p>
<p>		form.sword.focus();</p>
<p>    	return false;</p>
<p>	} else if(form.sword.value.length < 3) {</p>
<p>		echo = '\"' + form.sword.value + '\" слишком маленькое для поиска!';</p>
<p>		alert(echo);</p>
<p>		form.sword.focus();</p>
<p>	    return false;</p>
<p>	}</p>
<p>}</p>
