<p>    GR('$$', 'zakladka-link', $('rankingi-lista'), '', function(e) {</p>
<p>      nazwa = e.getElementsByTagName("a").item(0).innerHTML;</p>
<p>      tr.push({elements: [e], href: '/ranking/zakladka:' + nazwa.trim()});</p>
<p>    });</p>
<p>    </p>
<p>    ln = zakladki.length;</p>
<p>    for(i = 0; i < ln; i++) {</p>
<p>      licznik = 1;</p>
<p>      GR('$$', 'artykul-link-zakladka-' + lista[i], $('najpopularniejsze-box-linki'), '', function(e) {</p>
<p>        tr.push({elements: [e], href: '/ranking/' + lista[i] + '/link:' + licznik++});</p>
<p>      });</p>
<p>    }</p>
<p>    ln = zakladki.length;</p>
<p>    for(i = 0; i < ln; i++) {</p>
<p>      if($('zakladka-ranking_' + zakladki[i])) {</p>
<p>        GR('$$', 'wiecej', $('zakladka-ranking_' + zakladki[i]), '', function(e) {</p>
