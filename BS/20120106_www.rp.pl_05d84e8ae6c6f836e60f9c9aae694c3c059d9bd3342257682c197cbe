<p>		jQuery(document).ready(function() {</p>
<p>			empikBox_76904_tabsInit();		</p>
<p>		});</p>
<p></p>
<p>		function getAjaxProductParams(componentId, categoryId)</p>
<p>		{</p>
<p>			try</p>
<p>			{</p>
<p>				var url = '/ComponentAjax?componentId='+componentId+'&categoryId='+categoryId;</p>
<p>				jQuery.ajax({</p>
<p>					type: "GET",</p>
<p>					url: url,</p>
<p>					success: function(data) {</p>
<p>						jQuery('#empikBox_'+componentId+'_productsBox').html(data);</p>
<p>					}</p>
<p>				});</p>
<p>			}</p>
<p>			catch(err)</p>
<p>			{</p>
<p>			}</p>
<p>		}</p>
<p>		</p>
<p>		function empikBox_76904_tabsInit() {</p>
<p>				var sComponentId = jQuery('#empikBox_76904').attr('id').replace('empikBox_', '');</p>
<p>				try</p>
<p>				{</p>
<p>					getAjaxProductParams(sComponentId, jQuery('#empikBox_76904').find('.empikBox_tabs').children('li:nth-child(1)').attr('id').replace('empikBox_76904_tab_',''));</p>
<p>					jQuery('#empikBox_76904').find('.empikBox_tabs').children('li:nth-child(1)').children('span').addClass('empikBox_tabActive');</p>
<p>					empikBox_76904_fakePics();</p>
<p>				}</p>
<p>				catch(err)</p>
<p>				{</p>
<p>				}</p>
<p>			</p>
<p>			jQuery('#empikBox_76904').find('.empikBox_tabs').children('li').bind('click', function(){</p>
<p>				var sComponentId = jQuery('#empikBox_76904').attr('id').replace('empikBox_', '');</p>
<p>				try</p>
<p>				{</p>
<p>					getAjaxProductParams(sComponentId, jQuery(this).attr('id').replace('empikBox_76904_tab_',''));</p>
<p>					empikBox_tabsEngine(jQuery(this));</p>
<p>					empikBox_76904_fakePics();</p>
<p>				}</p>
<p>				catch(err)</p>
<p>				{</p>
<p>				}				</p>
<p>			});</p>
<p>		}</p>
<p>	</p>
<p>		function empikBox_tabsEngine(oActiveElement) {</p>
<p>			oActiveElement.parent('.empikBox_tabs').find('span').removeClass('empikBox_tabActive');</p>
<p>			oActiveElement.find('span').addClass('empikBox_tabActive');</p>
<p>		}</p>
<p>		</p>
<p>		function empikBox_76904_fakePics() {</p>
<p>			jQuery('#empikBox_76904_productsBox').find('.empikBox_productImg').find('img').bind('error', function(){</p>
<p>				jQuery(this).attr('src', 'http://www.rp.pl/static/img/empikbox_img.jpg');</p>
<p>			});</p>
<p>		}</p>
