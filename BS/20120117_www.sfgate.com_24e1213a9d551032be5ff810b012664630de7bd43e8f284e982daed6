<p>                function ToggleInnerLabel (input,label,on) {</p>
<p>                var labelExists = (input.value == label ? 1 : 0);</p>
<p>                if (on && !labelExists && !input.value) {</p>
<p>                input.value = label;</p>
<p>                input.className = 'textInputNote';</p>
<p>                }</p>
<p>                if (!on && labelExists) {</p>
<p>                input.value = '';</p>
<p>                input.className = 'textInput';</p>
