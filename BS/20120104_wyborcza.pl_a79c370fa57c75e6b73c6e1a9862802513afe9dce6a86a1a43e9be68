<p>function xxFromHref(href){</p>
<p>        var wynik=new String(href);</p>
<p>        var indx1=wynik.indexOf(",");</p>
<p>        var indx2=0;</p>
<p>        if (indx1 > 0) {//</p>
<p>                wynik=wynik.substring(indx1+1);</p>
<p>                indx1=wynik.indexOf(",");</p>
<p>                if (indx1 > 0){</p>
<p>                        wynik=wynik.substring(indx1+1);</p>
<p>                        indx1=wynik.indexOf(",");</p>
<p>                        if (indx1 > 1){</p>
<p>                                wynik=wynik.substring(0,indx1);</p>
<p>                        }</p>
<p>                }</p>
<p>        }</p>
<p>        indx2=wynik.lastIndexOf(".");</p>
<p>        if (indx2 > 0) wynik=wynik.substring(0,indx2);</p>
<p>        return wynik;</p>
<p>}</p>
