<p> function setAifCookie(name, value, params)</p>
<p>{</p>
<p>// не переписываем имеющуюся куку!</p>
<p>if (jQuery.cookie(name) != null) {</p>
<p>//alert(jQuery.cookie(name));</p>
<p>return false;</p>
<p>}</p>
<p></p>
<p>if (params == null){</p>
<p>jQuery.cookie(name, value, {expires: 7, path: '/', secure: false});</p>
<p>    } else {</p>
<p>jQuery.cookie(name, value, params);</p>
<p>}</p>
<p>return true;</p>
<p>}   </p>
