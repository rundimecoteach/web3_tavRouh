<p>		var _url = encodeURIComponent(document.location);</p>
<p>		var _appkey = encodeURI("5bcbe179798c477badb3eb99ed7a49de");//你从腾讯获得的appkey</p>
<p>		var _pic = encodeURI('');//（例如：var _pic='图片url1|图片url2|图片url3....）</p>
<p>		var _site = '';//你的网站地址</p>
<p>		var _u = 'http://v.t.qq.com/share/share.php?url='+_url+'&appkey='+_appkey+'&site='+_site+'&pic='+_pic+'&title='+_t;</p>
<p>		window.open( _u,'', 'width=700, height=680, top=0, left=0, toolbar=no, menubar=no, scrollbars=no, location=yes, resizable=no, status=no' );</p>
