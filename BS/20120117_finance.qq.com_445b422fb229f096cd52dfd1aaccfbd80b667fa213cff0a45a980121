<p>var SCRIPT_TIMEOUT = 20000;</p>
<p>var QVPL_PATH = "http://mat1.gtimg.com/www/html5js/QVPL1.0.4.js";</p>
<p>function loadHelper (jsurl) {</p>
<p>	var oScriptEl, oTimeoutHDL, oHead;</p>
<p>	oScriptEl = document.createElement("script");</p>
<p>	oScriptEl.type = "text/javascript";</p>
<p>	oScriptEl.language = "javascript";</p>
<p>	oScriptEl.src = jsurl;</p>
<p>	oScriptEl.onreadystatechange = doCallback;</p>
<p></p>
<p>	oScriptEl.onload = function()</p>
<p>	{</p>
<p>		this.readyState = "complete";</p>
<p>		doCallback(); </p>
<p>		if(typeof(lianbo) == "object"){</p>
<p>			lianbo.init(window.QVPL);</p>
<p>		}</p>
<p>	};</p>
<p></p>
<p>	oTimeoutHDL = window.setTimeout(doError,SCRIPT_TIMEOUT);</p>
<p>	document.getElementsByTagName("head")[0].appendChild(oScriptEl);</p>
<p></p>
<p>	function doCallback()</p>
<p>	{</p>
<p>		if (oScriptEl.readyState == "complete" || oScriptEl.readyState == "loaded")</p>
<p>		{</p>
<p>			oScriptEl.onload = oScriptEl.onreadystatechange = new Function();</p>
<p>			window.clearTimeout(oTimeoutHDL);</p>
<p>		}</p>
<p>	};</p>
<p></p>
<p>	function doError()</p>
<p>	{</p>
<p>		oScriptEl.parentNode.removeChild(oScriptEl);</p>
<p>	};</p>
<p>}</p>
<p></p>
<p>loadHelper(QVPL_PATH);</p>
<p></p>
<p> </p>
<p>//20110713</p>
<p>if(document.addEventListener)</p>
<p>        document.addEventListener("click", registerZone2, false);</p>
<p>else if(document.attachEvent)</p>
<p>        document.attachEvent("onclick", registerZone2);</p>
<p>var gImage;</p>
<p>//取自定义属性</p>
<p>function registerZone2(ev)</p>
<p>{</p>
<p>    var loopTryNum = 10;</p>
<p>    var bossID = 451;</p>
<p>	var sep = "_";</p>
<p>	var bossFilter = 6;</p>
<p>    try</p>
<p>	{</p>
<p>	    //qq</p>
<p>		var a=document.cookie.match(new RegExp('(^|)o_cookie=([^;]*)(;|$)'));</p>
<p>		var iQQ=(a==null?"":unescape(a[2]));</p>
<p>		var uin=parseInt(parseInt(iQQ)/100)%10;</p>
<p>        if(uin!=bossFilter) return;</p>
<p>		</p>
<p>	    var ev = window.event || ev;</p>
<p>	    var et = ev.srcElement || ev.target;</p>
<p>		var type=et.tagName;</p>
<p>	    if (type != "A" && type != "IMG" ) </p>
<p>		{</p>
<p>		    return true;</p>
<p>		}</p>
<p>		</p>
<p>		//purl</p>
<p>		var purl='';</p>
<p>		if (type == "A")</p>
<p>		{</p>
<p>		    purl = et.href;</p>
<p>		}</p>
<p>		else if (type == "IMG")</p>
<p>		{</p>
<p>		    purl = et.parentNode.href;</p>
<p>		}</p>
<p>		</p>
<p>		//pseudo attr</p>
<p>		var zoneId="";</p>
<p>		for (var i=loopTryNum-1,tagNode=et;i>=0;i--,tagNode=tagNode.parentNode)</p>
<p>		{</p>
<p>		    if(tagNode.attributes['bossZone'])</p>
<p>			{</p>
<p>		        zoneId = tagNode.attributes['bossZone'].nodeValue;</p>
<p>			}</p>
<p>		    if(zoneId) break; </p>
<p>		}		</p>
<p>		if(!zoneId) return;</p>
<p>		</p>
<p>		//site</p>
<p>		var localUrl = location.href;</p>
<p>		var site = localUrl.substring(7,localUrl.indexOf('.qq.com'));</p>
<p>		</p>
<p>		var iurl = 'http://btrace.qq.com/collect?sIp=&iQQ='+iQQ+'&sBiz=qq.com&sOp='+zoneId+sep+site+'&iSta=&iTy='+bossID+'&iFlow=&sUrl='+escape(purl)+'&sLocalUrl='+escape(location.href)+'&'+Math.random();</p>
<p>		gImage = new Image(1,1);</p>
<p>		gImage.src = iurl;</p>
<p>	} catch (e) {}</p>
<p>}</p>
<p></p>
