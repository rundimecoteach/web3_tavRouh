<p>                  var dS  = ''+tmN.getSeconds();dS=dS.length<2?'0'+dS:dS</p>
<p>                  var tmp = ' '+dH+':'+dM+':'+dS</p>
<p></p>
<p>                  d.getElementById('clock').innerHTML=tmp</p>
<p>                  var t = setTimeout('showTime()', 1000)</p>
<p></p>
<p>                }</p>
