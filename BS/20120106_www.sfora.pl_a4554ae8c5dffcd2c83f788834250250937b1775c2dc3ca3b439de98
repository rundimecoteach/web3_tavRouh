<p>				getCookie = function(name) {</p>
<p>					var cookie, cookieValue, cookies, i;</p>
<p>					cookieValue = null;</p>
<p>					if (document.cookie && document.cookie !== '') {</p>
<p>						cookies = document.cookie.split(';');</p>
<p>						i = 0;</p>
<p>						while (i < cookies.length) {</p>
<p>							cookie = cookies[i].replace(/^\s\s*/, '').replace(/\s\s*$/, '');</p>
<p>							if (cookie.substring(0, name.length + 1) === (name + '=')) {</p>
<p>								cookieValue = decodeURIComponent(cookie.substring(name.length + 1));</p>
<p>								break;</p>
<p>							}</p>
<p>							i++;</p>
<p>						}</p>
<p>					}</p>
<p>					return cookieValue;</p>
<p>				};</p>
