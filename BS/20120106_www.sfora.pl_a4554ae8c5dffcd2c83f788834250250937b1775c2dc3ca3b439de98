<p>function setCookie(c_name,value,exdays)</p>
<p>{</p>
<p>    var exdate=new Date();</p>
<p>    exdate.setDate(exdate.getDate() + exdays);</p>
<p>    var c_value=escape(value) + ((exdays==null) ? "" : "; expires="+exdate.toUTCString());</p>
<p>    document.cookie=c_name + "=" + c_value;</p>
<p>}</p>
