<p>var puffLinks = $('*[type=puff] a');</p>
<p>//Add click event</p>
<p>puffLinks.click(function(){</p>
<p>	//get object id</p>
<p>	var parentEl = $(this);</p>
<p>	var objectId = "";</p>
<p>	var containerName = "";</p>
<p>	</p>
<p>	//We want to find the content object on the page.</p>
<p>	do {</p>
<p>		parentEl = parentEl.parent();</p>
<p>	} while (parentEl && </p>
<p>			!parentEl.attr('tagName').toLowerCase().match(/(tm:)?contentobject/));</p>
<p></p>
<p>	if (!parentEl) {</p>
<p>		tm.logger.error(['Puff Tracking:', 'No parent tm:contentobject tag'], OMNI_DEBUG_LEVEL);</p>
<p>		return !tm.logger.enabled; //if debug mode don't follow link</p>
<p>	} else {</p>
<p>		objectId = parentEl.attr('objectid');</p>
<p>	}</p>
<p>	</p>
<p>	// So we now have the content object we are looking for - we continue on up the tree</p>
<p>	// to get the name of the container holding it.</p>
<p>	do {</p>
<p>		parentEl = parentEl.parent();</p>
<p>	} while (parentEl && </p>
<p>			!parentEl.attr('tagName').toLowerCase().match(/(tm:)?container/) );</p>
<p></p>
<p>	if (!parentEl) {</p>
<p>		tm.logger.error(['Puff Tracking:', 'no parent tm:container tag'], OMNI_DEBUG_LEVEL);</p>
<p>		return !tm.logger.enabled; //if debug mode don't follow link</p>
<p>	} else {</p>
<p>		containerName = parentEl.attr('name');</p>
<p>	}</p>
<p></p>
<p>	tm.logger.debug(['Puff Tracking:', 'objectId:', objectId, </p>
<p>			'containerName:', containerName, parentEl ], OMNI_DEBUG_LEVEL);</p>
<p></p>
<p>	s.eVar13 = s.pageName + '-' + containerName + '-' + objectId;</p>
<p>	s.events = 'event17';</p>
<p>	s.t();</p>
<p>	</p>
<p>	tm.logger.info(['Puff Tracking:', 'Omniture event sent' ], OMNI_DEBUG_LEVEL);</p>
<p></p>
<p>	return tm.logger.debugLevel != OMNI_DEBUG_LEVEL; //if debug mode don't follow link</p>
<p>});</p>
<p>//Log to console if required.</p>
<p>tm.logger.debug( [ 'Puff Tracking: Links', puffLinks ], OMNI_DEBUG_LEVEL );</p>
