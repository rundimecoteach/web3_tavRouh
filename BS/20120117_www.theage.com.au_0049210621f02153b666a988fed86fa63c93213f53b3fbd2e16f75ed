<p>    function getGoogleAdChannel(showAboveArticle) {</p>
<p>        var googleAdChannel = "LifeStyle";</p>
<p>        if (showAboveArticle) {</p>
<p>            googleAdChannel += " abovearticle";</p>
<p>        }</p>
<p>        return googleAdChannel;</p>
<p>    }</p>
<p>    function google_ad_request_done(google_ads) {</p>
<p>        var googleAd = new FD.GoogleAd({</p>
<p>                adBlockTitle: "Ads by Google",</p>
<p>                maximumAdsInBlock: 3,</p>
<p>                checkExternalReferrer: true,</p>
<p>                split: 1,</p>
<p>            googleAds: google_ads,</p>
<p>            googleAdsId: "googleAds",</p>
<p>            moreGoogleAdsId: "moreGoogleAds"</p>
<p>        });</p>
<p>        googleAd.insertGoogleAds();</p>
<p>    }</p>
<p>    var google_ad_client = "ca-fairfax-age_js";</p>
<p>    var google_ad_channel = "National";</p>
<p>        if (!FD.isInternalReferrer()) {</p>
