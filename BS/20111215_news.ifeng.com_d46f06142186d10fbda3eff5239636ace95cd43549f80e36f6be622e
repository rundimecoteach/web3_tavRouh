<p>function zoomDoc(size) {</p>
<p>    if(size == 14) {$('small').className="smallsize";    $('big').className="bigsize"; } </p>
<p>    else {$('small').className="bigsize";    $('big').className="smallsize"; }</p>
<p>    $('artical_real').style.fontSize = size + 'px';</p>
<p>    var __childs = $('artical_real').childNodes;</p>
<p>    for (var i = 0; i < __childs.length; i++) { if (__childs[i].nodeType == 1) {  __childs[i].style.fontSize = size + 'px'; } }</p>
<p>}</p>
