<p>function getRalateUid(aUrl){</p>
<p>	var _dUrl = decodeURIComponent(aUrl);</p>
<p>	var _ralateUid = "2615417307";</p>
<p>	var _arr = [</p>
<p>		["http://v.ifeng.com","1806128454"],</p>
<p>		["http://culture.ifeng.com","1894132677"],</p>
