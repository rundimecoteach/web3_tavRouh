<p>(function(){</p>
<p>var _el = document.getElementById('Article'+GEL.thepage.pageinfo.key); // first look for the current article</p>
<p>if (!_el) { // if not current article, find last article</p>
<p>  try {</p>
<p>    var _lis = document.getElementById('ody-rightrail-moreheadlines-scroll-list').getElementsByTagName('li');</p>
<p>    _el = _lis[_lis.length - 1];</p>
<p>  } catch (ex) {}</p>
<p>}</p>
<p>if (_el)  { _el.parentNode.removeChild(_el); } // remove article, if found</p>
<p>// apply class to remove dotted line from last child</p>
<p>try {</p>
<p>  var _lis = document.getElementById('ody-rightrail-moreheadlines-scroll-list').getElementsByTagName('li');</p>
<p>  _el = _lis[_lis.length - 1];</p>
<p>  _el.className+=" last";</p>
<p>} catch (ex) {}</p>
<p>})(); </p>
