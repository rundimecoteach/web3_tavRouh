<p>function setFavorite(value, domain){</p>
<p>	var fav = value;</p>
<p>	var len = fav.length;</p>
<p>	//alert(city);</p>
<p>	if(fav.indexOf("http://") == 0){</p>
<p>		fav = fav.substring(7,len);</p>
<p>	}</p>
<p>	if(fav.indexOf("https://") == 0){</p>
<p>		fav = fav.substring(8,len);</p>
<p>	}</p>
<p></p>
<p>	addToCookie("favorites",fav,domain);</p>
<p>}</p>
