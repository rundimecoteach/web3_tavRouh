<p>		if (val==def || val=='') {</p>
<p>			alert ("Please enter a comment");</p>
<p>			return false;</p>
<p>		}</p>
<p>		var btn = document.getElementById("postCommentBtn");</p>
<p>		btn.disabled=true;</p>
<p>		btn.textContent='Sending...';</p>
<p>		</p>
<p>		var myJSONObject = {"body": val, "action": "postcomment", "submitted": "true", "ajax": "true", "contentid": "ContentId(1.4135247)"};</p>
<p>		var postMessage = "<br /><b>Your comment has been posted.</b>";</p>
<p>		</p>
<p>		jQuery.post("http://www.jarrowandhebburngazette.com/news/health/shopping_for_way_to_break_cigs_habit_1_4135247", myJSONObject, function(result){</p>
<p></p>
<p>			var htmlResult = $(result).find ('#commentsSection');</p>
<p>			$('#commentsSection').html(htmlResult);</p>
<p>			$('#ajaxPost').html(postMessage);</p>
