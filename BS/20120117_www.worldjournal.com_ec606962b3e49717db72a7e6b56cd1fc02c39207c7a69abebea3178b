<p>  function updateChars(dom_id, remaining_chars_dom_id, max_length) {</p>
<p></p>
<p>    var message_text = $(dom_id).value;</p>
<p>    var remaining_characters = max_length - message_text.length;</p>
<p>    if (remaining_characters < 0) {</p>
<p>      message_text = message_text.substring(0, max_length);</p>
<p>      $(dom_id).value = message_text;</p>
<p>      remaining_characters = max_length - message_text.length;</p>
<p>    }</p>
<p></p>
<p>    $(remaining_chars_dom_id).innerHTML = remaining_characters;</p>
<p>  }</p>
