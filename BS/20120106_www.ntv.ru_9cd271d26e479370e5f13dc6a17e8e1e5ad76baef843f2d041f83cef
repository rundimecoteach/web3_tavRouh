<p>var serverTimeZ = 1365576955;</p>
<p>var localDate = new Date();</p>
<p></p>
<p>function setLocalTime() {</p>
<p>  localDate.setTime(serverTimeZ*1000);</p>
<p>  var moscowDate = new Date(localDate.getUTCFullYear(), localDate.getUTCMonth(), localDate.getUTCDate(),  </p>
