<p>          function pokazUkryjModul() {</p>
<p>            var pokazOd = 480,</p>
<p>                idModul = 182,</p>
<p>                szerokoscOkna = jQuery(window).width(),</p>
<p>                modul = jQuery('#modul_asynchroniczny_' + idModul);</p>
<p>            if (szerokoscOkna >= pokazOd && !modul.is(":visible")) {</p>
<p>              modul.show();</p>
<p>            } else if (szerokoscOkna < pokazOd && modul.is(":visible")) {</p>
<p>              modul.hide();</p>
<p>            }</p>
<p>          }</p>
<p>          pokazUkryjModul();</p>
<p>          jQuery(window).resize(function () {</p>
<p>            clearTimeout(resizeTimeout);</p>
<p>            resizeTimeout = setTimeout(function () {</p>
<p>              pokazUkryjModul();</p>
<p>            }, 200);</p>
<p>          });</p>
