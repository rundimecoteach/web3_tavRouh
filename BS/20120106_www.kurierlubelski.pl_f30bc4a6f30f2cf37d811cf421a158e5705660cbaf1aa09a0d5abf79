<p></p>
<p>(function ($) {</p>
<p>    var przycisk = $('a.tooltipZamknij');</p>
<p>    przycisk.click(function(event) {</p>
<p>        event.preventDefault();</p>
<p>        $('.cookieTooltipKontener').hide();</p>
<p>        var data = new Date();</p>
<p>        data.setDate(data.getDate() + 365);</p>
<p>        data = data.toUTCString();</p>
<p>        document.cookie = 'polityka_prywatnosci_cookies=true; expires=' + data + '; path = /';</p>
<p>    });</p>
<p>})(jQuery);</p>
<p></p>
<p></p>
<p></p>
<p>    oG_OSA.onclick_id = 'SA';</p>
<p>  GR('oTrackEvents', oG_OSA.tracking);</p>
