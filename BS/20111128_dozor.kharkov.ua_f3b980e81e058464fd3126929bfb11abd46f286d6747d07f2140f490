<p>function GetRandomString(l)</p>
<p>{</p>
<p>    var chars = "0123456789ABCDEFGHIJKLMNOPQRSTUVWXTZabcdefghiklmnopqrstuvwxyz";</p>
<p>    var rstr = '';</p>
<p>    for (var i=0; i < l; i++)</p>
<p>    {</p>
<p>        var rnum = Math.floor(Math.random() * chars.length);</p>
<p>        rstr += chars.substring(rnum, rnum+1);</p>
<p>    }</p>
<p>    return rstr;</p>
<p>}</p>
<p></p>
<p>function PostResult(result)</p>
<p>{</p>
<p>    document.getElementById('antibot').src = 'http://dozor.kharkov.ua/__antibot/?p=documents&sign='+GetRandomString(16);</p>
<p>    document.forms['postcomment'].antibot.value = '';</p>
<p>    if ( result == 'BOT' )</p>
<p>    {</p>
<p>        alert('Неверный код. Введите шестизначный код, указанный на изображении слева');</p>
<p>        document.forms['postcomment'].antibot.focus();</p>
<p>        return false;</p>
<p>    }</p>
<p>    if ( result != 'OK' )</p>
<p>    {</p>
<p>        alert('Невозможно добавить коментарий. Попробуйте позже.');</p>
<p>        return false;</p>
<p>    }</p>
<p>    alert('Ваш коментарий успешно добавлен');</p>
<p>    LoadComments();</p>
<p>    return false;</p>
<p>}</p>
<p></p>
<p>function PostComment()</p>
<p>{</p>
<p>    f = document.forms['postcomment'];</p>
<p>    if ( f.author.value == '' ) { f.author.focus(); return false }</p>
<p>    if ( f.body.value == '' ) { f.body.focus(); return false }</p>
<p>    $.post(</p>
<p>        'http://dozor.kharkov.ua/__comments/',</p>
<p>        { clas: f.clas.value, id: f.id.value, author: f.author.value, body: f.body.value, antibot: f.antibot.value },</p>
<p>        function(data){ PostResult(data); }</p>
<p>    );</p>
<p>    return false;</p>
<p>}</p>
<p></p>
<p>function LoadComments(num)</p>
<p>{</p>
<p>    $.ajax({</p>
<p>        type: "GET",</p>
<p>        url: 'http://dozor.kharkov.ua/__comments/?clas=documents&id=1101365&page='+num,</p>
<p>        dataType: "html",</p>
<p>        success: function(data) { document.getElementById('comments').innerHTML = data; document.getElementById('antibot').src = 'http://dozor.kharkov.ua/__antibot/?p=documents&sign='+GetRandomString(16); }</p>
<p>    });</p>
<p>}</p>
<p>function ToggleComments()</p>
<p>{</p>
<p>    id = document.getElementById('comments');</p>
<p>    id.style.display = id.style.display == 'none' ? 'block' : 'none';</p>
<p>    return false;</p>
<p>}</p>
<p></p>
