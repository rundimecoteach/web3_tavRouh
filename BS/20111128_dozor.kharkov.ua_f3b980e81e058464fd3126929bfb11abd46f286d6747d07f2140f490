<p>function PostRating(r)</p>
<p>{</p>
<p>    document.getElementById('star-rating').innerHTML = '<img src="http://dozor.kharkov.ua/images/spinner_mac.gif" border="0px" alt="">';</p>
<p>    $.post(</p>
<p>        'http://dozor.kharkov.ua/__ratings/',</p>
<p>        { clas: 'documents', id: '1101365', compid: GetClientID(), rating: r },</p>
<p>        function(data)</p>
<p>        {</p>
<p>            val = Math.round(((0+parseInt(data))/1)*20);</p>
<p>            document.getElementById('ratings').innerHTML =</p>
<p>            '<ul class="star-rating">'+</p>
<p>                '<li class="current-rating" style="width: '+val+'%;"></li>'+</p>
<p>            '</ul>'+</p>
<p>            '<p style="float: left; margin: 0; padding: 8px 5px 0 0; color: #666">'+</p>
<p>                '<b>Рейтинг:<b> '+val/20+' (оценок: 1)'+</p>
<p>            '</p>';</p>
<p>        }</p>
<p>    );</p>
<p>    return false;</p>
<p>}</p>
