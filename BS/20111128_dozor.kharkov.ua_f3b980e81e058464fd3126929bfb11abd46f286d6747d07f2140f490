<p>                            function AjaxCalendar(year, month)</p>
<p>                            {</p>
<p>                                if ( month == 0 ) { month = 12; year--; }</p>
<p>                                else if ( month == 13 ) { month = 1; year++; }</p>
<p>                                $.ajax({</p>
<p>                                    type: "GET",</p>
<p>                                    url: 'http://dozor.kharkov.ua/ajaxcalendar.html?calendar_year='+year+'&calendar_month='+month,</p>
<p>                                    dataType: "html",</p>
<p>                                    success: function(data) { document.getElementById('calBody').innerHTML = data;}</p>
<p>                                });</p>
<p>                                return false;</p>
<p>                            }</p>
