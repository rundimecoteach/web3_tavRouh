<p>                                        var cur_menu = '1';</p>
<p>                    var base_menu = '1';</p>
<p>                    var red_menu = 65;</p>
<p>                    var tid = '';</p>
<p></p>
<p>                    function SetMenuTimeout(id)</p>
<p>                    {</p>
<p>                        tid = id;</p>
<p>                        setTimeout( function(id) { return function(){ ShowMenu(id); } }(id), 500);</p>
<p>                        return false;</p>
<p>                    }</p>
<p></p>
<p>                    function ShowMenu(id)</p>
<p>                    {</p>
<p>                        if ( tid != id ) return false;</p>
<p>                        if ( cur_menu != '' ) document.getElementById('menu_'+cur_menu).childNodes[0].childNodes[0].className = base_menu == cur_menu ? 'ac' : ( red_menu == cur_menu ? 'red-ac' : '');</p>
<p>                        if ( cur_menu != '' && document.getElementById('sub'+cur_menu) != undefined ) document.getElementById('sub'+cur_menu).style.display = 'none';</p>
<p>                        if ( document.getElementById('sub'+id) != undefined ) document.getElementById('sub'+id).style.display = 'block';</p>
<p>                        cur_menu = id;</p>
<p>                        return false;</p>
<p>                    }</p>
<p></p>
<p>                    function HideMenu(id)</p>
<p>                    {</p>
<p>                        //document.getElementById('sub'+id).style.display = 'none';</p>
<p>                        document.getElementById('menu_'+cur_menu).childNodes[0].childNodes[0].className = 'menuac';</p>
<p>                        tid = '';</p>
<p>                        return false;</p>
<p>                    }</p>
