<p>				var sPath= location.href;</p>
<p>				var aPath = sPath.split('//');</p>
<p></p>
<p>				if(aPath[1].length>0)</p>
<p>				{</p>
<p>				var aDomainDet = aPath[1].split('/');</p>
<p>				var sXmlPath="";</p>
<p>				for (var i=1;i<aDomainDet.length;i++)</p>
<p>				{</p>
<p>				if(i>0 && i<aDomainDet.length-1)</p>
<p>				{</p>
<p>				sXmlPath+=aDomainDet[i]+'/';</p>
<p>				}else if(i>0)</p>
<p>				{</p>
<p>				sXmlPath+=aDomainDet[i].replace('.htm','.xml');</p>
<p>				}</p>
<p>				}</p>
<p>				}</p>
