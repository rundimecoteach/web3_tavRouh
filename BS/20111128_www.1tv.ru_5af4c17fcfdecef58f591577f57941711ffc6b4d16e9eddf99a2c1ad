<p>$(document).ready(function() {</p>
<p>                $("#vote_shed").ajaxForm( {beforeSubmit:vote_shedValidate, target:  "#vote_rez", success: hide_vote} );});</p>
<p>                function getRadioGroupValue(radioGroupObj)</p>
<p>                {</p>
<p>                  for (var i=0; i < radioGroupObj.length; i++)</p>
<p>                    if (radioGroupObj[i].checked) return radioGroupObj[i].value;</p>
<p>                </p>
<p>                  return null;</p>
<p>                }</p>
<p>                function vote_shedValidate(formData, jqForm, options) {</p>
<p>                  var form = jqForm[0];</p>
<p>                  if (!getRadioGroupValue(form.radio01)) {</p>
<p>                    alert("Выберите ответ.");</p>
<p>                    return false;</p>
<p>                  }</p>
<p>                  return true;</p>
<p>                }</p>
<p>                function hide_vote(){</p>
<p>                  $("#vote_div").hide();</p>
<p>                  $("#vote_rez").show();</p>
<p>                }</p>
<p>                </p>
