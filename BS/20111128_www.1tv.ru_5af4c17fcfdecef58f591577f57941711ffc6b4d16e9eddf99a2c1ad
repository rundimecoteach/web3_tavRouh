<p>$(document).ready(function() {</p>
<p>         	$('#tabed1').bind('click', function(e){</p>
<p>         		$(".tab1").show().removeClass("none");</p>
<p>         		$(".tab2").hide().addClass("none");</p>
<p>         		$(".tab3").hide().addClass("none");</p>
<p>         		$(".tab4").hide().addClass("none");</p>
<p>         		$('#tabed1').addClass("selected");</p>
<p>         		$('#tabed2').removeClass("selected");</p>
<p>         		$('#tabed3').removeClass("selected");</p>
<p>         		$('#tabed4').removeClass("selected");</p>
<p>         		e.preventDefault();</p>
<p>         	});</p>
<p>         	$('#tabed2').bind('click', function(e){</p>
<p>         		$(".tab1").hide().addClass("none");</p>
<p>         		$(".tab2").show().removeClass("none");</p>
<p>         		$(".tab3").hide().addClass("none");</p>
<p>         		$(".tab4").hide().addClass("none");</p>
<p>         		$('#tabed2').addClass("selected");</p>
<p>         		$('#tabed1').removeClass("selected");</p>
<p>         		$('#tabed3').removeClass("selected");</p>
<p>         		$('#tabed4').removeClass("selected");</p>
<p>         		e.preventDefault();</p>
<p>         	});</p>
<p>         	$('#tabed3').bind('click', function(e){</p>
<p>         		$(".tab1").hide().addClass("none");</p>
<p>         		$(".tab2").hide().addClass("none");</p>
<p>         		$(".tab3").show().removeClass("none");</p>
