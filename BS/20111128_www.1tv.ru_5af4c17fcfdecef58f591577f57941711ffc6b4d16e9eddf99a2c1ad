<p>             	$(function(){</p>
<p>             	 setPageUrls();	</p>
<p>                 function setPageUrls() {</p>
<p>                   $("#list_abc_search div.all_pagination a").click(function() {</p>
<p>                     $.get($(this).attr("href"), function(data){</p>
<p>                       $("#list_abc_search").empty().prepend(data);</p>
<p>             //          window.scroll(0,190);</p>
<p>                       setPageUrls();</p>
<p>                     });</p>
<p>                     return false;</p>
<p>                   });</p>
<p>                 }</p>
<p>                  }</p>
<p>                  );</p>
