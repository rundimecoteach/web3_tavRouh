<p>var IfengAdCookie = {</p>
<p>author:"JingWu",</p>
<p>version:"0.0.1",</p>
<p>tmp:{},</p>
<p>set:function(param) {</p>
<p>if(typeof(param) === 'undefined' || typeof(param.name) === 'undefined' || typeof(param.value) === 'undefined') {</p>
<p>alert('参数有误，请返回修改! ex:{\'name\':,\'value\':,\'time\':}');</p>
<p>return false;</p>
<p>}</p>
<p>if(typeof(param.time) === 'undefined') {</p>
<p>var now = new Date();</p>
<p>var date = new Date(now.getFullYear(), (now.getMonth() + 1), now.getDate(), 23, 59, 59);</p>
<p>} else {</p>
<p>var date = new Date(param.time);</p>
<p>}</p>
<p></p>
<p>document.cookie = param.name + "=" + escape(param.value) + ";path=/;expires=" + date.toGMTString() + ";"</p>
<p>},</p>
<p>get:function(param) {</p>
<p>if(typeof(param) === 'undefined' || typeof(param.name) === 'undefined') {</p>
<p>alert('参数有误，请返回修改! ex:{\'name\':}');</p>
<p>return false;</p>
<p>}</p>
<p>var c = document.cookie.split("; ");</p>
<p>for(var i = 0; i < c.length; i ++) {</p>
<p>var d = c[i].split("=");</p>
<p>if(d[0] == param.name) {</p>
<p>return unescape(d[1]);</p>
<p>}</p>
<p>}</p>
<p>return "";</p>
<p>}</p>
<p>};</p>
<p></p>
<p>var IfengAdAlternate = {</p>
<p>version:'0.0.1',</p>
<p>author:'JingWu',</p>
<p>tmp:{},</p>
<p>stock:{},</p>
<p>play:function() {</p>
<p>if(this.checkParam()) {</p>
<p>} else {</p>
<p>return false;</p>
<p>}</p>
<p>this.init_stock();</p>
<p>this.install();</p>
<p>},</p>
<p>checkParam:function() {</p>
<p>var param = this.stock[this.tmp.id];</p>
<p>var len = this.stock[this.tmp.id].length;</p>
<p>for(var i=0; i<len; i++) {</p>
<p>if(typeof(param[i]) == 'undefined' ) {</p>
<p>continue;</p>
<p>}</p>
<p>if( param[i].length == 4 ){</p>
<p>if( (param[i][0] === '') ) {</p>
<p>alert('广告代码参数有误，在第 ' + i + ' 个广告，第 1 个参数');</p>
<p>return false;</p>
<p>}</p>
<p>if( param[i][1] === '' || isNaN(param[i][1]) ) {</p>
<p>alert('广告代码参数有误，在第 ' + i + ' 个广告，第 2 个参数');</p>
<p>return false;</p>
<p>}</p>
<p>if( param[i][2] === true || param[i][2] === false ) {</p>
<p>} else {</p>
<p>alert('广告代码参数有误，在第 ' + i + ' 个广告，第 3 个参数');</p>
<p>return false;</p>
<p>}</p>
<p>if( param[i][3] === '' ) {</p>
<p>alert('广告代码参数有误，在第 ' + i + ' 个广告，第 4 个参数');</p>
<p>return false;</p>
<p>}</p>
<p>} else {</p>
<p>alert('第 ' + i + ' 个广告参数不全');</p>
<p>return false;</p>
<p>}</p>
<p>}</p>
<p>return true;</p>
<p>},</p>
<p>checkInstall:function() {</p>
<p>if(typeof(IfengAdAlternate.tmp.checkInstall) == 'undefined') {</p>
<p>IfengAdAlternate.tmp.checkInstall = 0;</p>
<p>}</p>
<p>document.write('<scr'+'ipt language="javascript" type="text/javascript" src="http://img.ifeng.com/tres/html/birangtest/checkInstall.js\">'+'</'+'script>');</p>
<p>},</p>
<p>install:function() {</p>
<p>IfengAdAlternate.tmp.checkInstall = 0;</p>
<p>var current_ad = this.getSequence();</p>
<p>if(current_ad === 'complete') {</p>
<p>return true;</p>
<p>} else {</p>
<p>document.write(current_ad[0]);</p>
<p>this.tmp.current = current_ad; // 将当前播放的广告放入当前页面的临时变量</p>
<p>this.checkInstall();</p>
<p>}</p>
<p>},</p>
<p>installSucess:function() {</p>
<p>return true;</p>
<p>},</p>
<p>installFail:function() {</p>
<p>if(IfengAdAlternate.stock[IfengAdAlternate.tmp.id].length == 1) {</p>
<p>return true;</p>
<p>}</p>
<p>IfengAdAlternate.tmp.checkInstall = 0;</p>
<p>IfengAdAlternate.install();</p>
<p>},</p>
<p>cookie_flag:function() {</p>
<p>var flag_cookie = "";</p>
<p>var domain = document.domain.substr(0, (document.domain.length-10));</p>
<p></p>
<p>var href = document.location.href;</p>
<p>var start = href.lastIndexOf("/") + 1;</p>
<p>var end = href.lastIndexOf(".");</p>
<p>href = href.substr(start, end-start);</p>
<p></p>
<p>flag_cookie = domain + href;</p>
<p>return 'IfengAA_' + flag_cookie;</p>
<p>},</p>
<p>getSequence:function() {</p>
<p>var len_current_level = 0;</p>
<p>var cookie_arr = new Array();</p>
<p>var next_level = 0;</p>
<p>var level = 0;</p>
<p></p>
<p>IfengAdAlternate.tmp.complete = IfengAdAlternate.tmp.complete + 1;</p>
<p>// 获取 cookie </p>
<p>var cookieFlag = IfengAdAlternate.cookie_flag();</p>
<p>var cookie = IfengAdCookie.get({'name': cookieFlag + IfengAdAlternate.tmp.id});</p>
<p></p>
<p>// 判断需要播放的广告</p>
<p>if(cookie == '') {</p>
<p>len_current_level = IfengAdAlternate.tmp.level_list[IfengAdAlternate.tmp.level_arr[0]].length;</p>
<p>if(len_current_level == 1) { // 只有一个广告</p>
<p>cookie_arr = [0, 0, 1];</p>
<p>} else {</p>
<p>cookie_arr = [0, IfengAdAlternate.random(len_current_level), 1];</p>
<p>}</p>
<p>} else {</p>
<p>cookie_arr = cookie.split('_');</p>
<p>level = IfengAdAlternate.tmp.level_arr[cookie_arr[0]];</p>
<p>len_current_level = IfengAdAlternate.tmp.level_list[level].length;</p>
<p>if(cookie_arr[2] >= len_current_level) {</p>
<p>// 当前级别广告是否播放完成</p>
<p>if(cookie_arr[0] >= IfengAdAlternate.tmp.level_arr.length-1) {</p>
<p>// 广告轮播完成，重新开始轮播</p>
<p>len_first_level = IfengAdAlternate.tmp.level_list[IfengAdAlternate.tmp.level_arr[0]].length;</p>
<p>if(len_first_level == 1) { // 只有一个广告</p>
<p>cookie_arr = [0, 0, 1];</p>
<p>} else {</p>
<p>cookie_arr = [0, IfengAdAlternate.random(len_first_level), 1];</p>
<p>}</p>
<p>} else {</p>
<p>// 播放下一级别广告</p>
<p>next_level = IfengAdAlternate.tmp.level_arr[parseInt(cookie_arr[0])+1];</p>
<p>len_next_level = IfengAdAlternate.tmp.level_list[next_level].length;</p>
<p>if(len_next_level == 1) { // 只有一个广告</p>
<p>cookie_arr = [parseInt(cookie_arr[0])+1, 0, 1];</p>
<p>} else {</p>
<p>cookie_arr = [parseInt(cookie_arr[0])+1, IfengAdAlternate.random(len_next_level), 1];</p>
<p>}</p>
<p>}</p>
<p>} else {</p>
<p>if(cookie_arr[1] >= len_current_level-1) {</p>
<p>cookie_arr = [cookie_arr[0], 0, parseInt(cookie_arr[2])+1];</p>
<p>} else {</p>
<p>cookie_arr = [cookie_arr[0], parseInt(cookie_arr[1]) + 1, parseInt(cookie_arr[2])+1];</p>
<p>}</p>
<p>}</p>
<p>}</p>
<p>IfengAdCookie.set({'name':cookieFlag + IfengAdAlternate.tmp.id, 'value': cookie_arr[0] + '_' + cookie_arr[1] + '_' + cookie_arr[2]});</p>
<p>if(IfengAdAlternate.tmp.complete >= 10) {</p>
<p>return 'complete';</p>
<p>} else {</p>
<p>level = this.tmp.level_arr[cookie_arr[0]];</p>
<p>return this.tmp.level_list[level][cookie_arr[1]];</p>
<p>}</p>
<p>},</p>
<p>random:function(len) {</p>
<p>return (parseInt(Math.random()*100000) % len);</p>
<p>},</p>
<p>init_stock:function() {</p>
<p>var list = this.stock[this.tmp.id];</p>
<p>var len = list.length;</p>
<p>var len_list=0;</p>
<p>var level=0;</p>
<p>var level_arr = new Array();</p>
<p>var level_list = new Array();</p>
<p></p>
<p>// 序列存储对原始数据的要求为：按等级顺序排列</p>
<p>for(var i=0; i<len; i++) {</p>
<p>level = list[i][1]; // 获取广告级别</p>
<p>if(level_arr[level_arr.length-1] == level) { // 广告级别已存在，将广告放入同级别数组中</p>
<p>len_list = level_list[level].length;</p>
<p>level_list[level][len_list] = list[i];</p>
<p>} else { // 新的广告级别</p>
<p>level_arr[level_arr.length] = level;</p>
<p>level_list[level] = [list[i]];</p>
<p>}</p>
<p>}</p>
<p>IfengAdAlternate.tmp.level_arr = level_arr;</p>
<p>IfengAdAlternate.tmp.level_list = level_list;</p>
<p>},</p>
<p>init:function(param) {</p>
<p>this.tmp.id = param.id;</p>
<p>this.tmp.complete = 0;</p>
<p>this.stock[param.id] = param.list;</p>
<p>this.play();</p>
<p>}</p>
<p>};</p>
<p></p>
