<p>(function(iYear,iMonth,sBlock)</p>
<p>{</p>
<p>	var iCYear=parseInt(iYear,10);</p>
<p>	var iCMonth=parseInt(iMonth,10);</p>
<p>	var sCBlock=sBlock;</p>
<p>	var fUpdate=function(iYear,iMonth)</p>
<p>	{</p>
<p>		$.getJSON("http:\/\/news.day.az\/"+"archive/calendar.jsp?jsoncallback=?",{</p>
<p>			"m": iMonth,</p>
<p>			"y": iYear</p>
<p>		},function(json){</p>
<p>			$(sCBlock).replaceWith(json.html);</p>
<p>			if(typeof(deferredLoader)!="undefined")</p>
<p>				deferredLoader.run();</p>
<p>		});</p>
<p>		return false;</p>
<p>	};</p>
<p>	var fn=function()</p>
<p>	{</p>
<p>		$("a[name=pmonth]").click(function(){ return fUpdate(iCMonth==1?(iCYear-1):iCYear,iCMonth==1?12:(iCMonth-1)); });</p>
<p>		$("a[name=nmonth]").click(function(){ return fUpdate(iCMonth==12?(iCYear+1):iCYear,iCMonth==12?1:(iCMonth+1)); });</p>
<p>		$("a[name=pyear]").click(function(){ return fUpdate(iCYear-1,iCMonth); });</p>
<p>		$("a[name=nyear]").click(function(){ return fUpdate(iCYear+1,iCMonth); });</p>
