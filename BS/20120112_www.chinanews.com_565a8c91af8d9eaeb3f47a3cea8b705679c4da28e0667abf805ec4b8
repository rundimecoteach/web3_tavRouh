<p>Array.prototype.inArray = function (value)</p>
<p>{</p>
<p>var i;</p>
<p>for (i=0; i < this.length; i++) {</p>
<p>if (this[i] === value) {</p>
<p>return true;</p>
<p>}</p>
<p>}</p>
<p>return false;</p>
<p>};</p>
<p></p>
<p>var web</p>
<p>function bshareApi(web){</p>
<p>var platform =Array("sinaminiblog","qqmb","qzone","qqxiaoyou","kaixin001");</p>
<p></p>
<p>if(platform.inArray(web)){</p>
<p>var targetUrl="http://api.bshare.cn/share/"+web+"?url="+encodeURIComponent(document.location.href)+"&title="+encodeURIComponent(document.title)+"&publisherUuid=bdd81480-47aa-4432-a9cb-9b7ef9f7523f&pic=http://www.chinanews.com//shipin/2012/01-12/U405P883T4D50991F158DT20120112163627.jpg&video=http://i2.chinanews.com/shipin/flv/video2010V1.0.1.swf?vInfo=http://video.chinanews.com/tvmining/News/MP4ZXW/ShanDongTV/2012/01/12/ShanDongTV_20120112_072109129_706707_129271.mp4 ";</p>
<p> window.open(targetUrl);</p>
<p> }else{</p>
<p> alert('plat not support!');</p>
<p> }</p>
<p>}</p>
