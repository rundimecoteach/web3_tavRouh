<p></p>
<p>var timerlen = 5;</p>
<p>var slideAniLen = 300;</p>
<p>var pluginDivsHeight = {};</p>
<p></p>
<p>// plugin v2 class</p>
<p>function PluginV2 (id, num) {</p>
<p>	this.id = id;</p>
<p>	this.num = num;</p>
<p>	this.parent_id = 'parent_' + id;</p>
<p>}</p>
<p></p>
<p>PluginV2.prototype = {</p>
<p>	GA_account: 'UA-3119939-34',</p>
<p>	num: 0, // real number of the plugin</p>
<p>	id: '',	// id of the div</p>
<p>	parent_id: '',	// id of the parent div</p>
<p>	moving: false,	// true if it's already moving</p>
<p>	dir: null,	// direction of moving</p>
<p>	endLength: 0,	// length of opened div</p>
<p>	startTime: 0,	// start time</p>
<p>	timerID: 0, // timer id</p>
<p>	obj: null, // div to slide</p>
<p>	initialized: false, 	// whether plugins were initialized</p>
<p></p>
<p>	trackLink: function( domain, pluginName ){</p>
<p>		</p>
<p>		var pageTracker=_gat._getTracker(this.GA_account);    </p>
<p>		pageTracker._trackEvent('баян', domain, pluginName );</p>
<p>		return true;</p>
<p>	},</p>
<p>	</p>
<p>	// openes plugin</p>
<p>	open: function() {</p>
<p>		if (this.moving)</p>
<p>			return;</p>
<p></p>
<p>		if (document.getElementById(this.id).style.display != "none")</p>
<p>			return;</p>
<p></p>
<p>		this.addClass(this.parent_id, 'gb-informers_project_open');</p>
<p></p>
<p>		this.moving = true;</p>
<p>		this.dir = 'down';</p>
<p>		this.startSlide();</p>
<p>	},</p>
<p></p>
<p>	// closes plugin</p>
<p>	close: function() {</p>
<p>		if (this.moving)</p>
<p>			return;</p>
<p></p>
<p>		if (document.getElementById(this.id).style.display == "none")</p>
<p>			return;</p>
<p></p>
<p>		this.removeClass(this.parent_id, 'gb-informers_project_open');</p>
<p>		document.getElementById(this.id).style.overflow = 'hidden';</p>
<p></p>
<p>		this.moving = true;</p>
<p>		this.dir = 'up';</p>
<p>		this.startSlide();</p>
<p>	},</p>
<p></p>
<p>	// toggles plugin</p>
<p>	toggle: function() {</p>
<p>		if (!this.initialized) {</p>
<p>			return;</p>
<p>		}</p>
<p></p>
<p>		if (document.getElementById(this.id).style.display == "none") {</p>
<p>			// div is hidden, so let's open it</p>
<p>			this.open();</p>
<p>		}</p>
<p>		else {</p>
<p>			// div is visible, so close it</p>
<p>			this.close();</p>
<p>		}</p>
<p>	},</p>
<p></p>
<p>	isOpened: function() {</p>
<p>		return document.getElementById(this.id).style.display != "none";</p>
<p>	},</p>
<p></p>
<p>	// starts plugin slide</p>
<p>	startSlide: function() {</p>
<p>		this.obj = document.getElementById(this.id);</p>
<p>		this.endLength = pluginDivsHeight[this.obj.id];</p>
<p>		this.startTime = (new Date()).getTime();</p>
<p></p>
<p>		if (this.dir == "down")</p>
<p>			this.obj.style.height = "1px";</p>
<p></p>
<p>		this.obj.style.display = "block";</p>
<p></p>
<p>		var self = this;	// closure</p>
<p></p>
<p>		this.timerID = setInterval(function(){self.slideTick()}, timerlen);</p>
<p>	},</p>
<p></p>
<p>	/**</p>
<p>	*  	checks if it's time to end the slide or to keep sliding</p>
<p>	*/</p>
<p>	slideTick: function() {</p>
<p>		var elapsed = (new Date()).getTime() - this.startTime;</p>
<p></p>
<p>		if (elapsed > slideAniLen)</p>
<p>			this.endSlide();</p>
<p>		else {</p>
<p>			var d = Math.round(elapsed / slideAniLen * this.endLength);</p>
<p>			if(this.dir == "up")</p>
<p>				d = this.endLength - d;</p>
<p></p>
<p>			obj = this.obj;</p>
<p>			obj.style.height = d + "px";</p>
<p>		}</p>
<p></p>
<p>		return;</p>
<p>	},</p>
<p></p>
<p>	/*</p>
<p>   	*	ends sliding</p>
<p>   	*/</p>
<p>	endSlide: function () {</p>
<p>		clearInterval(this.timerID);</p>
<p></p>
<p>		if (this.dir == "up")</p>
<p>			this.obj.style.display = "none";</p>
<p></p>
<p>		this.obj.style.height = this.endLength + "px";</p>
<p></p>
<p>		delete(this.moving);</p>
<p>		delete(this.timerID);</p>
<p>		delete(this.startTime);</p>
<p>		delete(this.endLength);</p>
<p>		delete(this.obj);</p>
<p>		delete(this.dir);</p>
<p></p>
<p>		return;</p>
<p>	},</p>
<p></p>
<p>	// some utils</p>
<p></p>
<p>	hasClass: function(id, className) {</p>
<p>		var obj = document.getElementById(id);</p>
<p>		return obj.className.indexOf(className) != -1;</p>
<p>	},</p>
<p></p>
<p>	addClass: function(id, className) {</p>
<p>		var obj = document.getElementById(id);</p>
<p>		if (!this.hasClass(id, className)) obj.className += " " + className;</p>
<p>	},</p>
<p></p>
<p>	removeClass: function(id, className) {</p>
<p>		var obj = document.getElementById(id);</p>
<p>		if (this.hasClass(id, className)) {</p>
<p>			var st = obj.className;</p>
<p>    		obj.className = st.replace(' gb-informers_project_open', '');</p>
<p>		}</p>
<p>	},</p>
<p></p>
<p>	getElementCurrentStyle: function(element, CssStyleProperty) {</p>
<p>		if (!element) return false;</p>
<p>		if (element.currentStyle) {</p>
<p>			return element.currentStyle[this.cssToJavascriptStyle(CssStyleProperty)];</p>
<p>		}</p>
<p>		else if (window.getComputedStyle) {</p>
<p>			var compStyle = window.getComputedStyle(element, '');</p>
<p>			return compStyle.getPropertyValue(CssStyleProperty);</p>
<p>		}</p>
<p>		else {</p>
<p>			return '';</p>
<p>		}</p>
<p>	},</p>
<p></p>
<p>	cssToJavascriptStyle: function(CssProperty) {</p>
<p>		var stringArray = CssProperty.toLowerCase().split('-');</p>
<p>		if (stringArray.length == 1) {</p>
<p>			return stringArray[0];</p>
<p>		}</p>
<p>		var ret = (CssProperty.indexOf("-") == 0) ? stringArray[0].charAt(0).toUpperCase() + stringArray[0].substring(1) : stringArray[0];</p>
<p>		for (var i = 1; i < stringArray.length; i++) {</p>
<p>			var s = stringArray[i];</p>
<p>			ret += s.charAt(0).toUpperCase() + s.substring(1);</p>
<p>		}</p>
<p>		return ret;</p>
<p>	},</p>
<p></p>
<p>	addGoogleAnalytics: function () {</p>
<p></p>
<p>//		var pluginDiv = document.getElementById(this.parent_id);</p>
<p>//		var children = pluginDiv.getElementsByTagName('a');</p>
<p>//		var parameters = pluginDiv.getAttribute('rel');</p>
<p>//</p>
<p>//		for (var i = 0; i < children.length; i++) {</p>
<p>//			var tagName = children[i].tagName.toLowerCase();</p>
<p>//			children[i].href = children[i].href + parameters;</p>
<p>//		}</p>
<p>		</p>
<p>	},</p>
<p></p>
<p>	init: function() {</p>
<p>    </p>
<p>		var checked = false;</p>
<p>		if (pluginTicksCounter++ > 15) {</p>
<p>			checked = true;</p>
<p>		}</p>
<p></p>
<p>		var rootDiv = document.getElementById('gb_informers');</p>
<p>		var pluginDivs = rootDiv.getElementsByTagName("div");</p>
<p>		var photoChecked = true;</p>
<p></p>
<p>		if (document.getElementById('gb-informers_photo_box') != null) {</p>
<p>			photoChecked = false;</p>
<p>		}</p>
<p></p>
<p>		for (var i = 0; i < pluginDivs.length; i++) {</p>
<p></p>
<p>			// check whether the style is applied already</p>
<p>			if (!checked && pluginDivs[i].className == 'gb-informers_project_content') {</p>
<p></p>
<p>				if (!photoChecked) {</p>
<p>					//gel all child divs</p>
<p>					var childDivs = pluginDivs[i].getElementsByTagName("div");</p>
<p>					for (var j = 0; j < childDivs.length; j++) {</p>
<p>						if (typeof(childDivs[j].className) != 'undefined') {</p>
<p>							if (childDivs[j].className == 'gb-informers_photo') {</p>
<p></p>
<p>								if (pluginPhotoLoadCounter > 3) {</p>
<p>									photoChecked = true;</p>
<p>								}</p>
<p>								else {</p>
<p>									return;</p>
<p>								}</p>
<p>							}</p>
<p>						}</p>
<p>					}</p>
<p></p>
<p>					if (!photoChecked) {</p>
<p>						continue;</p>
<p>					}</p>
<p>				}</p>
<p></p>
<p>				if (((this.getElementCurrentStyle(pluginDivs[i], 'width') == '300px') && photoChecked)) {</p>
<p>					checked = true;</p>
<p>				}</p>
<p>				else {</p>
<p>					return;</p>
<p>				}</p>
<p>			}</p>
<p>		}</p>
<p>        </p>
<p>		if (checked) {</p>
<p>			for (var i = 0; i < pluginDivs.length; i++) {</p>
<p>				if (pluginDivs[i].className == 'gb-informers_project_content_box') {</p>
<p></p>
<p>					if (pluginDivs[i].style.display == 'none') {</p>
<p>						pluginDivs[i].style.display = 'block';</p>
<p>						pluginDivsHeight[pluginDivs[i].id] = pluginDivs[i].offsetHeight;</p>
<p>						pluginDivs[i].style.display = 'none';</p>
<p>					}</p>
<p>					else {</p>
<p>						pluginDivsHeight[pluginDivs[i].id] = pluginDivs[i].offsetHeight;</p>
<p>					}</p>
<p></p>
<p>					var id = pluginDivs[i].id;</p>
<p>					var key = 'plugin_v2_';</p>
<p></p>
<p>					var num = id.substring(id.indexOf(key) + key.length)</p>
<p>					PluginsV2[num] = new PluginV2(id, num);</p>
<p>					PluginsV2[num].addGoogleAnalytics();</p>
<p>				}</p>
<p></p>
<p>				this.initialized = true;</p>
<p>                togglePluginFirst();</p>
<p>				clearInterval(pluginTimerID);</p>
<p>			}</p>
<p>		}</p>
<p>       </p>
<p>	},</p>
<p></p>
<p>	incPhotoLoadCounter: function() {</p>
<p>		pluginPhotoLoadCounter++;</p>
<p>	}</p>
<p>}</p>
<p></p>
<p>var PluginsV2 = new Array();</p>
<p>var pluginTicksCounter = 0;</p>
<p>var pluginPhotoLoadCounter = 0;</p>
<p>var pluginTimerID = setInterval('PluginV2.prototype.init()', 100);</p>
<p></p>
<p>function togglePluginFirst() {</p>
<p>    var firstBlock = 1;</p>
<p>    </p>
<p>    </p>
<p>                                                firstBlock = 3; // re-init firstBlock</p>
<p>                        </p>
<p>    </p>
<p>    for (i in PluginsV2) {</p>
<p>		if (typeof PluginsV2[i] == 'object' && typeof PluginsV2[i].num != 'undefined') {</p>
<p>            if (i!=firstBlock) {</p>
<p>                PluginsV2[i].close();</p>
<p>            }</p>
<p>		}</p>
<p>	}</p>
<p>}</p>
<p></p>
<p>function togglePlugin(id) {</p>
<p>    for (i in PluginsV2) {</p>
<p>		if (typeof PluginsV2[i] == 'object' && typeof PluginsV2[i].num != 'undefined' && PluginsV2[i].isOpened()) {</p>
<p>			PluginsV2[i].close();</p>
<p>		}</p>
<p>	}</p>
<p>	var Plugin = PluginsV2[id];</p>
<p>	Plugin.toggle();</p>
<p>	return false;</p>
<p>}</p>
<p></p>
<p>//Analytics </p>
<p>var _gaq = _gaq || [];</p>
<p>_gaq.push(['_setAccount', PluginV2.prototype.GA_account]);</p>
<p>_gaq.push(['_trackPageview']);</p>
<p></p>
<p>(function() {</p>
<p>    var ga = document.createElement('script'); ga.type = 'text/javascript'; ga.async = true;</p>
<p>    ga.src = ('https:' == document.location.protocol ? 'https://ssl' : 'http://www') + '.google-analytics.com/ga.js';</p>
<p>    var s = document.getElementsByTagName('script')[0]; s.parentNode.insertBefore(ga, s);</p>
<p>  })();</p>
<p></p>
