<p>var hb 								= "";</p>
<p>var hbmatch 						= "";</p>
<p>var hb 								=  $.cookie('HasBeen'); //Current value of cookie</p>
<p>var hbArray 						= new Array();</p>
<p>var scw 							= screen.width; </p>
<p>var sch 							= screen.height;					</p>
<p>var wloc 							= "none";</p>
<p>var wlow 							= "none";</p>
<p>var whigh 							= "none";</p>
<p>var wicon 							= "none";</p>
<p>var cid 							= "1";</p>
<p>var classification 					= "NOTRWC"; //Variable for RWC classification </p>
<p>var relatedcid 						= ""; // Variable for related RWC classification</p>
<p>var syndicator 						= "NONE"; //Variable for syndicator (to check for Fair Use, Official RWC and herald RWC content)</p>
<p>var geoblock 						= "N"; // Variable for Geoblock</p>
<p>var time 							= "NONE"; // Variable for video time</p>
<p>var value 							= "NONE"; //Variable for video value</p>
<p>var play 							= "NONE"; // Variable for click to play or auto play</p>
<p>var relatedClass 					= "N"; //Related RWC classification</p>
<p>var subarea 						= "";</p>
<p>// Weather Location</p>
<p>if ($.cookie('nzh_weatherlocation') != null) {</p>
<p>var locationid 					= $.cookie('nzh_weatherlocation');</p>
<p>} else {</p>
<p>var locationid 					= 12; /* Default to Auckland*/</p>
<p>}</p>
<p>var region 							= nzhdata.json.weather["_" + locationid].name;</p>
<p>// AI Match Segs</p>
<p>var rsi_segs 						= []; </p>
<p>var segs_beg 						= document.cookie.indexOf('rsi_segs='); </p>
<p>if (segs_beg>=0){   </p>
<p>segs_beg 						= document.cookie.indexOf('=',segs_beg)+1;   </p>
<p>if(segs_beg>0){     </p>
<p>var segs_end 				= document.cookie.indexOf(';',segs_beg);     </p>
<p>if(segs_end==-1) segs_end 	= document.cookie.length; </p>
<p>rsi_segs 					= document.cookie.substring(segs_beg,segs_end)              </p>
<p>.split('|');   </p>
<p>} </p>
<p>} </p>
<p>var segLen 							= 20; </p>
<p>var segQS 							= ""; </p>
<p>if (rsi_segs.length<segLen){</p>
<p>segLen 							= rsi_segs.length</p>
<p>} </p>
<p>for (var i=0;i<segLen;i++){</p>
<p>if (rsi_segs == undefined || rsi_segs == "") {</p>
<p>segQS += ("NONE");</p>
<p>}</p>
<p>else {</p>
<p>var temp_segs = '';</p>
<p>temp_segs = rsi_segs[i].split("H11217_")[1];</p>
<p>segQS += ("AS" + temp_segs + ".");</p>
<p>} </p>
<p>}</p>
<p>var subarea 						= "/S1=FIRES/S2=NONE/S3=NONE/S4=NONE/S5=NONE";</p>
<p>if(cid == 522 || relatedcid == "Y" || cid == 1503158 ) { //Rugby World Cup</p>
<p>var classification 				= "RWC";</p>
<p>}				</p>
<p>hbArray.length 						= 5;				</p>
<p>if(hb == null)</p>
<p>{					</p>
<p>hb 								= "NATIONAL" + "._._._._";</p>
<p>$.cookie('HasBeen', hb, {path:'/', expires:30, domain:BaseDomain}); </p>
<p>}</p>
<p>else</p>
<p>{</p>
<p>var hbmatch 					= hb.match("_"); </p>
<p>if(hbmatch == "_")</p>
<p>{</p>
<p>//replace the '_' with the new value</p>
<p>var hbvalue 				= "";</p>
<p>hbvalue 					= "NATIONAL";						</p>
<p>hb 							= hb.replace("_",hbvalue); 		</p>
<p>$.cookie('HasBeen', hb, {path:'/', expires:30, domain:BaseDomain}); 			</p>
<p>}					</p>
<p>else</p>
<p>{</p>
<p>var hbArray 				= hb.split(".");  /* No match - all values filled, split at period */</p>
<p>if(hbArray.length == 5)</p>
<p>{ </p>
<p>hbArray.shift(); /* Move out the first array element */</p>
<p>hbArray[4] 				= "NATIONAL"; /* Set the last array element */</p>
<p>var hbnew 				= "";</p>
<p>for(i=0;i<hbArray.length; i++)</p>
<p>{</p>
<p>if(hbnew == "")		</p>
<p>{	</p>
<p>hbnew 			= hbArray[i];</p>
<p>}</p>
<p>else</p>
<p>{</p>
<p>hbnew 			= hbnew + "." + hbArray[i];</p>
<p>}</p>
<p>}</p>
<p>hb 						= hbnew;</p>
<p>hbnew 					= "";</p>
<p>//var hb = hbArray[0]+ "." + hbArray[1] + "."+ hbArray[2] + "." + hbArray[3] + "." + hbArray[4];</p>
<p>$.cookie('HasBeen', hb, {path:'/', expires:30, domain:BaseDomain}); 	</p>
<p>}</p>
<p>}</p>
<p>}</p>
<p>if (nzhdata.json.weather != undefined)</p>
<p>{</p>
<p>var data 						= nzhdata.json.weather["_" + locationid];</p>
<p>if (data != undefined) { // Generally because we have a cooke but its value is dud.</p>
<p>if($.cookie('nzh_weatherlocation') != null) wloc = data.name.toLowerCase().replace(' ','');	// Only set wloc if cookie is set</p>
<p>// Weather class, high and low to be passed to the ad call through siteTarget variable</p>
<p>var whigh, wlow, wicon;</p>
<p>if (data.forecasts[0] != undefined) { // Make sure we have forecast data.</p>
<p>whigh 					= data.forecasts[0].high;</p>
<p>wlow 					= data.forecasts[0].low;</p>
<p>wicon 					= data.forecasts[0].cssclass;</p>
<p>}</p>
<p>}</p>
<p>}</p>
<p>var siteTarget 						= "/SITE=NZH/AREA=SEC.NATIONAL.STY/CHA=NATIONAL/SS=NATIONAL" + subarea + "/HB=" + hb + "/SCW=" + scw + "/SCH=" + sch + "/WLOC=" + wloc +  "/WH=" + whigh + "/WL=" + wlow + "/WC=" + wicon + "/AS=" + segQS + "/ST=" + syndicator + "/GB=" + geoblock + "/VT=" + time + "/VV=" + value + "/VP=" + play;</p>
<p>// Audience Science Tag</p>
<p>function DM_prepClient(csid,client){</p>
<p>client.DM_cat("NZH > NATIONAL");</p>
<p>client.DM_event("NZH - NATIONAL - NATIONAL");</p>
<p>client.DM_addEncToLoc("S1","FIRES");</p>
<p>client.DM_addEncToLoc("S2","NONE");</p>
<p>client.DM_addEncToLoc("S3","NONE");</p>
<p>client.DM_addEncToLoc("S4","NONE");</p>
<p>client.DM_addEncToLoc("S5","NONE");</p>
<p>client.DM_addEncToLoc("Region",region);</p>
<p>}</p>
