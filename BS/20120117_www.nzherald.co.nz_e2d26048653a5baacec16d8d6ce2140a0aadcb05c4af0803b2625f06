<p>$(document).ready(function()</p>
<p>{</p>
<p>// Add an onclick function to the links in the footer to manage the tabs</p>
<p>// In some cases these are just regular links and not tabs...</p>
<p>$('ul#footerTabs li a').click(function() {</p>
<p>if($(this).parent().hasClass('ui-tabs-selected')){//close the current tab if it is clicked again</p>
<p>$('div.footerTabsExpanded').hide();</p>
<p>$('ul#footerTabs li.ui-tabs-selected').removeClass('ui-tabs-selected');</p>
<p>}else{</p>
<p>var url = $(this).attr('href');					</p>
<p>if(url.substr(0,4)=='http') { location.href = url; return false; }										</p>
<p>$('div.footerTabsExpanded').hide();					</p>
<p>$('div' + url).show();					</p>
<p>$('ul#footerTabs li.ui-tabs-selected').removeClass('ui-tabs-selected');</p>
<p>$(this).parent().addClass('ui-tabs-selected');</p>
<p>$(this).blur();					</p>
<p>return false;</p>
<p>}</p>
<p>});				</p>
<p>});</p>
