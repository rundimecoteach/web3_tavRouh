<p>function omniture_share(obj,tag,fire) {</p>
<p>// Get ominiture variables</p>
<p>var s				= s_gi(s_account);</p>
<p>var type 			= "Share";</p>
<p>var value 			= 3;</p>
<p>// Catch Add Comment</p>
<p>if (tag == "Add Comment"){</p>
<p>type 			= "Comment";</p>
<p>tag 			= "Add";</p>
<p>value 			= 4;</p>
<p>}</p>
<p>// Catch Like Comment</p>
<p>if (tag == "Like Comment"){</p>
<p>type 			= "Comment";</p>
<p>tag 			= "Like";</p>
<p>value 			= 2;</p>
<p>}</p>
<p>// Catch Share buttons</p>
<p>if ((tag == "Twitter")||(tag == "Facebook")||(tag == "Google")){</p>
<p>s.prop23		= "NZH:" + tag.toLowerCase();</p>
<p>}</p>
<p>//setOmnitureVariables();</p>
<p>// Variables to pass on click</p>
<p>s.linkTrackVars		= 'eVar9,eVar10,events,products,prop23';</p>
<p>s.linkTrackEvents	= 'event17';</p>
<p>// User Scoring</p>
<p>s.eVar9 	 		= "+" + value;</p>
<p>s.eVar10  			= "+" + value;</p>
<p>s.events			= "event17";</p>
<p>s.products			= ";;;;event17=" + value;</p>
<p>// Submit this call</p>
<p>if (fire == true) {</p>
<p>s.tl(this, 'o', type + ' - ' + tag);</p>
<p>}</p>
<p>}</p>
