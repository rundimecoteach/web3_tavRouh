<p>function YellowSearchPage()</p>
<p>{</p>
<p>var what = jQuery.trim(document.getElementById('keywordwhat').value).replace(/ /gi,"+");</p>
<p>var what = escape(what);</p>
<p>var where = escape(jQuery.trim(document.getElementById('keywordwhere').value));</p>
<p>if (where == "Where")</p>
<p>{</p>
<p>where="";</p>
<p>}</p>
<p>if (what == "" && where == "") {	</p>
<p>window.open('http://yellow.co.nz/index.jsp?utm_source=nzherald.co.nz&utm_medium=searchwidget','open_window','menubar, toolbar, location, directories, status, scrollbars, resizable, dependent, width=960, height=800, left=0, top=0, _newtab');		</p>
<p>} </p>
<p>else {</p>
<p>window.open('http://yellow.co.nz/search.ds?what='+what +'&where='+where+'&newSearch=true&utm_source=nzherald.co.nz&utm_medium=searchwidget','open_window','menubar, toolbar, location, directories, status, scrollbars, resizable, dependent, width=960, height=800, left=0, top=0, _newtab');</p>
<p>}</p>
<p>}</p>
<p>//clears default value of 'Where' field</p>
<p>$("#keywordwhere").inputDefault({</p>
<p>defaultText		: " Where "</p>
<p>});</p>
