<p>$(function(){</p>
<p>if (nzhdata.json.weather != undefined) {</p>
<p>// Reference our main container.</p>
<p>var weatherItem = $("#nzh-header-weather").find(".locationitem");</p>
<p>var weatherForm = $("#nzh-header-weather").find(".locationsform");</p>
<p>// Populate our change form.</p>
<p>var optGroup1 = $('<optgroup label="North Island"></optgroup>');</p>
<p>var optGroup2 = $('<optgroup label="South Island"></optgroup>');</p>
<p>$.each(nzhdata.json.weather, function(){</p>
<p>if (this.id > -1) {</p>
<p>var option = $('<option value="' + this.id + '">' + this.name + '</option>');</p>
<p>(this.region == "north") ? optGroup1.append(option) : optGroup2.append(option);</p>
<p>}</p>
<p>});</p>
<p>$("#nzh-header-weatherselect").append(optGroup1).append(optGroup2);</p>
<p>// Event to manage our swapping of items.</p>
<p>$("#nzh-header-weatherselect").change(function(){</p>
<p>$.cookie('nzh_weatherlocation', $(this).val(), {path:'/', expires:365, domain:BaseDomain}); // Set the cookie.</p>
<p>changeWeather($(this).val()); weatherForm.hide(); weatherItem.show(); return false;</p>
<p>})</p>
<p>// Modify the weather item on request.</p>
<p>var changeWeather = function(id) {</p>
<p>var data = nzhdata.json.weather["_" + id];</p>
<p>if (data == undefined || data.forecasts[0] == undefined) { // Generally because we have a cooke but its value is dud.</p>
<p>data = nzhdata.json.weather["_error"];</p>
<p>}</p>
<p>weatherItem.find("a.icon").addClass("icon_" + data.forecasts[0].cssclass);</p>
<p>weatherItem.find("a.icon").attr("href", data.link);</p>
<p>weatherItem.find("a.location").attr("href", data.link);</p>
<p>weatherItem.find("span.loc").html(data.name);</p>
<p>weatherItem.find("span.high").html(data.forecasts[0].high + "&deg;");</p>
<p>weatherItem.find("span.low").html(data.forecasts[0].low + "&deg;");</p>
<p>weatherItem.find("a.change")</p>
<p>.click(function(){</p>
<p>weatherItem.hide(); weatherForm.show();</p>
<p>weatherForm.find("option:first").attr("selected", true);</p>
<p>return false;</p>
<p>});</p>
<p>}</p>
<p>// Load the initial weather item.</p>
<p>if ($.cookie('nzh_weatherlocation') != null) { changeWeather($.cookie('nzh_weatherlocation')); } else { changeWeather(12); }</p>
<p>weatherItem.show();	// Show the weather.</p>
<p>}</p>
<p>});</p>
