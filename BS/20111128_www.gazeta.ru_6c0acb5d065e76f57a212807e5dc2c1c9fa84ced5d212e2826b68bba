<p>function setCurDate() {</p>
<p>    var t,re,a,y,m,d;</p>
<p>    t=new Date();</p>
<p>    re=new RegExp("/([0-9]+)/([0-9]+)/([0-9]+)");</p>
<p>    a=re.exec(document.location.href);</p>
<p>    if(a==null) {</p>
<p>  y=t.getFullYear();</p>
<p>  m=t.getMonth()+1; m=(m<10?'0'+m:m);</p>
<p>  d=t.getDate();    d=(d<10?'0'+d:d);</p>
<p>    } else {</p>
<p>  y=a[1]; m=a[2]; d=a[3];</p>
<p>    }</p>
<p>    document.fcal.y.value=y;</p>
<p>    document.fcal.m.value=m;</p>
<p>    document.fcal.d.value=d;</p>
<p>}</p>
<p></p>
