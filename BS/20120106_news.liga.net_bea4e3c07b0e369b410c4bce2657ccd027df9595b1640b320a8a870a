<p>    function topMenuResize()</p>
<p>    {</p>
<p>        var TM = document.getElementById('main_top_menu');</p>
<p>        var IE='\v'=='v';</p>
<p>            var tdToHide0 = document.getElementById('menu_item_multimedia');</p>
<p>                var tdToHide1 = document.getElementById('menu_item_smi');</p>
<p>        </p>
<p>        var topMenuWidth = TM.clientWidth;</p>
<p>        if(topMenuWidth < 1024)</p>
<p>        {</p>
<p>                    if(tdToHide0) tdToHide0.style.display = 'none';</p>
<p>                        if(tdToHide1) tdToHide1.style.display = 'none';</p>
<p>                    }</p>
<p>        else</p>
<p>        {</p>
<p>            if (document.all && !document.querySelector) {</p>
<p>                            if(tdToHide0) {tdToHide0.style.display = 'block';}</p>
<p>                                if(tdToHide1) {tdToHide1.style.display = 'block';}</p>
<p>                            } else {</p>
<p>                            if(tdToHide0) {tdToHide0.style.display = 'table-cell';}</p>
<p>                                if(tdToHide1) {tdToHide1.style.display = 'table-cell';}</p>
<p>                            }</p>
<p></p>
<p>        }</p>
<p>        return true;</p>
<p>    }</p>
<p>    window.onresize = function(event)</p>
<p>    {</p>
<p>        topMenuResize();</p>
<p>        return true;</p>
<p>    }</p>
<p>    topMenuResize();</p>
