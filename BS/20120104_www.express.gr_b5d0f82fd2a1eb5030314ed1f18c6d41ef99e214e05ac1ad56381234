<p>function bookmark(){</p>
<p>    var title="EXPRESS.gr"</p>
<p>    var url="http://www.express.gr"</p>
<p></p>
<p>    if (window.sidebar) window.sidebar.addPanel(title, url,"");</p>
<p></p>
<p>    else if( window.opera && window.print )</p>
<p>    {</p>
<p>    var mbm = document.createElement('a');</p>
<p>    mbm.setAttribute('rel','sidebar');</p>
<p>    mbm.setAttribute('href',url);</p>
<p>    mbm.setAttribute('title',title);</p>
<p>    mbm.click();</p>
<p>    }</p>
<p></p>
<p>    else if( document.all ) window.external.AddFavorite( url, title);</p>
<p></p>
<p>}</p>
