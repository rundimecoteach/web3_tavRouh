<p>function make_regexp(text){</p>
<p>  var text1='';</p>
<p>  for(var i=0; i<text.length;i++){</p>
<p>  	var in_alphabet = false;</p>
<p>    for(var j=0;j<alphabet.length;j++){</p>
<p>      if(alphabet[j].exec(text.substring(i,i+1))){</p>
<p>        text1 += alphabet[j].source;</p>
<p>        in_alphabet = true;</p>
<p>        break;</p>
<p>      }</p>
<p>    }</p>
<p>    if(!in_alphabet){</p>
<p>      text1 += text.substring(i,i+1);</p>
<p>    }</p>
<p>  }</p>
<p>  if (text1.length>0){</p>
<p>    return new RegExp('^'+text1,'i');</p>
<p>  }</p>
<p>  else {</p>
<p>  	return new RegExp('');</p>
<p>  }</p>
<p>}</p>
<p>function  search_in_select_box(text,select_box){</p>
<p>  if (text.length>0){</p>
<p>    re = make_regexp(text);</p>
<p>    for(var i = 0; i <select_box.options.length;i++){</p>
<p>      if(re.exec(select_box.options[i].text)){</p>
<p>        select_box.options[i].selected = true;</p>
<p>        break;      	</p>
<p>      }</p>
<p>    }</p>
<p>  }</p>
<p>}</p>
