<p>function hide(srcElementId){</p>
<p>  var srcElement;</p>
<p>  if(srcElement = get_element(srcElementId)){ </p>
<p>    srcElement.style.display = "none";  </p>
<p>  }</p>
<p>}</p>
<p>function show(srcElementId,left,top){</p>
<p>  var element_to_reveal;</p>
<p>  if(element_to_reveal = get_element(srcElementId)){ </p>
<p>  	if(left){</p>
<p>  	  element_to_reveal.style.left = left +"px";</p>
<p>  	}</p>
<p>  	if(top){</p>
<p>  	  element_to_reveal.style.top = top + "px";</p>
<p>  	}</p>
<p>    element_to_reveal.style.display = "";  </p>
<p>  }</p>
<p>}</p>
