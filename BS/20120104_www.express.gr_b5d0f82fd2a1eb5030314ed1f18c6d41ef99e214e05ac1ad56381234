<p>function PadDigits(n, totalDigits) { </p>
<p>  n = n.toString(); </p>
<p>  var pd = ''; </p>
<p>  if (totalDigits > n.length) { </p>
<p>    for (i=0; i < (totalDigits-n.length); i++) { </p>
<p>      pd += '0'; </p>
<p>    } </p>
<p>  } </p>
<p>  return pd + n.toString(); </p>
<p>} </p>
