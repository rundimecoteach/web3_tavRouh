<p>    		function largeFont() {</p>
<p>            	$("#article").addClass( 'large_font' );</p>
<p>				$("#article").removeClass( 'small_font' );				</p>
<p>            }</p>
<p>    		function smallFont() {</p>
<p>            	$("#article").removeClass( 'large_font' );</p>
<p>				$("#article").addClass( 'small_font' );	</p>
<p>            }</p>
