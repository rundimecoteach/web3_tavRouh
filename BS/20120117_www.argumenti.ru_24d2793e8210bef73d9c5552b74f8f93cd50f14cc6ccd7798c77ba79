<p>						toggler = $(this),</p>
<p>						block 	= $(block);</p>
<p>						toggler.css({'outline': 'none'})</p>
<p></p>
<p>						toggler.click(function(e) {</p>
<p>							e.preventDefault();</p>
<p>							$(block).css({</p>
<p>								'position' 	: 'absolute',</p>
<p>								'top' 		: (toggler.offset().top + opts['top']) + 'px',</p>
<p>								'left' 		: (toggler.offset().left + opts['left']) + 'px'</p>
<p>							});</p>
<p>						if($(block).is(':visible')) $(block).fadeOut(opts['speed']);</p>
<p>						else $(block).fadeIn(opts['speed']);</p>
<p>						this.focus();</p>
<p>						});</p>
<p>						toggler.blur(function() {</p>
<p>							$(block).fadeOut(opts['speed']);</p>
<p>						});</p>
<p>					};</p>
<p>				})(jQuery);</p>
