<p>var google_adnum = 0;</p>
<p>function google_ad_request_done(google_ads) {</p>
<p>var s = '';</p>
<p>var i;</p>
<p>if (google_ads.length == 0) {</p>
<p>return;</p>
<p>}</p>
<p>if (google_ads[0].type == "flash") {</p>
<p>s += '<a href=\"' +</p>
<p>google_info.feedback_url + '\" style="color:000000">Ads by Google</a><br>' + </p>
<p>'<object classid="clsid:D27CDB6E-AE6D-11cf-96B8-444553540000"' +</p>
<p>' codebase="http://download.macromedia.com/pub/shockwave/cabs/flash/swflash.cab#version=6,0,0,0" WIDTH="' + </p>
<p>google_ad.image_width +	'" HEIGHT="' + </p>
<p>google_ad.image_height + '"> <PARAM NAME="movie" VALUE="' + </p>
<p>google_ad.image_url + '">' + </p>
<p>'<PARAM NAME="quality" VALUE="high">' + </p>
<p>'<PARAM NAME="AllowScriptAccess" VALUE="never">' + </p>
<p>'<EMBED src="' + </p>
<p>google_ad.image_url + '" WIDTH="' + </p>
<p>google_ad.image_width + '" HEIGHT="' + </p>
<p>google_ad.image_height + </p>
<p>'" TYPE="application/x-shockwave-flash"' + </p>
<p>' AllowScriptAccess="never" ' + </p>
<p>' PLUGINSPAGE="http://www.macromedia.com/go/getflashplayer"></EMBED></OBJECT>';</p>
<p>} else if (google_ads[0].type == "image") {</p>
<p>s += '<a href=\"' +</p>
<p>google_info.feedback_url + '\" style="color:000000">Ads by Google</a><br> <a href="' + </p>
<p>google_ads[0].url + '" target="_top" title="go to ' + </p>
<p>google_ads[0].visible_url + '" onmouseout="window.status=\'\'" onmouseover="window.status=\'go to ' + </p>
<p>google_ads[0].visible_url + '\';return true"><img border="0" src="' + </p>
<p>google_ads[0].image_url + '"width="' + </p>
<p>google_ads[0].image_width + '"height="' + </p>
<p>google_ads[0].image_height + '"></a>';</p>
<p>} else if (google_ads[0].type == "html") {</p>
<p>s += '<a href=\"' +</p>
<p>google_info.feedback_url + '\" style="color:000000">Διαφημίσεις Google</a><br>' + </p>
<p>google_ads[0].snippet;</p>
<p>} else {</p>
<p>if (google_ads.length > 1) {</p>
<p>s += "<table id=\"ga_ethnosgads\" cellspacing=\"0\" cellpadding=\"0\">"; </p>
<p>s += "  <tr>";</p>
<p>s += "    <td id=\"ga_head\">";</p>
<p>s += '<a href=\"' + google_info.feedback_url + '\">Διαφημίσεις Google</a>'</p>
<p>s += " <\/td>";</p>
<p>s += " <\/tr>";</p>
<p>s += " <tr>";</p>
<p>s += " <td>";</p>
<p>for(i = 0; i < google_ads.length; ++i) {</p>
<p>s += '<br><a id=\"ga_title\" target="_blank" href="' + </p>
<p>google_ads[i].url + '" onmouseout="window.status=\'\'" onmouseover="window.status=\'go to '+</p>
<p>google_ads[i].visible_url + '\';return true"> <span>' + </p>
<p>google_ads[i].line1 + '</span></a> <br><span>' +</p>
<p>google_ads[i].line2 + '</span><br/> <a id=\"ga_adlink\" target="_blank" href="' + </p>
<p>google_ads[i].url + '" onmouseout="window.status=\'\'" onmouseover="window.status=\'go to ' +</p>
<p>google_ads[i].visible_url + '\';return true"><span>' + </p>
<p>google_ads[i].visible_url + '</span></a>';</p>
<p>}</p>
<p>s += " <\/td>";</p>
<p>s += " <\/tr>";</p>
<p>s += " <\/table>";</p>
<p>}}</p>
<p>if (google_ads[0].bidtype == "CPC") {</p>
<p>google_adnum = google_adnum + google_ads.length;</p>
<p>}</p>
<p>document.write(s);</p>
<p>return;</p>
<p>}</p>
