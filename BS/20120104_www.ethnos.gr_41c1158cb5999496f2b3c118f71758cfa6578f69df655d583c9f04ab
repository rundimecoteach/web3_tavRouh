<p>google_ad_channel = '2746532400';</p>
<p>google_ad_output = 'js';</p>
<p>google_max_num_ads = '4';</p>
<p>google_ad_type = 'text';</p>
<p>google_feedback = 'on';</p>
<p>google_encoding = 'UTF-8';</p>
