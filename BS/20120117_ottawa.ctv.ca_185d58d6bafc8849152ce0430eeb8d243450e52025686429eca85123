<p></p>
<p>function findPos(obj) {</p>
<p></p>
<p>	var curleft = curtop = 0;</p>
<p></p>
<p>	if (obj.offsetParent) {</p>
<p></p>
<p>		curleft = obj.offsetLeft</p>
<p></p>
<p>		curtop = obj.offsetTop</p>
<p></p>
<p>		while (obj = obj.offsetParent) {</p>
<p></p>
<p>			curleft += obj.offsetLeft</p>
<p></p>
<p>			curtop += obj.offsetTop</p>
<p></p>
<p>		}</p>
<p></p>
<p>	}</p>
<p></p>
<p>	return [curleft,curtop];</p>
<p></p>
<p>}</p>
