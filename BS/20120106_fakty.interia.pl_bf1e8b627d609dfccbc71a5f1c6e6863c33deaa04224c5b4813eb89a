<p>objectWeatherJQ.find(".wheater-deg").text(data.list.day0.tempAvg);</p>
<p>}</p>
<p>if (data.list.day1) {</p>
<p>weatherDay1JQ = objectWeatherJQ.find(".weather-day1");</p>
<p>weatherDay1JQ.css("background-image", '/i/weather-icons/small' + data.list.day1.iconId + '.png');</p>
<p>weatherDay1JQ.find("span").html(data.list.day1.tempMin + '/' + data.list.day1.tempMax + '&deg;C');</p>
<p>}</p>
<p>if (data.list.day2) {</p>
<p>weatherDay1JQ = objectWeatherJQ.find(".weather-day2");</p>
<p>weatherDay1JQ.css("background-image", '/i/weather-icons/small' + data.list.day2.iconId + '.png');</p>
