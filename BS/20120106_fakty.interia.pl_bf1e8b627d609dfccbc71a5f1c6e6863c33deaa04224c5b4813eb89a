<p>objectWeatherJQ.find(".weather-more").attr("href", "http://pogoda.interia.pl/miasta?id=" + cityId);</p>
<p>jQuery(".weather-selected-city").removeClass("weather-selected-city");</p>
<p>objectWeatherJQ.removeClass("weather-cities-active");</p>
