<p>function mapHover(){</p>
<p>var mapJQ = jQuery("#boxPolishMap1"),</p>
<p>mapAreaJQ = mapJQ.find("area"),</p>
<p>deafultClass =  'map-province',</p>
<p>mapProvinceJQ = jQuery("<div/>").addClass(deafultClass).appendTo(mapJQ),</p>
<p>mapTip = jQuery("<div/>").addClass("map-tip").html("<span class='map-text'></span>").appendTo(mapProvinceJQ),</p>
<p>mapTipTxt = mapTip.find(".map-text"),</p>
<p>mapClass;</p>
<p></p>
<p>mapAreaJQ</p>
<p>.mouseover( function()</p>
<p>{</p>
<p>mapClass = jQuery(this).attr('class');</p>
<p>mapProvinceJQ.addClass(mapClass + ' map-active');</p>
<p>mapTipTxt.html(jQuery(this).attr('alt'));</p>
<p>mapTip.show();</p>
<p>})</p>
<p>.mouseout( function() {</p>
<p>mapProvinceJQ.removeClass().addClass(deafultClass)</p>
<p>mapTipTxt.html('');</p>
<p>mapTip.hide();</p>
<p>});</p>
<p>}</p>
