<p>function metoxes_moy_init(){</p>
<p>  this.set = new Array();</p>
<p>  this.count = 0;</p>
<p>  this.set_just_changed = new Array();</p>
<p>  this.last_modified = 0;</p>
<p>  this.last_transaction_id = -1;</p>
<p>  </p>
<p>  if (!this.listenerAttached) {</p>
<p>    this.listenerAttached = true;</p>
<p>    my_objects[my_objects.length] = this;</p>
<p>  }	</p>
<p>  	</p>
<p>}</p>
<p></p>
<p>function metoxes_moy_load(what){</p>
<p>  this.set_just_changed = new Array();</p>
<p>  var length = "";var length2 = "";var metavoli=0.0;</p>
<p>  for(var i in what){ </p>
<p>    //var length = this.set.length;</p>
<p>    length = what[i]["secsym"];</p>
<p>    metavoli = (typeof(this.set[length])=="object" && this.set[length].price>0.0)?(parseFloat(what[i]["price"])-this.set[length].price):0.0; // se sxesi me proigoymeni timi     </p>
<p>    if(typeof(this.set[length])!="object"){</p>
<p>      this.set[length] = new Object();</p>
<p>    }</p>
<p>    this.set[length].secsym = what[i]["secsym"];</p>
<p>    this.set[length].price = parseFloat(what[i]["price"]);</p>
<p>    this.set[length].diff = parseFloat(what[i]["diff"]);</p>
<p>    this.set[length].price_diff = parseFloat(what[i]["price_diff"]);</p>
<p>    this.set[length].apodosi_etous = parseFloat(what[i]["apodosi_etous"]);</p>
<p>    this.set[length].metavoli = metavoli;</p>
<p>   </p>
<p>    length2 = this.set_just_changed.length;</p>
<p>    this.set_just_changed[length2] = length;</p>
<p>  }  </p>
<p>  delete length; delete length2;</p>
<p>}</p>
<p></p>
<p>function metoxes_moy_cretera_tosend(check_cookie){</p>
<p>  var toret = new Array();</p>
<p>  toret["last_modified"] = this.last_modified;</p>
<p>  toret["last_transaction_id"] = this.last_transaction_id;</p>
<p>  var secsyms_to_send = new Array();</p>
<p>  this.count = 0;</p>
<p>  for(var i in this.set){</p>
<p>    secsyms_to_send[secsyms_to_send.length] = "'"+this.set[i].secsym+"'";</p>
<p>    this.count++;	</p>
<p>  }</p>
<p> // alert(d(readCookie("my_secsyms")))</p>
<p>  if(this.count>0){</p>
<p>    toret["secsyms"] = e(secsyms_to_send.join(","));</p>
<p>  }</p>
<p>  else if(check_cookie) {</p>
<p>  	this.count++;</p>
<p>  	toret["secsyms"] = e(d(readCookie("my_secsyms")));</p>
<p>  }</p>
<p>  var date = new Date();</p>
<p>  date.setTime(date.getTime()+(10*365*24*60*60*1000));</p>
<p>//  document.cookie="my_secsyms="+e(secsyms_to_send.join(","))+"; expires="+date.toGMTString()+"; path=/";</p>
<p>  document.cookie="my_secsyms="+toret["secsyms"]+"; expires="+date.toGMTString()+"; path=/";</p>
<p>  delete secsyms_to_send;</p>
<p>  delete date;</p>
<p>  return toret;</p>
<p>}</p>
<p>function metoxes_moy_after_get() {//alert("Dummy metoxes_moy_after_get");</p>
<p>}</p>
<p>function metoxes_moy_get(check_cookie){   </p>
<p>	</p>
<p>  var a = new PHP_Serializer();</p>
<p>  var b = a.serialize(this.cretera_tosend(check_cookie));	</p>
<p>  if(!this.getting && this.count>0){</p>
<p>    var what_to_do = new Object();</p>
<p>    what_to_do.callback = do_metoxes_moy_get;</p>
<p>    what_to_do.extra_data = this;</p>
<p>    x_get_metoxes_moy(b,true,what_to_do);</p>
<p>    this.getting = true;</p>
<p>  }</p>
<p>  else {</p>
<p>  	this.is_shown = false;</p>
<p>  	this.is_opened = false;</p>
<p>  	this.show();</p>
<p>  }</p>
<p>  delete a; delete b;</p>
<p>}</p>
<p>function do_metoxes_moy_get(that,aaa){</p>
<p>  aaa.getting = false;</p>
<p>  var b = that;</p>
<p>  if(b["status"]==1){</p>
<p>    aaa.load(b["data"]["set"]);</p>
<p>    aaa.last_modified = (aaa.last_modified<b["data"]["last_modified"])?b["data"]["last_modified"]:aaa.last_modified;</p>
<p>    aaa.last_transaction_id = (aaa.last_transaction_id<b["data"]["last_transaction_id"])?b["data"]["last_transaction_id"]:aaa.last_transaction_id;</p>
<p>    aaa.show();</p>
<p>  }</p>
<p>  else if(b["status"]==0){</p>
<p>  	aaa.last_modified = (aaa.last_modified<b["data"]["last_modified"])?b["data"]["last_modified"]:aaa.last_modified;</p>
<p>    aaa.show();</p>
<p>  }</p>
<p>  aaa.after_get();</p>
<p>  handle_status(b["status"],b["mesg"]); </p>
<p>  delete b; delete that;</p>
<p>}</p>
