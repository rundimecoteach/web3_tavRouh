<p>// methods</p>
<p>  this.init = metoxes_my_init;</p>
<p>  this.get_by_letter = metoxes_my_get_by_letter;</p>
<p>  this.get_by_secsym = metoxes_my_get_by_secsym;</p>
<p>  this.get_by_catego = metoxes_my_get_by_catego;</p>
<p>  this.fill_found_metoxes = metoxes_my_fill_found_metoxes;</p>
<p>  this.onselect = metoxes_my_onselect;</p>
<p>//</p>
<p>  this.init();</p>
<p>}</p>
<p>function metoxes_my_init(){</p>
<p>  this.set = new Array();</p>
<p>}</p>
<p>function metoxes_my_get_by_letter(letter,do_regexp){</p>
<p>  this.init();</p>
<p>  if (letter.length>0){</p>
<p>    var re = (do_regexp)?make_regexp(letter):new RegExp('^'+letter);</p>
<p>    for(var i in this.set_oles){</p>
<p>      if(re.exec(this.set_oles[i].compnam)){</p>
<p>        this.set[i]=this.set_oles[i];</p>
<p>      }</p>
<p>    }</p>
<p>  }</p>
<p>  this.fill_found_metoxes();</p>
<p>}</p>
<p>function metoxes_my_get_by_secsym(secsym){</p>
<p>  this.init();</p>
<p>  if (secsym.length>0){</p>
<p>    var re = make_regexp(secsym);</p>
<p>    for(var i in this.set_oles){</p>
<p>      if(re.exec(this.set_oles[i].secsym)){</p>
<p>        this.set[i]=this.set_oles[i];</p>
<p>      }</p>
<p>    }</p>
<p>  }</p>
<p>  this.fill_found_metoxes();</p>
<p>}</p>
<p>function metoxes_my_get_by_catego(comp_categ){</p>
<p>  this.init();</p>
<p>  if (comp_categ.length>0){</p>
<p>    for(var i in this.set_oles){</p>
<p>      if(comp_categ==this.set_oles[i].comp_categ){</p>
<p>        this.set[i]=this.set_oles[i];</p>
<p>      }</p>
<p>    }</p>
<p>  }</p>
<p>  else {</p>
<p>    this.set=this.set_oles;</p>
<p>  }</p>
<p>  this.fill_found_metoxes();</p>
<p>}</p>
<p>function metoxes_my_fill_found_metoxes(){</p>
<p>  if(typeof(this.forma)=='undefined'){</p>
<p>    return false;</p>
<p>  }</p>
<p>  var t = '';</p>
<p>  for( var i in this.set ){</p>
<p>    t +=  '<div onclick="search_metoxes_my.onselect(\''+this.set[i].secsym+'\');" class="link">'+this.set[i].compnam+' ('+this.set[i].secsym+')'+'</div>\n';   </p>
<p>  }</p>
<p>  get_element('found_metoxes_set_inner_my').innerHTML = t;</p>
<p>}</p>
<p>function metoxes_my_onselect(value){</p>
<p>  alert(value);</p>
<p>}</p>
<p>var search_metoxes_my = new Metoxes_my('search_metoxes_my');</p>
<p>search_metoxes_my.forma = document.search_metoxi_my_forma;</p>
