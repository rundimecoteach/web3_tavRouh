<p>function metoxes_moy_rem(secsym, position){</p>
<p>  if(typeof(this.set[secsym])=="object"){</p>
<p>  	var tmp = new Array();</p>
<p>  	for(var i in this.set){</p>
<p>  	  if(i!=secsym){</p>
<p>  	    tmp[i] = this.set[i];</p>
<p>  	  }</p>
<p>  	}</p>
<p>  	this.set = tmp;</p>
<p>  	innerHTML(this.inner_popup4,this.draw_afairesi_metoxis());</p>
<p>  	delete tmp;</p>
<p>  }</p>
<p>}</p>
<p></p>
<p>function static_metoxes_moy_hide(e) {</p>
<p>  e = e || window.event;	</p>
<p>  for (var i in my_objects){</p>
<p>    if (typeof(my_objects[i])=="object"){</p>
<p>      if(my_objects[i].hide_prosthesi_metoxis) {</p>
<p>        my_objects[i].hide_prosthesi_metoxis(e);</p>
<p>      }</p>
<p>      if(my_objects[i].hide_afairesi_metoxis){</p>
<p>      	my_objects[i].hide_afairesi_metoxis(e);</p>
<p>      }            </p>
<p>    }</p>
<p>  }</p>
<p>}</p>
<p>window.popupWindowOldEventListener2 = document.onmouseup;</p>
<p>if (window.popupWindowOldEventListener2 != null) {</p>
<p>  document.onmouseup = function(e){</p>
<p>    window.popupWindowOldEventListener2(e); </p>
<p>    static_metoxes_moy_hide(e);</p>
<p>  }</p>
<p>}</p>
<p>else {</p>
<p>  document.onmouseup = static_metoxes_moy_hide;</p>
<p>}</p>
