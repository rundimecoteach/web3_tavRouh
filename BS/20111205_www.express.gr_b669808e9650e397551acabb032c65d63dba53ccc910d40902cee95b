<p>      var selectedTab = null;</p>
<p>      function showPanel(tab, name)</p>
<p>      {</p>
<p>        if (selectedTab) </p>
<p>        {</p>
<p>          selectedTab.style.backgroundColor = '';</p>
<p>                  selectedTab.style.Color = '#1c6a05';</p>
<p>          selectedTab.style.background = 'url(styles/tabs2.gif)';</p>
<p>        }</p>
<p>        selectedTab = tab;</p>
<p>        selectedTab.style.Color = '#aa1415';</p>
<p>          selectedTab.style.background = 'url(styles/tabs1.gif)';</p>
<p>          </p>
<p>        for(i = 0; i < panels.length; i++)</p>
<p>        {</p>
<p>          document.getElementById(panels[i]).style.display = (name == panels[i]) ? 'block':'none';</p>
<p>        }</p>
<p>        return false;</p>
<p>      }</p>
