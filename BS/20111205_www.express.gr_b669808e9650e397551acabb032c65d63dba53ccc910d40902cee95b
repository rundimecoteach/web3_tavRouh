<p>function check_radio_box(radio_box,what){</p>
<p>  for (var i = 0; i < radio_box.length;i++){</p>
<p>    if(radio_box[i].value == what){</p>
<p>      radio_box[i].checked = true;</p>
<p>      break;</p>
<p>    }</p>
<p>  }</p>
<p>}</p>
