<p>function metoxes_moy_show(){</p>
<p></p>
<p>  if(this.is_shown && this.count>0){</p>
<p>  	this.draw_changes();</p>
<p>  }</p>
<p>  else {</p>
<p>    if(innerHTML(this.inner,this.draw())){</p>
<p>      show(this.inner);</p>
<p>    }</p>
<p>    if(this.count>0){  </p>
<p>      if(innerHTML(this.inner2,this.draw_inside())){</p>
<p>        if(this.is_opened){</p>
<p>          show(this.inner2);</p>
<p>        }</p>
<p>        else {</p>
<p>          hide(this.inner2)		</p>
<p>        }</p>
<p>      }	</p>
<p>    }</p>
<p>    else {</p>
<p>      hide(this.inner2)	</p>
<p>    }</p>
<p>  }</p>
<p>} </p>
