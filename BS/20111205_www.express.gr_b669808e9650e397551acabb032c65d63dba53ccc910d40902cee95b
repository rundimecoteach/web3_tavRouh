<p><!--</p>
<p>//</p>
<p>// class metoxes_moy</p>
<p>//</p>
<p>function Metoxes_moy(name,parent){</p>
<p>  this.name = name;</p>
<p>  this.parent = parent;</p>
<p>  this.inner = make_name(this.name)+"_inner";</p>
<p>  this.inner2 = make_name(this.name)+"_inner2";</p>
<p></p>
<p>  this.inner_popup4 = make_name(this.name)+"_inner_popup4";</p>
<p>  this.inner_popup3 = make_name(this.name)+"_inner_popup3";</p>
<p>  this.inner_popup2 = make_name(this.name)+"_inner_popup2";</p>
<p>  this.inner_popup = make_name(this.name)+"_inner_popup";</p>
<p>  this.inner_help2 = make_name(this.name)+"_inner_help2";</p>
<p>  this.inner_help = make_name(this.name)+"_inner_help";</p>
<p>  this.listenerAttached = false;</p>
<p>  </p>
<p>  this.getting = false;</p>
<p>  this.is_shown = false;</p>
<p>  this.is_opened = false;</p>
<p>//methods</p>
<p>  this.init = metoxes_moy_init;</p>
<p>  this.load = metoxes_moy_load;</p>
<p>  this.cretera_tosend = metoxes_moy_cretera_tosend;</p>
<p>  this.after_get = metoxes_moy_after_get;</p>
<p>  this.get = metoxes_moy_get;</p>
