<p>function is_numeric(sText){</p>
<p>  var ValidChars = "0123456789";</p>
<p>  var Char;</p>
<p>  IsNumber = true</p>
<p>  for (i = 0; i < sText.length && IsNumber == true; i++){</p>
<p>    Char = sText.charAt(i);</p>
<p>    if (ValidChars.indexOf(Char) == -1){</p>
<p>      IsNumber = false;</p>
<p>    }</p>
<p>  }</p>
<p>  return IsNumber;</p>
<p>}</p>
