<p>function metoxes_moy_draw_changes(){  </p>
<p>  var secsym = "";var tmp_prefix_id = "";</p>
<p>  if(!this.is_ana_klado){</p>
<p>    for(var i in this.set){</p>
<p>  	  tmp_prefix_id = make_name(this.name)+"_"+make_name(i);</p>
<p>      get_element(tmp_prefix_id+"_name_inner").style.background = "";	</p>
<p>      get_element(tmp_prefix_id+"_price_inner").style.background = "";</p>
<p>      get_element(tmp_prefix_id+"_diff_inner").style.background = "";	</p>
<p>      get_element(tmp_prefix_id+"_price_diff_inner").style.background = "";	</p>
<p>    }</p>
<p>  }</p>
<p>  for(var i in this.set_just_changed){</p>
<p>  	secsym = this.set_just_changed[i];</p>
<p>  	if(typeof(this.set[secsym])=="object"){</p>
<p>  	  tmp_prefix_id = make_name(this.name)+"_"+make_name(secsym);</p>
<p>  	  color = (this.set[secsym].price_diff>0.0)?"green":((this.set[secsym].price_diff<0.0)?"red":"blue");</p>
<p>  //	  innerHTML(tmp_prefix_id+"_metavoli_inner","<img src='http://cnt4.express.gr/xrimatistirio/images//"+((this.set[secsym].metavoli==0.0)?"blue-square.gif":((this.set[secsym].metavoli>0.0)?"up.gif":"down.gif"))+"' alt='' width='10'>");</p>
<p>  	  innerHTML(tmp_prefix_id+"_metavoli_inner","<img src='http://cnt4.express.gr/xrimatistirio/images//"+((this.set[secsym].price_diff==0.0)?"blue-square.gif":((this.set[secsym].price_diff>0.0)?"up.gif":"down.gif"))+"' alt='' width='10'>");</p>
<p>      get_element(tmp_prefix_id+"_name_inner").style.color = color;</p>
<p>      </p>
<p>      get_element(tmp_prefix_id+"_price_inner").style.color = color;</p>
<p>      innerHTML(tmp_prefix_id+"_price_inner",number_format(this.set[secsym].price,4,",","."));</p>
<p>  </p>
<p>      get_element(tmp_prefix_id+"_diff_inner").style.color = color;</p>
<p>      innerHTML(tmp_prefix_id+"_diff_inner",number_format(this.set[secsym].diff,4,",",".",true));</p>
<p>            </p>
<p>      get_element(tmp_prefix_id+"_price_diff_inner").style.color = color;</p>
<p>      innerHTML(tmp_prefix_id+"_price_diff_inner",number_format(this.set[secsym].price_diff,2,",",".",true)+"%");   </p>
<p>       </p>
<p>      get_element(tmp_prefix_id+"_apodosi_etous_inner").style.color = color;</p>
<p>      innerHTML(tmp_prefix_id+"_apodosi_etous_inner",number_format(this.set[secsym].apodosi_etous,2,",",".",true)+"%");        </p>
<p>      </p>
<p>      get_element(tmp_prefix_id+"_name_inner").style.background = "yellow";</p>
<p>      get_element(tmp_prefix_id+"_price_inner").style.background = "yellow";</p>
<p>      get_element(tmp_prefix_id+"_diff_inner").style.background = "yellow";</p>
<p>      get_element(tmp_prefix_id+"_price_diff_inner").style.background = "yellow";</p>
<p>      get_element(tmp_prefix_id+"_apodosi_etous_inner").style.background = "yellow";</p>
<p>  	}</p>
<p>  }</p>
<p>} </p>
