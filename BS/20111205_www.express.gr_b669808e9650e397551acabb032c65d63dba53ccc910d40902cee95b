<p>function metoxes_moy_draw_afairesi_metoxis(){</p>
<p>  var t = "";</p>
<p>  t += "<TABLE border='0' cellpadding='2' cellspacing='2' width='300' class='' >";</p>
<p>  t += "<TR>";</p>
<p>  t += "  <TD class='big bold'>Αφαίρεση Μετοχής</TD>";</p>
<p>  t += "</TR>"; </p>
<p>  t += "<TR>";</p>
<p>  t += "  <TD>Πατήστε πάνω στο «x» δίπλα στο σύμβολο για το αφαιρέσετε απο το χαρτοφυλακίο σας</TD>";</p>
<p>  t += "</TR>";   </p>
<p>  t += "<TR>";</p>
<p>  t += "  <TD>";</p>
<p>  t += "  <TABLE border='0' cellpadding='2' cellspacing='2'>";</p>
<p>  t += "  <TR>";</p>
<p>  var j=0;</p>
<p>  for(var i in this.set){</p>
<p>  	if(j%5==0 && j>0){</p>
<p>  	  t += "</tr><tr>";	</p>
<p>  	}</p>
<p>  	t += "<TD class='small'>"+i+"</td><td><div style='width: 12px; height: 12px;' class='border link bold center bg_white' onclick=\""+this.name+".rem('"+i+"',"+j+");\">X</div></TD>"; </p>
<p>  	j++;    </p>
<p>  }</p>
<p>  this.count = j;</p>
<p>  t += "  </TR>";</p>
<p>  t += "  </TABLE>";</p>
<p>  t += "  </TD>";</p>
<p>  t += "</TR>";  </p>
<p>  t += "<TR>";</p>
<p>  t += "  <TD class='right'><img src='http://cnt4.express.gr/xrimatistirio/images//go.gif' alt='close' id='"+make_name(this.name)+"_add_secsyms_go_ctl'  onclick=\""+this.name+".is_opened=true;"+this.name+".is_shown=false;"+this.name+".get();hide("+this.name+".inner_popup3);\" class='link' /></TD>";</p>
<p>  t += "</TR>";     </p>
<p>  t += "</TABLE>";</p>
<p>  return t;		</p>
<p>}</p>
<p>function metoxes_moy_hide_afairesi_metoxis(e){</p>
<p>  var is_clicked_inside = false;</p>
<p>  var p = getElementPosition(this.inner_popup3);</p>
<p>  var c = mouseCoords(e);</p>
<p>  is_clicked_inside = ((c.x > p.left) && (c.x < p.left+p.width) && (c.y > p.top) && (c.y < p.top+p.height))?true:false; </p>
<p>  if(!is_clicked_inside){</p>
<p>    hide(this.inner_popup3)	</p>
<p>    if(this.count>0 && p.width>0){</p>
<p>      this.is_opened=true;</p>
<p>      this.is_shown=false;</p>
<p>      this.get();</p>
<p>    }</p>
<p>  }	</p>
<p>  delete p;delete c;delete is_clicked_inside;</p>
<p>}</p>
