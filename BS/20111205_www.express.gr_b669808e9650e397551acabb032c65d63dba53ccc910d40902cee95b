<p>function metoxes_moy_draw_prosthesi_metoxis(){</p>
<p>  var t = "";</p>
<p>  t += "<TABLE border='0' cellpadding='1' cellspacing='1' width='100%'>";</p>
<p>  t += "<TR>";</p>
<p>  t += "  <TD class='big bold'>Πρόσθεση Μετοχής</TD>";</p>
<p>  t += "</TR>"; </p>
<p>  t += "<TR>";</p>
<p>  t += "  <TD>Εισάγετε μέχρι 15 σύμβολα</TD>";</p>
<p>  t += "</TR>";   </p>
<p>  t += "<TR>";</p>
<p>  t += "  <TD>";</p>
<p>  t += "  <TABLE border='0' cellpadding='1' cellspacing='1' width='100%'>";</p>
<p>  t += "  <TR>";</p>
<p>  var j=0;</p>
<p>  for(var i in this.set){</p>
<p>  	if(j%5==0 && j>0){</p>
<p>  	  t += "</tr><tr>";	</p>
<p>  	}</p>
<p>  	t += "<TD><INPUT type='text' name='"+make_name(this.name)+"_add_secsym_"+j+"_input' value='"+i+"' class='small border' size='9' maxlength='9' readonly></TD>"; </p>
<p>  	j++;    </p>
<p>  }</p>
<p>  this.count = j;</p>
<p>  while (j<15 && j>=0){</p>
<p>  	if(j%5==0 && j>0){</p>
<p>  	  t += "</tr><tr>";	</p>
<p>  	}</p>
<p>    t += "<TD><INPUT type='text' name='"+make_name(this.name)+"_add_secsym_"+j+"_input' value='' class='small border' size='9' maxlength='9' readonly></TD>";	</p>
<p>    j++;</p>
<p>  }</p>
<p>  t += "  </TR>";</p>
<p>  t += "  </TABLE>";</p>
<p>  t += "  </TD>";</p>
<p>  t += "</TR>";  </p>
<p>  t += "<TR>";</p>
<p>  t += "  <TD class='right'><img src='http://cnt4.express.gr/xrimatistirio/images//go.gif' alt='go' id='"+make_name(this.name)+"_add_secsyms_go_ctl' onclick=\""+this.name+".is_opened=true;"+this.name+".is_shown=false;"+this.name+".get();hide("+this.name+".inner_popup);\" class='link'/></TD>";</p>
<p>  t += "</TR>";     </p>
<p>  t += "</TABLE>";</p>
<p>  return t;	</p>
<p>}</p>
<p>function metoxes_moy_hide_prosthesi_metoxis(e){</p>
<p>  var is_clicked_inside = false;</p>
<p>  var p = getElementPosition(this.inner_popup);</p>
<p>  var c = mouseCoords(e);</p>
<p>  is_clicked_inside = ((c.x > p.left) && (c.x < p.left+p.width) && (c.y > p.top) && (c.y < p.top+p.height))?true:false; </p>
<p>  if(!is_clicked_inside){</p>
<p>    hide(this.inner_popup)	</p>
<p>  }	</p>
<p>}</p>
<p></p>
