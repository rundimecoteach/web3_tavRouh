<p>// --- функция подсветки ---</p>
<p>function bgLighting (div,bg_Color)</p>
<p>{</p>
<p>  if(dom) document.getElementById(div).style.backgroundColor = bg_Color;</p>
<p>  if(ie4) document.all[div].style.backgroundColor = bg_Color;</p>
<p>  if(nn4) document.layers[div].bgColor = bg_Color;</p>
<p>}</p>
