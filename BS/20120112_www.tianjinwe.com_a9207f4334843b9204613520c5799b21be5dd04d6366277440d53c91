<p>	function makeurl(url) {</p>
<p>		url = url.replace(/{url}/ig, encodeURIComponent("http://www.tianjinwe.com/tianjin/tbbd/201201/t20120112_4976286.html"));</p>
<p>		url = url.replace(/{title}/ig, encodeURIComponent("深圳回应史上最牛工资单:"人均年薪30万"没违规"));</p>
<p>		url = url.replace(/{site}/ig, encodeURI("http://www.tianjinwe.com"));</p>
<p>		url = url.replace(/{pic}/ig, '');</p>
<p>		url = url.replace(/{cont}/ig, encodeURIComponent("深圳回应史上最牛工资单:"人均年薪30万"没违规"));</p>
<p>		url = url.replace(/{content}/ig, encodeURIComponent("深圳回应史上最牛工资单:"人均年薪30万"没违规"));</p>
<p>		return url;</p>
<p>	}</p>
