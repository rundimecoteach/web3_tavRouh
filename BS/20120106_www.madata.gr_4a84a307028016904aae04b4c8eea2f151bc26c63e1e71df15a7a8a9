<p>						function updateEmailToAFriend(){</p>
<p>							if ($('email_to_a_friend_dump')){</p>
<p>								$('email_to_a_friend_dump').remove();</p>
<p>							}</p>
<p>							</p>
<p>							emailParam = $('email_to_a_friend_form').serialize(true);</p>
<p>							emailParam.template_output = 'box/email_to_a_friend';</p>
<p>								</p>
<p>							$('send_email').disable();</p>
<p>							</p>
<p>							new Ajax.Updater('email_to_afriend', 'http://www.madata.gr/index.php', {</p>
<p>								parameters: emailParam,</p>
<p>								evalScripts: true,</p>
<p>								insertion: Insertion.Before,</p>
<p>								onComplete: function () { </p>
<p>									$('send_email').enable();</p>
<p>								}</p>
<p>							});</p>
<p>							return false;</p>
<p>						}</p>
