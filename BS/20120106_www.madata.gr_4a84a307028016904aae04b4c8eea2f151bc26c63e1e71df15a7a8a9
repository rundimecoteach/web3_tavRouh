<p>function runLiveTime()</p>
<p>{</p>
<p>    var time = new Date();</p>
<p>    var hours = time.getHours();</p>
<p>    var minutes = time.getMinutes();</p>
<p>    minutes=((minutes < 10) ? "0" : "") + minutes;</p>
<p>    </p>
<p>    var clock = hours + ":" + minutes;</p>
<p>    if(clock != document.getElementById('livetime').innerHTML) document.getElementById('livetime').innerHTML = clock;</p>
<p>    timer = setTimeout("runLiveTime()",1000);</p>
<p>}</p>
