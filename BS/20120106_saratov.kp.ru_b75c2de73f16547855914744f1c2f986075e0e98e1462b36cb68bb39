<p>	var pollrnd = Math.floor( Math.random()*10000 );</p>
<p>	new Ajax.Updater(</p>
<p>		'poll456_443',</p>
<p>		'/_ajax/gpoll/456/?par='+pollrnd,</p>
<p>		{	</p>
<p>			method: 'get',	 </p>
<p>			parameters: { sid: 443 },</p>
<p>			onLoading:function(){</p>
<p>				$('poll456_443'+'_preloader').show();</p>
<p>			},</p>
<p>			onLoaded:function(){</p>
<p>				$('poll456_443'+'_preloader').hide();</p>
<p>			},</p>
<p>			onComplete: function(tr) {</p>
<p>				if (tr.status == 200) {</p>
<p>					Effect.Appear($('poll456_443'));</p>
<p>				}</p>
<p>				$('poll456_443'+'_preloader').hide();</p>
<p>			}</p>
<p>		}</p>
<p>	);</p>
