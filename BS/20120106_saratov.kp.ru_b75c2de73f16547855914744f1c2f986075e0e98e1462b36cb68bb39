<p>			live: true,</p>
<p>			hashtags: true,</p>
<p>			timestamp: true,</p>
<p>			avatars: true,</p>
<p>			toptweets: true,</p>
<p>			behavior: 'default'</p>
<p>		}</p>
<p>	}).render().start();</p>
