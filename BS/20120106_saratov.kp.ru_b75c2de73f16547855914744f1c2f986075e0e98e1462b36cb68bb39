<p>    function getPosition(obj){</p>
<p>        var t = 0, l = 0;</p>
<p>        if (obj) {</p>
<p>            while (obj.offsetParent) {</p>
<p>                t += obj.offsetTop;</p>
<p>                l += obj.offsetLeft;</p>
<p>                obj = obj.offsetParent;</p>
<p>            }</p>
<p>        }</p>
<p>        return {"left":l, "top":t};</p>
<p>    }</p>
<p>    function getWindowGeometry() {</p>
<p>        var cMode = document.compatMode,</p>
<p>            ch = self.innerHeight || cMode == "CSS1Compat" && document.documentElement.clientHeight || document.body.clientHeight,</p>
<p>            st = self.pageYOffset || cMode == "CSS1Compat" && document.documentElement.scrollTop || document.body.scrollTop,</p>
<p>            cw = self.innerWidth || cMode == "CSS1Compat" && document.documentElement.clientWidth || document.body.clientWidth,</p>
<p>            sl = self.pageXOffset || cMode == "CSS1Compat" && document.documentElement.scrollLeft || document.body.scrollLeft;</p>
<p>        return {"clientHeight":ch, "clientWidth":cw, "scrollTop":st, "scrollLeft":sl};</p>
<p>    }</p>
