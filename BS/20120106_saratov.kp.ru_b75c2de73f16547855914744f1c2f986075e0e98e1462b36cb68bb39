<p>        Event.observe(window, "load", function(){ </p>
<p>                var links = $$('a[href="'+document.location.pathname+'"]');</p>
<p>                for (var i=0; i<links.length; i++) {</p>
<p>                    var ctr = links[i].up('li');</p>
<p>                    if ( ctr && ctr.hasClassName("s_also") == true ) {</p>
<p>                        ctr.hide();</p>
<p>                    }</p>
<p>                }</p>
<p>        });</p>
