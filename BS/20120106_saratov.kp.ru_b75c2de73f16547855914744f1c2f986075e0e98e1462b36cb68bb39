<p>//	google.load("prototype", "1.6.1.0");</p>
<p>//	google.load("scriptaculous", "1.8.2");</p>
<p>	google.setOnLoadCallback(OnLoad);</p>
<p></p>
<p>	function OnLoad(){</p>
<p>			//consolelog('Google JavaScript API are loaded.');</p>
<p>			var f_login=0;</p>
<p> </p>
<p>			window.fbAsyncInit = function() {</p>
<p>				FB.init({appId: '191944177588942', cookie: true, logging: true, status: true, xfbml: true});</p>
<p> </p>
<p>				FB.Event.subscribe('auth.login', function(response) {</p>
<p>					//consolelog('User is Logged In');</p>
<p>				});</p>
<p> </p>
<p>				FB.Event.subscribe('auth.logout', function(response) {</p>
<p>					//consolelog('Session was terminated!');</p>
<p>				});</p>
<p> </p>
<p>				FB.Event.subscribe('edge.create', function(response) {</p>
<p>					//consolelog("User liked <a target='_blank' href='" + response + "'>this page.</a>");</p>
<p>				});</p>
<p></p>
<p>				FB.getLoginStatus(function(response) {</p>
<p>					if (response.status === 'connected') {</p>
<p>						// the user is logged in and has authenticated your app, and response.authResponse supplies</p>
<p>						// the user's ID, a valid access token, a signed request, and the time the access token </p>
<p>						// and signed request each expire</p>
<p>						var uid = response.authResponse.userID;</p>
<p>						var accessToken = response.authResponse.accessToken;</p>
<p>						$('fb_loader').show();</p>
<p>						$('fb_nologin').hide();</p>
<p>						f_login=1;</p>
<p></p>
<p>						// проверяем состояние публикации</p>
<p>						var f_switch = parseInt(readCookie('fbSwitch'));</p>
<p>						if(f_switch != -1){ // если не запрещено, то ставим по умолчанию разрешение</p>
<p>							createCookie('fbSwitch', 1, 90);</p>
<p>							$('fb_switch').removeClassName('off');</p>
<p>							$('fb_switch').addClassName('on');</p>
<p>						}</p>
<p></p>
<p>						// получаем данные пользователя</p>
<p>						FB.api('/me?fields=first_name,last_name,link,username,picture', function(response) {</p>
<p>							////consolelog('Get user info. '+response.picture+' '+response.first_name+' '+response.last_name+' ');</p>
<p>							fbUserInfo(response); // выведем данные пользователя</p>
<p>							fbUserActivity();</p>
<p>						});</p>
<p>						</p>
<p></p>
<p>					} else if (response.status === 'not_authorized') {</p>
<p>						// the user is logged in to Facebook, but has not authenticated your app</p>
<p>						$('fb_loader').hide();</p>
<p>						$('fb_login').hide();</p>
<p>						$('fb_nologin').show();</p>
<p>						fbPopupShow();</p>
<p>					} else {</p>
<p>						// the user isn't logged in to Facebook.</p>
<p>						$('fb_loader').hide();</p>
<p>						$('fb_login').hide();</p>
<p>						$('fb_nologin').show();</p>
<p>						fbPopupShow();</p>
<p>					}</p>
<p>					//consolelog('Status: '+response.status);</p>
<p></p>
<p>				}, true);</p>
<p>			};</p>
<p></p>
<p>			(function() {</p>
<p>				var e = document.createElement('script');</p>
<p>				e.async = true;</p>
<p>				e.src = document.location.protocol + '//connect.facebook.net/ru_RU/all.js';</p>
<p>				document.getElementById('fb-root').appendChild(e);</p>
<p>			}());</p>
<p>	}</p>
<p>	</p>
<p>	function fbPopupShow() {</p>
<p>		$('fb_loader').hide();</p>
<p>		$('fb_login').hide();</p>
<p>		$('fb_nologin').show();</p>
<p></p>
<p>		var f_popup = parseInt(readCookie('fbPopup'));</p>
<p>		if(f_popup != 1){ // если еще не показали поп-ап</p>
<p>			//$('fb_popup').show(); // поп-ап для рекламмы приложения</p>
<p>			var domain = document.domain.split(".").splice(-2).join('.');</p>
<p>			createCookie('fbPopup', 1, 1,'.'+domain); // запоминаем в куке </p>
<p>		}</p>
<p>	}</p>
<p> </p>
<p>	function fbUserInfo(response){</p>
<p></p>
<p>		var img = new Element('img', {src: response.picture.data.url, title: response.first_name+" "+response.last_name});</p>
<p></p>
<p>		$('fb_avatar').href=response.link;</p>
<p>		$('fb_avatar').innerHTML='';</p>
<p>		$('fb_avatar').appendChild(img);</p>
<p></p>
<p>		$('fb_username').innerHTML = response.first_name+" "+response.last_name;</p>
<p>		$('fb_username').href=response.link;</p>
<p>		</p>
<p>		$('fb_profile_url').href=response.link;</p>
<p>		</p>
<p>		var f_switch = parseInt(readCookie('fbSwitch'));</p>
<p>		if(f_switch == 1){</p>
<p>			$('fb_switch').removeClassName('off');</p>
<p>			$('fb_switch').addClassName('on');</p>
<p>		}else{</p>
<p>			$('fb_switch').removeClassName('on');</p>
<p>			$('fb_switch').addClassName('off');</p>
<p>		}</p>
<p></p>
<p>		$('fb_loader').hide();</p>
<p>		$('fb_login').show(); // покажем инфо пользователя</p>
<p>	}</p>
<p></p>
<p>	function deleteRequest(requestId) {</p>
<p>		FB.api( "/" + requestId, "delete",</p>
<p>		function(response) {</p>
<p>			//consolelog(dump(response));</p>
<p>			$('act_li_'+requestId).fade({duration: 0.3});</p>
<p>		});</p>
<p>	}</p>
<p></p>
<p>	function fbUserActivity(){</p>
<p>		postAct();</p>
<p></p>
<p>		// удаляем старые действия</p>
<p>		while ($('fb_activity_list').childNodes.length > 0){</p>
<p>			$('fb_activity_list').removeChild($('fb_activity_list').lastChild);</p>
<p>		}</p>
<p></p>
<p>		// получаем последние действия</p>
<p>		FB.api('/me/news.reads?date_format=U', function(response) {</p>
<p>			for( var i = 0; i < (( response.data < 6 ) ? data.length : 6 ); i++  ){</p>
<p>				if(response.data[i] != undefined){</p>
<p>					//consolelog(" QQ: " +response.data[i]['data']['article']['title']+" "+response.data[i]['id']+" "+response.data[i]['start_time']);</p>
<p>				</p>
<p>					var li = new Element('li', {id: 'act_li_'+response.data[i]['id']});</p>
<p>					var a_t =  new Element('a', {href: response.data[i]['data']['article']['url'], target: '_blank', class: 'news'}).update(response.data[i]['data']['article']['title']);</p>
<p>					li.appendChild(a_t);</p>
<p>					var d_date = new Element('div', {class: 'date'}).update(GetTime(response.data[i]['start_time']));</p>
<p>					li.appendChild(d_date);</p>
<p>					var a_delete =  new Element('a', {href: '#', title: 'Удалить запись', class: 'close', onClick: 'deleteRequest(' + response.data[i]['id']+'); return false;'});</p>
<p>					li.appendChild(a_delete);</p>
<p>				</p>
<p>					$('fb_activity_list').appendChild(li);</p>
<p>				}</p>
<p>			}</p>
<p>		});		</p>
<p>	}</p>
<p></p>
<p> 	function fbLogout(){</p>
<p>		FB.logout(function(response) {</p>
<p>			$('fb_login').hide();</p>
<p>			$('fb_loader').show();</p>
<p>			document.location.href = document.location.href;</p>
<p>		});</p>
<p>	}</p>
<p> </p>
<p>	function fbLogin(){</p>
<p>		FB.login(function(response) {</p>
<p>			if (response.authResponse) {</p>
<p>			</p>
<p>				//consolelog('Welcome!  Fetching your information.... ');</p>
<p>				$('fb_nologin').hide();</p>
<p>				$('fb_loader').show();</p>
<p>				//$('fb_popup').hide(); // скрываем поп-ап</p>
<p></p>
<p>				// проверяем состояние публикации</p>
<p>				var f_switch = parseInt(readCookie('fbSwitch'));</p>
<p>				if(f_switch != -1){ // если не запрещено, то ставим по умолчанию разрешение</p>
<p>					createCookie('fbSwitch', 1, 90);</p>
<p>					$('fb_switch').removeClassName('off');</p>
<p>					$('fb_switch').addClassName('on');</p>
<p>				}</p>
<p></p>
<p>				FB.api('/me?fields=first_name,last_name,link,username,picture', function(response) {</p>
<p>					//consolelog('Good to see you, ' + response.name + '.');</p>
<p>					fbUserInfo(response);</p>
<p>					fbUserActivity();</p>
<p>				});</p>
<p></p>
<p>			} else {</p>
<p>				//consolelog('User cancelled login or did not fully authorize.');</p>
<p>			}</p>
<p>		}, {scope:'publish_actions'});</p>
<p>	}</p>
<p></p>
<p>//function createCookie( name, value, days ) {</p>
<p>//function readCookie( name ) {</p>
<p>//function eraseCookie( name ) {</p>
<p>	function fbSwitch(){</p>
<p>		var f_switch = parseInt(readCookie('fbSwitch'));</p>
<p>		if(f_switch == 1){</p>
<p>			createCookie('fbSwitch', -1, 90);</p>
<p>			$('fb_switch').removeClassName('on');</p>
<p>			$('fb_switch').addClassName('off');</p>
<p>		}else{</p>
<p>			createCookie('fbSwitch', 1, 90);</p>
<p>			$('fb_switch').removeClassName('off');</p>
<p>			$('fb_switch').addClassName('on');</p>
<p>			postAct(); // если включили, то сразу публикуем активность</p>
<p>		}</p>
<p>	}</p>
<p></p>
<p>	function fbRemoveApp(){</p>
<p>		FB.api("me/permissions", "DELETE", function(response){</p>
<p>			eraseCookie('fbSwitch');</p>
<p>			document.location.href = document.location.href;</p>
<p>		});</p>
<p>	}</p>
<p>	</p>
<p>	function postAct(){</p>
<p>		if($$("meta[property='og:type']")[0] == undefined) return;</p>
<p></p>
<p>		var f_switch = parseInt(readCookie('fbSwitch'));</p>
<p>		//consolelog("Switch: " +f_switch);</p>
<p>		var url='';</p>
<p>		</p>
<p>		if($$("meta[property='og:type']")[0].getAttribute('content') == 'article' && f_switch != -1){</p>
<p>			$('fb_switch').addClassName('switch-on');</p>
<p>			url=$$("meta[property='og:url']")[0].getAttribute('content');</p>
<p>		}else{</p>
<p>			return;</p>
<p>		}</p>
<p></p>
<p>		FB.api(</p>
<p>			'/me/news.reads?article=' + url,</p>
<p>			'post',</p>
<p>			function(response) {</p>
<p>				if (!response || response.error) {</p>
<p>					//consolelog('Error post act');</p>
<p>				} else {</p>
<p>					//consolelog('Read successful! Action ID: ' + response.id);</p>
<p>					//var ll = new Element('li').update('adasdasdasdasd'); // добавим в ленту активности</p>
<p>					//$('fb_activity_list').insertBefore(ll, $('fb_activity_list').firstChild);	</p>
<p></p>
<p>					</p>
<p>					fbUserActivity(); // обновляем активность, при успешной публикации</p>
<p>				}</p>
<p>		});</p>
<p>	}</p>
<p></p>
<p>	Event.observe(document.body, 'click', function(event) {</p>
<p>		// First element from event source with 'container' among its CSS classes</p>
<p>		if (!$(Event.element(event)).up('#fb_login') && !$(Event.element(event)).up('#fb_corner') && !$(Event.element(event)).up('#fb_settings') ){</p>
<p>			fbHidePopup();</p>
<p>		}else{</p>
<p>			return true; // alert("No..."); //</p>
<p>		}</p>
<p>	});	</p>
<p></p>
<p>	function GetTime (utime){</p>
<p>		var current_timestamp = Math.round((new Date()).getTime() / 1000);</p>
<p>		var time_ago = current_timestamp - utime,</p>
<p>		days = Math.floor( time_ago/86400 ),</p>
<p>		hours = Math.floor( ( time_ago - days * 86400)/3600 ),</p>
<p>		min = Math.floor( ( time_ago - days * 86400 - hours * 3600 )/60 ),</p>
<p>		sec = time_ago - days * 86400 - hours * 3600 - min * 60,</p>
<p>		time_ago_text = (( days > 0 ) ? days + " " + WordMorf( days, "день", "дня", "дней" ) :</p>
<p>			(( hours > 0 ) ? </p>
<p>				hours + " " + WordMorf( hours, "час", "часа", "часов" ) : </p>
<p>					(( min > 0 ) ? </p>
<p>						min + " " + WordMorf( min, "минута", "минуты", "минут" ) : </p>
<p>							(( sec > 0 ) ? sec + " " + WordMorf( sec, "секунда", "секунды", "секунд" ) : "0" ) </p>
<p>					)</p>
<p>			)</p>
<p>		) + " назад";</p>
<p>										</p>
<p>		return time_ago_text;</p>
<p>	}</p>
<p>	</p>
<p>	</p>
<p>	function WordMorf(n, one, two, many){</p>
<p>		//WordMorf( n, 'кальмар', 'кальмара', 'кальмаров' ); </p>
<p>		return n % 10 == 1 && n % 100 != 11 ? one : n % 10 >= 2 && n % 10 <= 4 && (n % 100 < 10 || n % 100 >= 20) ? two : many;</p>
<p>	}</p>
<p></p>
<p>	function fbHidePopup(){</p>
<p>		if($('fb_activity').style.display != "none") $('fb_activity').fade({ duration: 0.2 });</p>
<p>		if($('fb_settings').style.display != "none") $('fb_settings').fade({ duration: 0.2 });</p>
<p>		if($('fb_corner').style.display != "none") $('fb_corner').fade({ duration: 0.2 });</p>
<p>	}</p>
<p></p>
<p>	</p>
<p>	</p>
<p>		function dump(arr,level) {</p>
<p>	var dumped_text = "";</p>
<p>	if(!level) level = 0;</p>
<p>	</p>
<p>	//The padding given at the beginning of the line.</p>
<p>	var level_padding = "";</p>
<p>	for(var j=0;j<level+1;j++) level_padding += "	";</p>
<p>	</p>
<p>	if(typeof(arr) == 'object') { //Array/Hashes/Objects </p>
<p>		for(var item in arr) {</p>
<p>			var value = arr[item];</p>
<p>			</p>
<p>			if(typeof(value) == 'object') { //If it is an array,</p>
<p>				dumped_text += level_padding + "'" + item + "' ...\n";</p>
<p>				dumped_text += dump(value,level+1);</p>
<p>			} else {</p>
<p>				dumped_text += level_padding + "'" + item + "' => \"" + value + "\"\n";</p>
<p>			}</p>
<p>		}</p>
<p>	} else { //Stings/Chars/Numbers etc.</p>
<p>		dumped_text = "===>"+arr+"<===("+typeof(arr)+")";</p>
<p>	}</p>
<p>	return dumped_text;</p>
<p>}	</p>
