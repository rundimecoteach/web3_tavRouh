<p>			th.layerbg.className = 'share_layer';</p>
<p>			th.layerbg.innerHTML = '<div class="share_layer_main" id="login_box_main"><div class="share_layer_title" style="cursor: move;"><h3 class=""></h3><a title="关闭" class="del_fri">X</a></div><div class="share_layer_cont"><iframe frameborder="0" name="Login_Frame" id="Login_Frame" width="397" height="190" scrolling="no"></iframe></div></div>';</p>
<p>			th.layerbg.children[0].children[0].children[0].innerHTML = th.title;</p>
<p>			th.layerbg.children[0].children[0].children[1].onmousedown = function(e){</p>
<p>				th.releaseEvent(th.layerTitle);</p>
<p>				th.closePopup();</p>
<p>			};</p>
<p></p>
