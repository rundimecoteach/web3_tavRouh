<p>_MUI.ready(function(){</p>
<p>	_MI.ShareArticle.build('tmblogbt','qqcom.dp.tmblog');</p>
<p>	_MI.ShareArticle.build('tmblogbt1','qqcom.dp.tmblog1');</p>
