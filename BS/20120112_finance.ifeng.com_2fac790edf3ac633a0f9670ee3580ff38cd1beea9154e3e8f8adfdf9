<p>	},</p>
<p>	getRandom:function() {</p>
<p>		return (parseInt(Math.random()*100000) % this.max);</p>
<p>	},</p>
<p>	logic:function() {</p>
<p>		var sequence = this.getCookie(this.cookie_flag);</p>
<p>		if(typeof(sequence) === 'undefined' || sequence === '') {</p>
<p>			this.current = this.getRandom();</p>
<p>		} else {</p>
<p>			this.current = sequence;</p>
<p>		}</p>
<p>		var sequence_next = (parseInt(this.current) + 1) % this.max;</p>
<p>		this.setCookie(this.cookie_flag, sequence_next, 60);</p>
<p>	},</p>
<p>	play:function() {</p>
<p>		var _AdHtml = document.getElementById('ArpAdPro_' + this.id + this.current).innerHTML;</p>
<p>		_AdHtml = _AdHtml.replace('<!-- ArpAdPro_Date','').replace('ArpAdPro_Date_End -->','').replace(/{\+\+}/g,'--');</p>
<p>		document.write(_AdHtml);</p>
<p>	},</p>
<p>	init:function() {</p>
<p>		this.cookie_flag = 'IfengArp_' + this.id;</p>
<p>		this.logic();</p>
<p>		this.play();</p>
<p>	}</p>
<p>};</p>
