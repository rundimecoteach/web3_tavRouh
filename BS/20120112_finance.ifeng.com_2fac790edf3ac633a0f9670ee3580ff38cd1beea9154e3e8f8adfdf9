<p>	function getCookie_comment(name){</p>
<p>		var arr = document.cookie.match(new RegExp('(^| )'+name+'=([^;]*)(;|$)'));</p>
<p>		if(arr!=null) return arr[2];</p>
<p>		return null;</p>
<p>	}</p>
