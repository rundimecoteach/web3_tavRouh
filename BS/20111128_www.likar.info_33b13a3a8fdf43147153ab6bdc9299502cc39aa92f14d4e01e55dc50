<p>function changeSlide( sldname, sldhide1, slidehide2) {</p>
<p></p>
<p>	document.getElementById(sldname).style.display = 'block';</p>
<p>	document.getElementById(sldhide1).style.display = 'none';</p>
<p>	document.getElementById(slidehide2).style.display = 'none';</p>
<p></p>
<p>	document.getElementById('c_' + sldname).className = sldname + ' selected';</p>
<p>	document.getElementById('c_' + sldhide1).className = sldhide1;</p>
<p>	document.getElementById('c_' + slidehide2).className = slidehide2;</p>
<p>}</p>
