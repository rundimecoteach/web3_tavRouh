<p>		$('.feeds').each(function(){</p>
<p>			if (feedcount < 3) {</p>
<p>				var target = "." + $(this).attr('id');</p>
<p>				var feedurl = $('a', this).attr('rel');</p>
<p>				var feed = new google.feeds.Feed(feedurl);</p>
<p>				feed.setNumEntries(10);</p>
<p>				feed.load(function(result){</p>
<p>					if(!result.error){</p>
<p>						for (var i=0; i < result.feed.entries.length; i++) {</p>
<p>							var entry = result.feed.entries[i];</p>
<p>								$('ul', target).append('<li class="mb"><a href="'+entry.link+'" onclick="_gaq.push([\'_trackEvent\', \''+ result.feed.title +'\', \''+ entry.title +'\',\'/news/2012/jan/10/hhs-offers-aid-over-guatemalan-std-lawsuit/\']);trackclick(this);return false;">'+entry.title+'</a></li>');</p>
<p>						}</p>
<p>					}</p>
<p>				});</p>
<p>				feedcount++;</p>
<p>			}</p>
<p>		});</p>
<p>	}</p>
<p>	google.setOnLoadCallback(init);</p>
