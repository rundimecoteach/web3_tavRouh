<p>      var selectedTab = null;</p>
<p>      function showPanel(tab, name)</p>
<p>      {</p>
<p>        for(i = 0; i < panels.length; i++)</p>
<p>        {</p>
<p>          document.getElementById(panels[i]).style.display = (name == panels[i]) ? 'block':'none';</p>
<p>        }</p>
<p>        return false;</p>
<p>      }</p>
