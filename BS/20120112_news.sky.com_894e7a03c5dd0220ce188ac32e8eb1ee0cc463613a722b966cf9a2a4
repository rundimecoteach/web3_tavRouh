<p>function setSelectedSearchType(val)</p>
<p>{</p>
<p>	var searchType=document.getElementById('search_type');</p>
<p>	var search_type_list = document.getElementById('searchTypes');</p>
<p>	searchType.value=val;</p>
<p>	switch(val) {</p>
<p>		case "news":</p>
<p>			search_type_list.getElementsByTagName('li')[0].innerHTML ='|<strong class="selectedTab">News</strong>|';</p>
<p>			search_type_list.getElementsByTagName('li')[1].innerHTML ='<a href="javascript:setSelectedSearchType(\'web\')">Web</a> |';</p>
<p>			break;</p>
<p>		case "web":</p>
<p>			search_type_list.getElementsByTagName('li')[1].innerHTML ='<strong class="selectedTab">Web</strong> |';</p>
<p>			search_type_list.getElementsByTagName('li')[0].innerHTML = '| <a href="javascript:setSelectedSearchType(\'news\')">News</a> |';</p>
<p>			break;</p>
<p>	}</p>
<p>}</p>
