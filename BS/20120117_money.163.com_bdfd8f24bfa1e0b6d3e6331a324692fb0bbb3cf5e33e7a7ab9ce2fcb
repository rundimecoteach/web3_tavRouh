<p>            //点击复制</p>
<p>            var EndPage = function(){</p>
<p>            }</p>
<p>            if (NTES("#h1title")) {</p>
<p>				var getTitle = NTES("#h1title").innerHTML.replace(/["']+/g,'');</p>
<p>			}</p>
<p>				else {</p>
<p>				// alert ("获取标题失败");</p>
<p>			}</p>
<p>                EndPage.clearField = function(){</p>
<p>                if (NTES("#inputWord").value == EndPage.tip) {</p>
<p>                NTES("#inputWord").value = "";</p>
<p>                }</p>
<p>            }</p>
<p>            EndPage.mail = function(){</p>
<p>                try {</p>
<p>                    var RightMailAgent = new ActiveXObject("FlashMailShell.FlashMailIEExt");</p>
<p>                } </p>
<p>                catch (e) {</p>
<p>                    window.open("http://fm.163.com/commend.html?tab=5?yjzf100201", "")</p>
<p>                }</p>
<p>                var str = NTES("#endText").innerHTML;</p>
<p>                str = str.replace(/<\/?(?:embed|object|param)[^>]*>/ig, "");</p>
<p>				str += "<br /><br />";</p>
<p>				str += '“你也想和好友分享精彩新闻吗?请\<a href=\"http:\/\/dl.163.com\/NxMail\/Update\/installer\/flashmail-2.0beta.exe\"\>下载闪电邮\<\/a\>,并阅读\<a href=\"http:\/\/fm.163.com\/introduce.html?yjzf100201\"\>闪电邮介绍\<\/a\>”';</p>
<p>				</p>
<p>				if (RightMailAgent != null) {</p>
<p>                    RightMailAgent.addSubject("您的好友推荐文章《"+getTitle+"》");</p>
<p>                    RightMailAgent.addContent(str);</p>
<p>                }</p>
<p>            }</p>
<p>            EndPage.copyUrl = function(){</p>
<p>                var cbc = "";</p>
<p>                cbc += getTitle +'\r\n' + document.URL;</p>
<p>                window.clipboardData.setData("Text", cbc);</p>
<p>                NTES("#IECopyUrl").innerHTML = "<span class=\"cDRed\">已复制</span>";</p>
<p>            }</p>
<p>           </p>
<p>		   //初始化 </p>
<p>            if (NTES.browser.msie) {</p>
<p>                NTES("#IEHtml").innerHTML = "\<span class=\"right cDGray\"\>\<span class=\"copyLink\" id=\"IECopyUrl\" title=\"点击复制标题和链接\" style=\"background: url(http:\/\/img1.cache.netease.com\/cnews\/css09\/bg_n1.gif\) no-repeat scroll left -362px transparent; padding: 2px 0pt 2px 23px;\"\>\<a style=\"cursor:pointer; text-decoration:none;\"\>复制网址\<\/a\>\<\/span\> \<a href=\"mailto:\" class=\"mail\" id=\"IEMailSend\" title=\"你也想和好友分享精彩新闻吗?请下载闪电邮(点击直接可下载)\" style=\"background: url(http:\/\/img1.cache.netease.com\/cnews\/css09\/bg_n1.gif) no-repeat scroll -154px -363px transparent; padding: 2px 0pt 2px 23px;\"\>闪电邮转发\<\/a\>\<\/span\>";</p>
<p>                NTES("#IEMailSend").addEvent("click", function(e){</p>
<p>                    EndPage.mail();</p>
<p>                    e.preventDefault();</p>
<p>                });</p>
<p>                NTES("#IECopyUrl").addEvent("click", function(e){</p>
