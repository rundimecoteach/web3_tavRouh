<p>			$(document).ready(function(){</p>
<p>				$(".categoryMachItem .formCheck").attr("disabled", true);</p>
<p>				$(".checkboxGathering").addClass("hide");</p>
<p>				$(".rok_produkcji").removeClass("hide");</p>
<p></p>
<p>				$(".formRadio").click(function(){</p>
<p>					$(".categoryMachItem .formCheck").attr("disabled", true);</p>
<p>					$(".categoryMachItem .formCheck").attr("checked", false);</p>
<p>					$(".checkboxGathering").addClass("hide");</p>
<p>					$(".rok_produkcji").removeClass("hide");</p>
<p>					$(this).parent().find(".checkboxGathering").removeClass("hide");</p>
<p>					$(this).parent().find(".formCheck").removeAttr("disabled");</p>
<p>				});</p>
<p>				$(".formRadio.uslugi").click(function(){</p>
<p>					$(".rok_produkcji").addClass("hide");</p>
<p>				});</p>
<p>			});</p>
<p>			</p>
