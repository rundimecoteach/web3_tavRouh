<p>$(document).ready(function() {</p>
<p>	var login = $.cookie('login');</p>
<p></p>
<p>	if (login == null)</p>
<p>	{</p>
<p>		$('.hello').html('Здравствуйте, Гость<br /><a href="/auth/login">Вход</a> | <a href="/auth/registration">Регистрация</a>');</p>
<p>	} else {</p>
<p>		$('.hello').html('Здравствуйте, ' + login + '!<br /><a href="/auth/logout">Выйти</a>');</p>
<p>	}</p>
<p>});</p>
