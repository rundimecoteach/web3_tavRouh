<p>    window.fbAsyncInit = function () {</p>
<p>        FB.init({ appId: '112587685485572', status: true, cookie: true,</p>
<p>            xfbml: true</p>
<p>        });</p>
<p>        if (!NvUser.IsUserLoggedIn()) {</p>
<p>            try {</p>
<p></p>
<p>                FB.getLoginStatus(function (response) {</p>
<p>                    if (response.session) {</p>
<p>			  if (window.location.href.indexOf("fbconnectreturn") == -1)</p>
<p>				 {	</p>
<p>					var url = window.location.href;</p>
<p>					if(url.indexOf("?") == -1)</p>
<p>					{</p>
<p>						url+="?fbconnectreturn=1";</p>
<p>					}else</p>
<p>					{</p>
<p>						url+="&fbconnectreturn=1";</p>
<p>					}</p>
<p>					</p>
<p>					//window.location = '/FBConnect.axd?returnUrl=' + escape(url);</p>
<p>				}</p>
<p>                    }</p>
<p>                });</p>
<p>            } catch (e)</p>
<p>            { }</p>
<p>        }</p>
<p></p>
<p></p>
<p>    };</p>
