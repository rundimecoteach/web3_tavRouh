<p>                        var comments_ajax_url = '/comments/news/814857/';</p>
<p></p>
<p>                        function commentReply(pid) {</p>
<p>                          jQuery('#comments_pid').val(parseInt(pid, 10));</p>
<p>                          jQuery('#comments_form').show();</p>
<p>                          jQuery('#comment_text').focus();</p>
<p>                        }</p>
<p></p>
<p>                        function validateCommentsForm()</p>
<p>                        {</p>
<p>                          var name = jQuery('#comments_name').val();</p>
<p>                          var text = jQuery('#comments_text').val();</p>
<p>                          var code = jQuery('#comments_code').length ? jQuery('#comments_code').val() : '1';</p>
<p>                          var agree = jQuery('#comments_agree').length ? jQuery('#comments_agree').attr('checked') : true;</p>
<p>                          var error = '';</p>
<p>                          if(name.length == 0) {</p>
<p>                            error = 'Вы должны указать имя';</p>
<p>                            jQuery('#comments_name').focus();</p>
<p>                          }</p>
<p>                          else if(name.length < 3) {</p>
<p>                            error = 'Минимальная длина имени 3 символа';</p>
<p>                            jQuery('#comments_name').focus();</p>
<p>                          }</p>
<p>                          else if(text.length == 0) {</p>
<p>                            error = 'Вы должны ввести свой комментарий';</p>
<p>                            jQuery('#comments_text').focus();</p>
<p>                          }</p>
<p>                          else if(text.length > 500) {</p>
<p>                            error = 'Максимальная длина комментария 500 символов';</p>
<p>                            jQuery('#comments_text').focus();</p>
<p>                          }</p>
<p>                          else if(!agree) {</p>
<p>                            error = 'Вы должны принять условия Пользовательского соглашения';</p>
<p>                          }</p>
<p>                          else if(code.length == 0) {</p>
<p>                            error = 'Вы должны ввести код подтверждения';</p>
<p>                            jQuery('#comments_code').focus();</p>
<p>                          }</p>
<p>                          if(error) {</p>
<p>                            jQuery('#comments_error').html(error).show();</p>
<p>                            return false;</p>
<p>                          }</p>
<p>                          return true;</p>
<p>                        }</p>
