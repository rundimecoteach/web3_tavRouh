<p>jQuery("#articleObjectListCategory1").boxList({</p>
<p>params: {sort: ['dates', 'views', 'rates', 'forum']},</p>
<p>listAjaxUrl: '/ajax/article-category/rank?categoryId=555',</p>
<p>get: function (liJQ, params) {</p>
<p>var icon = {};</p>
<p>liJQ.find('.title a span.icon').each(function(index, elem){</p>
<p>var iconElem = jQuery(elem).attr('class');</p>
<p>iconElem.substring(5)</p>
<p>icon[iconElem] = 1;</p>
<p>});</p>
<p>return {</p>
<p>title: liJQ.find('.title a').text(),</p>
<p>date: liJQ.find('.date').text(),</p>
<p>type: icon,</p>
<p>link: liJQ.find('.title a').attr('href')};</p>
<p></p>
<p>},</p>
<p>set: function (liJQ, data, params) {</p>
<p>liJQ.find('.title a span:not([class])').text(data.title);</p>
<p>liJQ.find('.date').text(data.date);</p>
<p>liJQ.find('.title a').attr('href', data.link);</p>
