<p>        function ashare( el ) {</p>
<p>            if ( el.tagName.toLowerCase() != 'a' ) {</p>
<p>                return;</p>
<p>            }</p>
<p>            var p = {</p>
<p>                    w : 600,</p>
<p>                    h : 380,</p>
<p>                    l : ( this.w - 600 ) / 2,</p>
<p>                    t : ( this.h - 400 ) / 2</p>
<p>            };</p>
<p>            if ( p.l < 0 || p.t < 0 ) {</p>
<p>                p.l = p.t = 0;</p>
<p>            }</p>
<p>            var w = window.open( '', 'Article share', 'top='+p.t+',left='+p.l+',width='+p.w+',height='+p.h );</p>
<p>            w.document.write( 'Loading...' );</p>
<p>            w.location.href = el.href;</p>
<p>        }</p>
